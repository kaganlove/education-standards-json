    },
    "8E8D504E59D044E884B01D03625689ED": {
      "id": "8E8D504E59D044E884B01D03625689ED",
      "asnIdentifier": null,
      "position": 1022,
      "depth": 1,
      "listId": "10",
      "statementNotation": "2.8.5.10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate an understanding that artistic works influence and are influenced by personal, societal, cultural, and historical contexts, including the contributions of Minnesota American Indian tribes and communities.",
      "comments": [],
      "ancestorIds": [
        "0E271430EDDD433C910685184F000D0D"
      ],
      "parentId": "0E271430EDDD433C910685184F000D0D"
    },
    "3A6BF6CDBCE6497E873E466D44F9A664": {
      "id": "3A6BF6CDBCE6497E873E466D44F9A664",
      "asnIdentifier": null,
      "position": 1021,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.8.5.9.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Evaluate the cycle of producing media artworks to make meaning of and respond to local and global events, and the power of media arts to affect social change.\n\nFor example: Artmaking in response to natural disaster; political and social campaigns.",
      "comments": [],
      "ancestorIds": [
        "0B4ADEEC6C9445428CD5C1A2A671E0D8",
        "0E271430EDDD433C910685184F000D0D"
      ],
