      "depth": 1,
      "listId": "6",
      "statementNotation": "2.5.3.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make artistic choices in order to convey meaning through presentation. ",
      "comments": [],
      "ancestorIds": [
        "80F05D7DE837441FAFCCB116CCD58A0B"
      ],
      "parentId": "80F05D7DE837441FAFCCB116CCD58A0B"
