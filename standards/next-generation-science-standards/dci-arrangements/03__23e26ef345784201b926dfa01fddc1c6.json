    "D73542C003114FF38807EC9E78441F56": {
      "id": "D73542C003114FF38807EC9E78441F56",
      "asnIdentifier": null,
      "position": 6000,
      "depth": 1,
      "listId": "b",
      "statementNotation": "3-PS2-4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Electric, and magnetic forces between a pair of objects\ndo not require that the objects be in contact. The sizes\nof the forces in each situation depend on the properties\nof the objects and their distances apart and, for forces\nbetween two magnets, on their orientation relative to\neach other.",
      "comments": [],
      "ancestorIds": [
        "F6E363BCA13442D4A0D0DD7DAD589A26"
      ],
      "parentId": "F6E363BCA13442D4A0D0DD7DAD589A26"
    },
    "BE6D2517DCF9456E8C373F0CA82A4267": {
      "id": "BE6D2517DCF9456E8C373F0CA82A4267",
      "asnIdentifier": null,
      "position": 5000,
      "depth": 1,
      "listId": "a",
      "statementNotation": "3-PS2-3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Objects in contact exert forces on each other.",
      "comments": [],
      "ancestorIds": [
        "F6E363BCA13442D4A0D0DD7DAD589A26"
      ],
      "parentId": "F6E363BCA13442D4A0D0DD7DAD589A26"
    },
