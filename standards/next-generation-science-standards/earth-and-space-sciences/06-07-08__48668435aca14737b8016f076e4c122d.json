    "A985DF38F2474DC7977E8A3E65D0ACD6": {
      "id": "A985DF38F2474DC7977E8A3E65D0ACD6",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 3,
      "listId": "iii",
      "statementNotation": "MS-ESS1-1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "This model of the solar system can explain eclipses of the sun and\nthe moon. Earth’s spin axis is fixed in direction over the short-term\nbut tilted relative to its orbit around the sun. The seasons are a\nresult of that tilt and are caused by the differential intensity of\nsunlight on different areas of Earth across the year.",
      "comments": [],
      "ancestorIds": [
        "79356AAE1BC34473B09FD3F24F0DB172",
        "1C35E229BE4B43DB95CB4151923BDD62",
        "04769809346846E7830B3BF1E38CFCC6"
      ],
      "parentId": "79356AAE1BC34473B09FD3F24F0DB172"
    },
