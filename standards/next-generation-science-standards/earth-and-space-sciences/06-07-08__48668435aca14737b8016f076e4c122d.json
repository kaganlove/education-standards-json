    "95B74D040B3D4D9B9DB5A04E6FF9981A": {
      "id": "95B74D040B3D4D9B9DB5A04E6FF9981A",
      "asnIdentifier": null,
      "position": 4028,
      "depth": 3,
      "listId": "v",
      "statementNotation": "MS-ESS2-2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Water’s movements—both on the land and underground—cause\nweathering and erosion, which change the land’s surface features and\ncreate underground formations.",
      "comments": [],
      "ancestorIds": [
        "F16BEBC3CA8E4D8E94D31333222953DE",
