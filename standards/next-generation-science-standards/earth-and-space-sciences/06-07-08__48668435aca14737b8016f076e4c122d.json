      "asnIdentifier": null,
      "position": 4026,
      "depth": 3,
      "listId": "iii",
      "statementNotation": "MS-ESS2-4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Global movements of water and its changes in form are propelled by\nsunlight and gravity.",
      "comments": [],
      "ancestorIds": [
        "F16BEBC3CA8E4D8E94D31333222953DE",
        "2B78F87CAD184EB6BB1543B15C30A1DC",
        "42E8194581F745E18F55230483EFC938"
      ],
      "parentId": "F16BEBC3CA8E4D8E94D31333222953DE"
    },
    "5511A88C3A1444008E734AA13E7347F3": {
      "id": "5511A88C3A1444008E734AA13E7347F3",
      "asnIdentifier": null,
      "position": 4025,
      "depth": 3,
      "listId": "ii",
      "statementNotation": "MS-ESS2-5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The complex patterns of the changes and the movement of water in the\natmosphere, determined by winds, landforms, and ocean temperatures\nand currents, are major determinants of local weather patterns.",
      "comments": [],
      "ancestorIds": [
        "F16BEBC3CA8E4D8E94D31333222953DE",
        "2B78F87CAD184EB6BB1543B15C30A1DC",
        "42E8194581F745E18F55230483EFC938"
