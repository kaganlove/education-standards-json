    },
    "F1E29D109FB44D368444EA7BCCDF1DA7": {
      "id": "F1E29D109FB44D368444EA7BCCDF1DA7",
      "asnIdentifier": null,
      "position": 6051,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-LS4-1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The collection of fossils and their placement in\nchronological order (e.g., through the location of the\nsedimentary layers in which they are found or through\nradioactive dating) is known as the fossil record. It\ndocuments the existence, diversity, extinction, and\nchange of many life forms throughout the history of life\non Earth. ",
      "comments": [],
      "ancestorIds": [
        "07BCD1B8559740A4AFE156538670EAD9",
        "96A08F645719497F918021CF6738104E",
