    "3A69017473A74ED1B63346F260AEDA18": {
      "id": "3A69017473A74ED1B63346F260AEDA18",
      "asnIdentifier": null,
      "position": 3019,
      "depth": 3,
      "listId": "ii",
      "statementNotation": "MS-LS1-7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Cellular respiration in plants and animals involve\nchemical reactions with oxygen that release stored\nenergy. In these processes, complex molecules\ncontaining carbon react with oxygen to produce carbon\ndioxide and other materials.",
      "comments": [],
      "ancestorIds": [
        "359556CED8844ED09F84FB90FDAE5417",
        "57B3130124A44F65A84CC4507823D8F7",
        "75AACE1F5E5F4BB69BF896AD1AF0C3A2"
      ],
      "parentId": "359556CED8844ED09F84FB90FDAE5417"
    },
    "6C4ED0FC3A1A491BBB4EB156B7A072FD": {
      "id": "6C4ED0FC3A1A491BBB4EB156B7A072FD",
      "asnIdentifier": null,
      "position": 3018,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-LS1-6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The chemical reaction by which plants produce complex\nfood molecules (sugars) requires an energy input (i.e.,\nfrom sunlight) to occur. In this reaction, carbon dioxide\nand water combine to form carbon-based organic\nmolecules and release oxygen.",
      "comments": [],
      "ancestorIds": [
        "359556CED8844ED09F84FB90FDAE5417",
        "57B3130124A44F65A84CC4507823D8F7",
        "75AACE1F5E5F4BB69BF896AD1AF0C3A2"
      ],
      "parentId": "359556CED8844ED09F84FB90FDAE5417"
    },
