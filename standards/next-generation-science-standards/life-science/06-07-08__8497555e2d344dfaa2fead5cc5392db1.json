    "971EFF7DF1A04E7DB4876644BE87CC0C": {
      "id": "971EFF7DF1A04E7DB4876644BE87CC0C",
      "asnIdentifier": null,
      "position": 2008,
      "depth": 3,
      "listId": "ii",
      "statementNotation": "MS-LS1-4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Plants reproduce in a variety of ways, sometimes\ndepending on animal behavior and specialized features\nfor reproduction. ",
      "comments": [],
      "ancestorIds": [
        "7E0AF59536A14AB59B1A3FC24C31635C",
