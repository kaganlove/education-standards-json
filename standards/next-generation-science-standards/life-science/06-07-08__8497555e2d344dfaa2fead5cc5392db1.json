    "6955855B74284FE1AC9118CDA162083E": {
      "id": "6955855B74284FE1AC9118CDA162083E",
      "asnIdentifier": null,
      "position": 3012,
      "depth": 3,
      "listId": "ii",
      "statementNotation": "MS-LS1-7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Within individual organisms, food moves through a\nseries of chemical reactions in which it is broken down\nand rearranged to form new molecules, to support\ngrowth, or to release energy. ",
      "comments": [],
      "ancestorIds": [
        "E35818D8F113492F9B8674F2DE262507",
        "5FF8EB04A5F24C09AB0EB357D0AB0026",
        "75AACE1F5E5F4BB69BF896AD1AF0C3A2"
      ],
      "parentId": "E35818D8F113492F9B8674F2DE262507"
    },
    "E632913A60504163A39C9FFBE7B06C6E": {
      "id": "E632913A60504163A39C9FFBE7B06C6E",
      "asnIdentifier": null,
      "position": 3011,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-LS1-6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Plants, algae (including phytoplankton), and many\nmicroorganisms use the energy from light to make\nsugars (food) from carbon dioxide from the atmosphere\nand water through the process of photosynthesis, which\nalso releases oxygen. These sugars can be used\nimmediately or stored for growth or later use.",
      "comments": [],
      "ancestorIds": [
        "E35818D8F113492F9B8674F2DE262507",
        "5FF8EB04A5F24C09AB0EB357D0AB0026",
        "75AACE1F5E5F4BB69BF896AD1AF0C3A2"
      ],
      "parentId": "E35818D8F113492F9B8674F2DE262507"
    },
