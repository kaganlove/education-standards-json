    "5445FF314A234A65B87D9ED036A43E1E": {
      "id": "5445FF314A234A65B87D9ED036A43E1E",
      "asnIdentifier": null,
      "position": 3029,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-LS2-3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Food webs are models that demonstrate how matter and energy\nis transferred between producers, consumers, and decomposers\nas the three groups interact within an ecosystem. Transfers of\nmatter into and out of the physical environment occur at every\nlevel. Decomposers recycle nutrients from dead plant or animal\nmatter back to the soil in terrestrial environments or to the\nwater in aquatic environments. The atoms that make up the\norganisms in an ecosystem are cycled repeatedly between the\nliving and nonliving parts of the ecosystem. ",
      "comments": [],
      "ancestorIds": [
        "0337301DFDB643938C60EC2475CC82E9",
        "5E76D5E884E44F00B7F000E6E39C81E3",
        "3EA73E400ABF4AE284EB2A41B74D3A61"
      ],
      "parentId": "0337301DFDB643938C60EC2475CC82E9"
    },
