    "E571450F260041E1800C97C7D5AA74D4": {
      "id": "E571450F260041E1800C97C7D5AA74D4",
      "asnIdentifier": null,
      "position": 3015,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-LS1-8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Each sense receptor responds to different inputs\n(electromagnetic, mechanical, chemical), transmitting\nthem as signals that travel along nerve cells to the\nbrain. The signals are then processed in the brain,\nresulting in immediate behaviors or memories. ",
      "comments": [],
      "ancestorIds": [
