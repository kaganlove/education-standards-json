    },
    "9B3B8289E06A49CAB6BBEF57EE35266A": {
      "id": "9B3B8289E06A49CAB6BBEF57EE35266A",
      "asnIdentifier": null,
      "position": 24000,
      "depth": 2,
      "listId": "1",
      "statementNotation": "HS-ESS2-D-1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The foundation for Earth's global climate systems is the electromagnetic radiation from the sun, as well as its reflection, absorption, storage, and redistribution among the atmosphere, ocean, and land systems, and this energy's re-radiation into space. (HS-ESS2-2),(HS-ESS2-4)",
      "comments": [],
      "ancestorIds": [
        "D59B8C5653EB40A79FC8CDE181C89E52",
        "7E32ED7489964EDD939372B96D21F27C"
      ],
      "parentId": "D59B8C5653EB40A79FC8CDE181C89E52"
    },
    "D59B8C5653EB40A79FC8CDE181C89E52": {
      "id": "D59B8C5653EB40A79FC8CDE181C89E52",
      "asnIdentifier": null,
      "position": 23000,
      "depth": 1,
      "listId": "D",
      "statementNotation": "HS-ESS2-D",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Weather and Climate ",
      "comments": [],
      "ancestorIds": [
        "7E32ED7489964EDD939372B96D21F27C"
      ],
      "parentId": "7E32ED7489964EDD939372B96D21F27C"
