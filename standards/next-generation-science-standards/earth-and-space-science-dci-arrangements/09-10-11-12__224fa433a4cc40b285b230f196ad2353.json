    "1636FA55AA4748E3962B53828FDA6FA2": {
      "id": "1636FA55AA4748E3962B53828FDA6FA2",
      "asnIdentifier": null,
      "position": 25000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "HS-ESS2-D-2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Gradual atmospheric changes were due to plants and other organisms that captured carbon dioxide and released oxygen. (HS-ESS2-6),(HS-ESS2-7) ",
      "comments": [],
      "ancestorIds": [
        "D59B8C5653EB40A79FC8CDE181C89E52",
        "7E32ED7489964EDD939372B96D21F27C"
