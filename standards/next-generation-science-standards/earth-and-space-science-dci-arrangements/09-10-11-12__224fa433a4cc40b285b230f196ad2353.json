    "C0D482EAD15B44768757D4D024E6AE4C": {
      "id": "C0D482EAD15B44768757D4D024E6AE4C",
      "asnIdentifier": null,
      "position": 9000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "HS-ESS1-B-2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Cyclical changes in the shape of Earth's orbit around the sun, together with changes in the tilt of the planet's axis of rotation, both occurring over hundreds of thousands of years, have altered the intensity and distribution of sunlight falling on the earth. These phenomena cause a cycle of ice ages and other gradual climate changes. (secondary to HS-ESS2-4) ",
      "comments": [],
      "ancestorIds": [
        "3298BFE3DAF74A1289DB43388CDC0319",
        "4C349631DF28439096F342E97AD786CC"
      ],
      "parentId": "3298BFE3DAF74A1289DB43388CDC0319"
    },
    "3C435359FE764DC08AC7EFF8809F1B09": {
      "id": "3C435359FE764DC08AC7EFF8809F1B09",
      "asnIdentifier": null,
      "position": 8000,
      "depth": 2,
      "listId": "1",
      "statementNotation": "HS-ESS1-B-1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Kepler's laws describe common features of the motions of orbiting objects, including their elliptical paths around the sun. Orbits may change due to the gravitational effects from, or collisions with, other objects in the solar system. (HS-ESS1-4) ",
      "comments": [],
      "ancestorIds": [
        "3298BFE3DAF74A1289DB43388CDC0319",
        "4C349631DF28439096F342E97AD786CC"
      ],
      "parentId": "3298BFE3DAF74A1289DB43388CDC0319"
    },
