    "0D391794954C46F681599809E21D95F4": {
      "id": "0D391794954C46F681599809E21D95F4",
      "asnIdentifier": null,
      "position": 12000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "HS-ESS1-C-2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Although active geologic processes, such as plate tectonics and erosion, have destroyed or altered most of the very early rock record on Earth, other objects in the solar system, such as lunar rocks, asteroids, and meteorites, have changed little over billions of years. Studying these objects can provide information about Earth's formation and early history. (HS-ESS1-6)",
      "comments": [],
      "ancestorIds": [
        "626B523AE9314912B7D205CBB7026ABC",
        "4C349631DF28439096F342E97AD786CC"
      ],
      "parentId": "626B523AE9314912B7D205CBB7026ABC"
    },
    "6D45500646784AF08603AFF6C2158BA7": {
      "id": "6D45500646784AF08603AFF6C2158BA7",
      "asnIdentifier": null,
      "position": 11000,
