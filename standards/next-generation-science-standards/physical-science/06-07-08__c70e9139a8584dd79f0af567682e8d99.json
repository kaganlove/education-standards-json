    "4D6E0FD8D33D42E3A1B69861D20E7C57": {
      "id": "4D6E0FD8D33D42E3A1B69861D20E7C57",
      "asnIdentifier": null,
      "position": 7011,
      "depth": 3,
      "listId": "i",
      "statementNotation": "MS-PS1-6",
      "altStatementNotation": null,
