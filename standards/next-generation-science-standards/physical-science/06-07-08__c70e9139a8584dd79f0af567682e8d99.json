      "depth": 2,
      "listId": "b",
      "statementNotation": "PS2.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Types of Interactions",
      "comments": [],
      "ancestorIds": [
        "B547391BA9954883BC850F421CA670F8",
        "EB9B10FF77334141A65A6F51E473DBD6"
      ],
      "parentId": "B547391BA9954883BC850F421CA670F8"
    },
    "B547391BA9954883BC850F421CA670F8": {
