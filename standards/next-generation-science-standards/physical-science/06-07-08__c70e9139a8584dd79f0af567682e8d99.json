    "2B7AFA335E9D48109475081F258EE764": {
      "id": "2B7AFA335E9D48109475081F258EE764",
      "asnIdentifier": null,
      "position": 7006,
      "depth": 3,
      "listId": "v",
      "statementNotation": "MS-PS1-6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Some chemical reactions release energy, others store energy",
      "comments": [],
      "ancestorIds": [
        "A3D350B13ABF4365BF050FDB5BEC7E12",
        "74BF1D4775824A07A0FDA33D70EEC934",
        "AC1D008158EB487B8B4F84A4F820A4A1"
      ],
      "parentId": "A3D350B13ABF4365BF050FDB5BEC7E12"
    },
    "A73DE80E04074E5597624A91C084C2CD": {
      "id": "A73DE80E04074E5597624A91C084C2CD",
      "asnIdentifier": null,
      "position": 7005,
      "depth": 3,
      "listId": "iv",
      "statementNotation": "MS-PS1-5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The total number of each type of atom is conserved, and thus\nthe mass does not change.",
      "comments": [],
      "ancestorIds": [
        "A3D350B13ABF4365BF050FDB5BEC7E12",
        "74BF1D4775824A07A0FDA33D70EEC934",
        "AC1D008158EB487B8B4F84A4F820A4A1"
      ],
      "parentId": "A3D350B13ABF4365BF050FDB5BEC7E12"
    },
    "DDF04734AAA54304B39F05DA0C8F3F4F": {
      "id": "DDF04734AAA54304B39F05DA0C8F3F4F",
      "asnIdentifier": null,
      "position": 6005,
      "depth": 3,
      "listId": "iii",
      "statementNotation": "MS-PS1-5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Substances react chemically in characteristic ways. In a\nchemical process, the atoms that make up the original\nsubstances are regrouped into different molecules, and these\nnew substances have different properties from those of the\nreactants.",
      "comments": [],
      "ancestorIds": [
        "A3D350B13ABF4365BF050FDB5BEC7E12",
        "74BF1D4775824A07A0FDA33D70EEC934",
        "AC1D008158EB487B8B4F84A4F820A4A1"
      ],
      "parentId": "A3D350B13ABF4365BF050FDB5BEC7E12"
    },
