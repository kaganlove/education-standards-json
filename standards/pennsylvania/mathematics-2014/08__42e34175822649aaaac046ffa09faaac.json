    "4E8BA2DD18A946CB9BE64B0AC9F69D7D": {
      "id": "4E8BA2DD18A946CB9BE64B0AC9F69D7D",
      "asnIdentifier": null,
      "position": 1025,
      "depth": 3,
      "listId": "4",
      "statementNotation": "M08.B-E.3.1.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Solve systems of two linear equations in two\nvariables algebraically and estimate solutions by\ngraphing the equations. Solve simple cases by\ninspection.\nExample: 3x + 2y = 5 and 3x + 2y = 6 have no\nsolution because 3x + 2y cannot simultaneously be\n5 and 6.",
      "comments": [],
      "ancestorIds": [
        "3440722A638D4C04917FFDC800B45FC5",
        "5B010CD0FEC64839B9393BB201A26EF0",
        "BE424BB00AA949DEA810E6F0A1F9BCCF"
      ],
      "parentId": "3440722A638D4C04917FFDC800B45FC5"
    },
    "50D3803AFD0A4086B274AA6A5251BDB6": {
