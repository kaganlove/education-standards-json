    "1C7FE73C0DC44F96A4F8255F54AE8424": {
      "id": "1C7FE73C0DC44F96A4F8255F54AE8424",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 3,
      "listId": "4",
      "statementNotation": "M08.B-E.1.1.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Perform operations with numbers expressed in\nscientific notation, including problems where both\ndecimal and scientific notation are used. Express\nanswers in scientific notation and choose units of\nappropriate size for measurements of very large or\nvery small quantities (e.g., use millimeters per year\nfor seafloor spreading). Interpret scientific notation\nthat has been generated by technology (e.g.,\ninterpret 4.7EE9 displayed on a calculator as\n4.7 × 109\n).",
      "comments": [],
      "ancestorIds": [
        "4FB6C49FAAC6431E9F9541B18C7999A3",
