      "asnIdentifier": null,
      "position": 1024,
      "depth": 3,
      "listId": "3",
      "statementNotation": "M08.B-E.3.1.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret solutions to a system of two linear\nequations in two variables as points of intersection\nof their graphs because points of intersection\nsatisfy both equations simultaneously. ",
      "comments": [],
      "ancestorIds": [
        "3440722A638D4C04917FFDC800B45FC5",
        "5B010CD0FEC64839B9393BB201A26EF0",
        "BE424BB00AA949DEA810E6F0A1F9BCCF"
