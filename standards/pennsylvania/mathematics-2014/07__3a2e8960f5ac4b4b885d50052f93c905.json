      "id": "2764AF6585D84DED8DC98C61D8F60637",
      "asnIdentifier": null,
      "position": 8009,
      "depth": 1,
      "listId": "1",
      "statementNotation": "M07.B-E.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Represent expressions in equivalent forms.",
      "comments": [],
      "ancestorIds": [
        "C7754E4062624FFFB2351602F8899302"
      ],
      "parentId": "C7754E4062624FFFB2351602F8899302"
