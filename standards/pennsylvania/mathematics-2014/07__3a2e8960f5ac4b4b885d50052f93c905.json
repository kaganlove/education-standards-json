      "asnIdentifier": null,
      "position": 7002,
      "depth": 2,
      "listId": "1",
      "statementNotation": "M07.A-R.1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Analyze, recognize, and\nrepresent proportional\nrelationships and use them to\nsolve real-world and\nmathematical problems. ",
      "comments": [],
      "ancestorIds": [
        "5BF6B583BCD344A9AB04675598C49543",
        "45CFBD2B4BFB4D4B99F4880347E361A4"
      ],
