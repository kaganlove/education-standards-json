    "41B7BC331343492C8D1CDB5AF94C1DB3": {
      "id": "41B7BC331343492C8D1CDB5AF94C1DB3",
      "asnIdentifier": null,
      "position": 39000,
      "depth": 0,
      "listId": "Strand 2",
      "statementNotation": "S2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students are acquiring Irish in an Irish-language environment. They notice both the accuracy and\nthe richness of the style of the language. They understand what it means to be a good language\nuser, and they try to emulate users who have a rich, accurate grasp of the language. They consider\nhow the language works so as to avoid any inaccuracy. As they are surrounded by the language,\nthey are very familiar with the living culture of Irish and have an opportunity to internalise that\nculture. They understand the descriptive power of the language and their respect for the language\ncommunity grows. They identify with the community and are encouraged to use the language in\nthe language community.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
