    "E9665BCABB784A3EA834ED378009E9BC": {
      "id": "E9665BCABB784A3EA834ED378009E9BC",
      "asnIdentifier": null,
      "position": 1031,
      "depth": 2,
      "listId": "1",
      "statementNotation": "CCR.MD.A.1.1",
      "statementLabel": null,
      "description": "Express the length of an object as a whole number of length units, by laying multiple copies of a shorter\n\nobject (the length unit) end to end; understand that the length measurement of an object is the number of\n\nsame-size length units that span it with no gaps or overlaps. Limit to contexts where the object being\n\nmeasured is spanned by a whole number of length units with no gaps or overlaps.",
      "ancestorIds": [
        "66D2C3E1A18A493F8C83370E6520B345",
        "E5ACB6A91D1A45A881A0E465CFC609E2"
      ],
      "parentId": "66D2C3E1A18A493F8C83370E6520B345"
    },
