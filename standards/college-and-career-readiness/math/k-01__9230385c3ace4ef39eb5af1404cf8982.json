      "asnIdentifier": null,
      "position": 1032,
      "depth": 1,
      "listId": "2",
      "statementNotation": "CCR.MD.A.2",
      "statementLabel": null,
      "description": "Represent and interpret data.",
      "ancestorIds": [
        "E5ACB6A91D1A45A881A0E465CFC609E2"
      ],
      "parentId": "E5ACB6A91D1A45A881A0E465CFC609E2"
