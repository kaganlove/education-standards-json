      "asnIdentifier": null,
      "position": 1026,
      "depth": 2,
      "listId": "1",
      "statementNotation": "CCR.G.A.1.1",
      "statementLabel": null,
      "description": "Analyze and compare two- and three-dimensional shapes, in different sizes and orientations, using\n\ninformal language to describe their similarities, differences, parts (e.g., number of sides and vertices/\n\n“corners”) and other attributes (e.g., having sides of equal length).",
      "ancestorIds": [
        "D954210D80DD487C8D9D9DEB68F521C0",
        "C465683494124A1CBAAC9BCAB4352F23"
      ],
      "parentId": "D954210D80DD487C8D9D9DEB68F521C0"
    },
    "D954210D80DD487C8D9D9DEB68F521C0": {
