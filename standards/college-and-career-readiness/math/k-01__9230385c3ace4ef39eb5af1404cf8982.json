      "asnIdentifier": null,
      "position": 1033,
      "depth": 2,
      "listId": "1",
      "statementNotation": "CCR.MD.A.2.1",
      "statementLabel": null,
      "description": "Organize, represent, and interpret data with up to three categories; ask and answer questions about the\n\ntotal number of data points, how many in each category, and how many more or less are in one category\n\nthan in another.",
      "ancestorIds": [
        "39E231B644CA4346A0980A4B19DD5672",
        "E5ACB6A91D1A45A881A0E465CFC609E2"
      ],
