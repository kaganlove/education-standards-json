    "B984FE25AF1646FF85E478F10AD815E0": {
      "id": "B984FE25AF1646FF85E478F10AD815E0",
      "asnIdentifier": null,
      "position": 1028,
      "depth": 2,
      "listId": "1",
      "statementNotation": "CCR.G.A.2.1",
      "statementLabel": null,
      "description": "Compose two-dimensional shapes (rectangles, squares, trapezoids, triangles, half-circles, and quarter-\ncircles) or three-dimensional shapes (cubes, right rectangular prisms, right circular cones, and right\n\ncircular cylinders) to create a composite shape, and compose new shapes from the composite shape.",
      "ancestorIds": [
        "2AAEBAB0A51D4437B4EEA2AF7DD8BCF7",
        "C465683494124A1CBAAC9BCAB4352F23"
      ],
      "parentId": "2AAEBAB0A51D4437B4EEA2AF7DD8BCF7"
    },
    "2AAEBAB0A51D4437B4EEA2AF7DD8BCF7": {
