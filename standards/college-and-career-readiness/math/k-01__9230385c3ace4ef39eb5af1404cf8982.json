      ],
      "parentId": "D2DF893171674C56AEB065776D4448EA"
    },
    "7868F7EA2AE545CE9D99C579B08CACF4": {
      "id": "7868F7EA2AE545CE9D99C579B08CACF4",
      "asnIdentifier": null,
      "position": 1002,
      "depth": 2,
      "listId": "1",
      "statementNotation": "CCR.NBT.A.1.1",
      "statementLabel": null,
      "description": "Understand that the two digits of a two-digit number represent amounts of tens and ones.",
