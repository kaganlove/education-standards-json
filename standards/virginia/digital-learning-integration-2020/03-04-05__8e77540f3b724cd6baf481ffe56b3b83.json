      "position": 1007,
      "depth": 3,
      "listId": "1",
      "statementNotation": "3-5.1.1.C.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students seek\nfeedback from\npeople,\ntechnologies, and\nstaff familiar with\nassistive\ntechnologies to\nmake\nimprovements, and\nuse appropriate\ntechnologies to\nshare and\ndemonstrate\nlearning. ",
      "comments": [],
      "ancestorIds": [
        "4283A213D9D2427E9661D72A18996272",
        "CC4FA212DCB64526A358C1461319EB2A",
        "A82B53555E9D41378DE6E995BCF86887"
      ],
      "parentId": "4283A213D9D2427E9661D72A18996272"
    },
