      "asnIdentifier": null,
      "position": 1004,
      "depth": 2,
      "listId": "B",
      "statementNotation": "3-5.1.1.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Build networks\nand customize\ntheir learning\nenvironments in\nways that support\nthe learning\nprocess.",
