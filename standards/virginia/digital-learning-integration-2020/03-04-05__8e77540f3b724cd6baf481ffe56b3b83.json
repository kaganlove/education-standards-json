      "asnIdentifier": null,
      "position": 1006,
      "depth": 2,
      "listId": "C",
      "statementNotation": "3-5.1.1.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use technology to\nseek feedback that\ninforms and\nimproves their\npractice and to\ndemonstrate their\nlearning in a\nvariety of ways. ",
      "comments": [],
      "ancestorIds": [
        "CC4FA212DCB64526A358C1461319EB2A",
        "A82B53555E9D41378DE6E995BCF86887"
      ],
      "parentId": "CC4FA212DCB64526A358C1461319EB2A"
