      "parentId": null
    },
    "DB20BEDE6B314039A97FC29E3B5A7DD3": {
      "id": "DB20BEDE6B314039A97FC29E3B5A7DD3",
      "asnIdentifier": null,
      "position": 1034,
      "depth": 3,
      "listId": "i",
      "statementNotation": "1.CE.1.i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe the equal symbol (=) as a balance representing an equivalent relationship between\nexpressions on either side of the equal symbol (e.g., 6 and 1 is the same as 4 and 3; 6 + 1 is\nbalanced with 4 + 3; 6 + 1 = 4 + 3).",
      "comments": [],
      "ancestorIds": [
        "3E41789EEA6341DFB0F14875FD37B2AB",
        "B753C1AEF7AD4C3699C29155BC688D80"
      ],
      "parentId": null
    },
    "A8E9D5AE10494612AE4CB37527746E09": {
