      "id": "A4B4F551010A4ECDB8741EA5AA10C6D6",
      "asnIdentifier": null,
      "position": 1029,
      "depth": 3,
      "listId": "d",
      "statementNotation": "1.CE.1.d",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate, recognize, and describe part-part-whole relationships for numbers up to 20 in a\nvariety of configurations (e.g., beaded racks, double ten frames).",
      "comments": [],
      "ancestorIds": [
        "3E41789EEA6341DFB0F14875FD37B2AB",
        "B753C1AEF7AD4C3699C29155BC688D80"
      ],
      "parentId": null
    },
