  "standards": {
    "1589320DE8A848E28F2CF78035AFE9AF": {
      "id": "1589320DE8A848E28F2CF78035AFE9AF",
      "asnIdentifier": null,
      "position": 1036,
      "depth": 1,
      "listId": "4",
      "statementNotation": "6-8.GC.D.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Select collaborative technologies and use them to work with others to investigate\nand develop solutions related to local and global issues.",
      "comments": [],
      "ancestorIds": [
        "DBD704A2302F442E94B58012E68D74BA"
      ],
      "parentId": "DBD704A2302F442E94B58012E68D74BA"
    },
    "CE41037559E046E18A49F0C9C08E581E": {
      "id": "CE41037559E046E18A49F0C9C08E581E",
      "asnIdentifier": null,
      "position": 1035,
      "depth": 1,
      "listId": "3",
      "statementNotation": "6-8.GC.C.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Determine their role on a team to meet goals, based on their knowledge of\ntechnology and content, as well as personal preference.",
      "comments": [],
      "ancestorIds": [
        "DBD704A2302F442E94B58012E68D74BA"
      ],
      "parentId": "DBD704A2302F442E94B58012E68D74BA"
    },
    "EC0D8A74384E460CB30B6CFDDFD8D80D": {
      "id": "EC0D8A74384E460CB30B6CFDDFD8D80D",
      "asnIdentifier": null,
      "position": 1034,
      "depth": 1,
      "listId": "2",
      "statementNotation": "6-8.GC.B.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use collaborative technologies to connect with others, including peers, experts,\nand community members, to learn about issues and problems or to gain broader\nperspectives.",
      "comments": [],
      "ancestorIds": [
        "DBD704A2302F442E94B58012E68D74BA"
      ],
      "parentId": "DBD704A2302F442E94B58012E68D74BA"
    },
    "BA182AE528D74E2C955C3DE73C4393F9": {
      "id": "BA182AE528D74E2C955C3DE73C4393F9",
      "asnIdentifier": null,
      "position": 1033,
      "depth": 1,
      "listId": "1",
      "statementNotation": "6-8.GC.A.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use digital tools to interact with others to develop a richer understanding of\ndifferent perspectives and cultures.",
      "comments": [],
      "ancestorIds": [
        "DBD704A2302F442E94B58012E68D74BA"
      ],
      "parentId": "DBD704A2302F442E94B58012E68D74BA"
    },
    "DBD704A2302F442E94B58012E68D74BA": {
      "id": "DBD704A2302F442E94B58012E68D74BA",
      "asnIdentifier": null,
      "position": 1032,
      "depth": 0,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Global Collaborator",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "3796A4CB51804CEEB6BEAB1037D4C5CB": {
      "id": "3796A4CB51804CEEB6BEAB1037D4C5CB",
      "asnIdentifier": null,
      "position": 1031,
      "depth": 1,
      "listId": "5",
      "statementNotation": "6-8.CC.D.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Publish or present content designed for specific audiences and select platforms\nthat will effectively convey their ideas to those audiences.",
      "comments": [],
      "ancestorIds": [
        "554DB64A266945C691AFA8CD81A29093"
      ],
      "parentId": "554DB64A266945C691AFA8CD81A29093"
    },
    "5C8E805462A9459496AD5DA29FCBA304": {
      "id": "5C8E805462A9459496AD5DA29FCBA304",
      "asnIdentifier": null,
      "position": 1030,
      "depth": 1,
      "listId": "4",
      "statementNotation": "6-8.CC.C.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Communicate complex ideas clearly using various digital tools to convey the\nconcepts textually, visually, graphically, etc.",
      "comments": [],
      "ancestorIds": [
        "554DB64A266945C691AFA8CD81A29093"
      ],
      "parentId": "554DB64A266945C691AFA8CD81A29093"
    },
    "5021876AC69F4DE7A3429D73BF19045B": {
      "id": "5021876AC69F4DE7A3429D73BF19045B",
      "asnIdentifier": null,
      "position": 1029,
      "depth": 1,
      "listId": "3",
      "statementNotation": "6-8.CC.B.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create original works and apply strategies for responsibly remixing or repurposing\nto create new artifacts.",
      "comments": [],
      "ancestorIds": [
        "554DB64A266945C691AFA8CD81A29093"
      ],
      "parentId": "554DB64A266945C691AFA8CD81A29093"
    },
    "6E6902F311FE47AD874AF58825BC047C": {
      "id": "6E6902F311FE47AD874AF58825BC047C",
      "asnIdentifier": null,
      "position": 1028,
      "depth": 1,
      "listId": "2",
      "statementNotation": "6-8.CC.A.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand and demonstrate how to construct an email to the appropriate\naudience, including replying to individual, groups, BCC, and CC.",
      "comments": [],
      "ancestorIds": [
        "554DB64A266945C691AFA8CD81A29093"
      ],
      "parentId": "554DB64A266945C691AFA8CD81A29093"
    },
    "88DC8BCBA3FC4F08B0322A8556477A47": {
      "id": "88DC8BCBA3FC4F08B0322A8556477A47",
      "asnIdentifier": null,
      "position": 1027,
      "depth": 1,
      "listId": "1",
      "statementNotation": "6-8.CC.A.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Select appropriate platforms and tools to create, share, and communicate their\nwork effectively.",
      "comments": [],
      "ancestorIds": [
        "554DB64A266945C691AFA8CD81A29093"
      ],
      "parentId": "554DB64A266945C691AFA8CD81A29093"
    },
    "554DB64A266945C691AFA8CD81A29093": {
      "id": "554DB64A266945C691AFA8CD81A29093",
