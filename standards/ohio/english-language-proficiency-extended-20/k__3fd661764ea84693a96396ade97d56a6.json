      "statementNotation": "K.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "An English learner with\nsignificant cognitive disabilities can\nparticipate in grade-appropriate oral\nand written exchanges of\ninformation, ideas, and analyses,\nresponding to peer, audience, or\nreader comments and questions. ",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
