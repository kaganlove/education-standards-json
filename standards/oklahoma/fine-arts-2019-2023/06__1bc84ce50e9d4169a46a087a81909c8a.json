      "position": 4029,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.CN.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nSynthesize  and relate knowledge and personal experiences to make art.\n",
      "comments": [],
      "ancestorIds": [
        "373D464638454D95AFBDF63404CFB5E5"
      ],
      "parentId": "373D464638454D95AFBDF63404CFB5E5"
