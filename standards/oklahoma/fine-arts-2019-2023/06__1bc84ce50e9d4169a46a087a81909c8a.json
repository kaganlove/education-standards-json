    "8518C6AAE1A94784BC000302ED98B526": {
      "id": "8518C6AAE1A94784BC000302ED98B526",
      "asnIdentifier": null,
      "position": 4018,
      "depth": 2,
      "listId": "1",
      "statementNotation": "6.D.CR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Revise dance compositions using collaboratively developed artistic criteria. Explain \nreasons for  revisions and how choices made relate to artistic intent.\n",
      "comments": [],
      "ancestorIds": [
        "37138E4549744DD3AB8A6FFEE2E3B539",
        "CDE084E87C684788A7300412559536C8"
      ],
      "parentId": "37138E4549744DD3AB8A6FFEE2E3B539"
    },
