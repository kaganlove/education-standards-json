    "4B141ADACE9C4CF5AD96AB15E2622C5A": {
      "id": "4B141ADACE9C4CF5AD96AB15E2622C5A",
      "asnIdentifier": null,
      "position": 4019,
      "depth": 2,
      "listId": "2",
      "statementNotation": "6.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nExplore or invent a system to record a dance sequence through writing, symbols, or a form \nof media technology. \n",
      "comments": [],
      "ancestorIds": [
        "37138E4549744DD3AB8A6FFEE2E3B539",
        "CDE084E87C684788A7300412559536C8"
