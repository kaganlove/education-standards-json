  "standards": {
    "1DCB6C2AAAB943E38135C62CFE6DD5FB": {
      "id": "1DCB6C2AAAB943E38135C62CFE6DD5FB",
      "asnIdentifier": null,
      "position": 4033,
      "depth": 2,
      "listId": "1",
      "statementNotation": "6.D.CN.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nInterpret and demonstrate how the movement and qualities of a dance communicate its \ncultural, historical, and/or community purpose or meaning, including those of Oklahoma  Native \nAmerican tribes and communities.\n",
      "comments": [],
      "ancestorIds": [
        "D3D69C18BB584EC4A640E01A7998CAD4",
        "373D464638454D95AFBDF63404CFB5E5"
      ],
      "parentId": "D3D69C18BB584EC4A640E01A7998CAD4"
    },
    "D3D69C18BB584EC4A640E01A7998CAD4": {
      "id": "D3D69C18BB584EC4A640E01A7998CAD4",
      "asnIdentifier": null,
      "position": 4032,
      "depth": 1,
      "listId": "2",
      "statementNotation": "D.CN.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nRelate artistic ideas and works with societal, cultural  and historical contexts to deepen \nunderstanding.\n",
      "comments": [],
      "ancestorIds": [
        "373D464638454D95AFBDF63404CFB5E5"
      ],
      "parentId": "373D464638454D95AFBDF63404CFB5E5"
    },
    "7DCDA188F6C748E694B5A4AC4EE4D88D": {
      "id": "7DCDA188F6C748E694B5A4AC4EE4D88D",
      "asnIdentifier": null,
      "position": 4031,
      "depth": 2,
      "listId": "2",
      "statementNotation": "6.D.CN.1.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Choose a topic, concept, or content from another subject of interest and research how \nother art forms have expressed the topic. Create and explain a dance study that expresses the  \nidea.\n",
      "comments": [],
      "ancestorIds": [
        "49855514431141548779F8982B50957C",
        "373D464638454D95AFBDF63404CFB5E5"
      ],
      "parentId": "49855514431141548779F8982B50957C"
    },
    "6CE356EB9FDA48A7A0A040CA3B221BA5": {
      "id": "6CE356EB9FDA48A7A0A040CA3B221BA5",
      "asnIdentifier": null,
      "position": 4030,
      "depth": 2,
      "listId": "1",
      "statementNotation": "6.D.CN.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nObserve the movement characteristics or qualities in a specific dance genre. Describe \ndifferences and similarities to one's attitudes and movement preferences.\n",
      "comments": [],
      "ancestorIds": [
        "49855514431141548779F8982B50957C",
        "373D464638454D95AFBDF63404CFB5E5"
      ],
      "parentId": "49855514431141548779F8982B50957C"
    },
