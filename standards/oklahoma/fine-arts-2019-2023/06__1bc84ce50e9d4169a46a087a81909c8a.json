      "id": "EBD9D17C85C94541A1E34F4C20A899B0",
      "asnIdentifier": null,
      "position": 4011,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.CR.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nGenerate and conceptualize artistic ideas and work.\n",
      "comments": [],
      "ancestorIds": [
        "CDE084E87C684788A7300412559536C8"
      ],
      "parentId": "CDE084E87C684788A7300412559536C8"
    },
