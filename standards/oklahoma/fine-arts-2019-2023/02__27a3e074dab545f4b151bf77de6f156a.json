    "B4A97CD1B73C4709A3B74BB2B066F72F": {
      "id": "B4A97CD1B73C4709A3B74BB2B066F72F",
      "asnIdentifier": null,
      "position": 3012,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.DT.PR.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\n\n\n\nDemonstrate that there are multiple choices for every aspect of drama/theatre work \n(character, character traits, locomotor/non-locomotor movement, speaking voice, facial expressions, \nset, props, costumes, etc.) and\n\nselect the  most supportable choice for the moment.\n",
      "comments": [],
      "ancestorIds": [
        "BABE3589A1764E81BEB5DEF6E2E72748",
        "D58042EC3ADC4E8880415407E9747818"
      ],
