    "69D54D2F32B54233B2C44CA8C8D0CB7A": {
      "id": "69D54D2F32B54233B2C44CA8C8D0CB7A",
      "asnIdentifier": null,
      "position": 4016,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.DT.PR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPresent a nonÂ­-exhibitional drama work with a defined purpose or intent with characters \nthat represent a diversity of people/cultures; use personal and partner space to  convey meaning \nabout characters, relationships, mood, and story.\n",
      "comments": [],
