    },
    "C951F8EFEC6442A694EC58BD76C9E42B": {
      "id": "C951F8EFEC6442A694EC58BD76C9E42B",
      "asnIdentifier": null,
      "position": 3014,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.DT.PR.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nDevelop expertise by assuming roles in a variety of dramatic forms and experimenting  \nwith the body and voice while making choices for setting, props, costumes, and technical elements \nto convey meaning and create an emotional impact.\n",
