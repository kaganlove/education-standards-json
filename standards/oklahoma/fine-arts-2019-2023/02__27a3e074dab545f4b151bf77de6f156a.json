      "asnIdentifier": null,
      "position": 3007,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.DT.CR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPrepare a unified drama/theatre work for presentation that conveys meaning by using \nbasic theatre vocabulary and demonstrating  skills of drama/theatre, which are also \"skills of the \nmind\": imagination, focus, concentration.\n",
      "comments": [],
      "ancestorIds": [
        "66322F0B1BA042548EFDE6C3500C8AE6",
        "CA2FC12A3B014BC5AF52A627747AB90E"
