    "9FE38927D55A4602BC2327963D53CD2A": {
      "id": "9FE38927D55A4602BC2327963D53CD2A",
      "asnIdentifier": null,
      "position": 3009,
      "depth": 2,
      "listId": "3",
      "statementNotation": "2.DT.CR.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integrate design elements that create an emotional impact or convey meaning by creating \npuppets, masks, and costume pieces to help define characters and selecting or creating materials to \n be used for scenery, props, costumes, and sound effects that support the story and add to its \nemotional impact and meaning.\n",
      "comments": [],
      "ancestorIds": [
        "66322F0B1BA042548EFDE6C3500C8AE6",
        "CA2FC12A3B014BC5AF52A627747AB90E"
      ],
      "parentId": "66322F0B1BA042548EFDE6C3500C8AE6"
    },
    "5C2B6D188490412A9AD71BD56067A43D": {
      "id": "5C2B6D188490412A9AD71BD56067A43D",
      "asnIdentifier": null,
      "position": 3008,
      "depth": 2,
      "listId": "2",
      "statementNotation": "2.DT.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use the  body to create a vivid character by manipulating energy, gestures, shape, body \nlanguage, and\nmovement; use voice to create a vivid character by manipulating volume, pitch, tone, rate, and \nclarity; and justify character  choices.         \n",
      "comments": [],
      "ancestorIds": [
        "66322F0B1BA042548EFDE6C3500C8AE6",
        "CA2FC12A3B014BC5AF52A627747AB90E"
      ],
      "parentId": "66322F0B1BA042548EFDE6C3500C8AE6"
    },
    "C07073D589BA4EB69509F56F9B5A0C17": {
