      "asnIdentifier": null,
      "position": 1010,
      "depth": 2,
      "listId": "1",
      "statementNotation": "I.D.PR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nDemonstrate characterÂ­ based skills (e.g., commitment, dependability, responsibility, \ncooperation) when preparing for performances. Demonstrate performance etiquette and performance \npractices during class, rehearsal. and performance.\n",
      "comments": [],
      "ancestorIds": [
        "ED0CB497956D461792D79249D12E9374",
        "5EF334AA37944664ACBE89B06DA86705"
      ],
      "parentId": "ED0CB497956D461792D79249D12E9374"
