    "866AED4BA91940E7B8E3BA03686D1939": {
      "id": "866AED4BA91940E7B8E3BA03686D1939",
      "asnIdentifier": null,
      "position": 2010,
      "depth": 2,
      "listId": "3",
      "statementNotation": "1.DT.CR.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integrate design elements that create an emotional impact or convey meaning in play \nplans, dramatic play, or guided drama by choosing a single object for multiple, imaginative \nrepresentations; creating puppets, masks, and  costume pieces; and creating sound effects that \nsupport the story.\n",
      "comments": [],
      "ancestorIds": [
        "E1C16DBD22874730BA7164C297C18478",
        "EF816978D95E41059D6D6271528F6607"
      ],
      "parentId": "E1C16DBD22874730BA7164C297C18478"
    },
    "CA49EFEF92964AB79F1A97716196FBAC": {
      "id": "CA49EFEF92964AB79F1A97716196FBAC",
      "asnIdentifier": null,
      "position": 2009,
      "depth": 2,
      "listId": "2",
      "statementNotation": "1.DT.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use the  body to create a vivid character using energy, gestures, shape, body language, \nand movement and use voice to create a vivid character by manipulating volume, pitch, tone, rate, \nand clarity.\n",
      "comments": [],
      "ancestorIds": [
        "E1C16DBD22874730BA7164C297C18478",
        "EF816978D95E41059D6D6271528F6607"
      ],
      "parentId": "E1C16DBD22874730BA7164C297C18478"
    },
