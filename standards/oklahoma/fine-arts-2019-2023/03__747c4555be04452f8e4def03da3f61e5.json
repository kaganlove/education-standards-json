      "parentId": "4E0E4A5ED7F0420F99B59083A29C2AB6"
    },
    "C0F9D8B9397C4BDB967091BD83C2DC70": {
      "id": "C0F9D8B9397C4BDB967091BD83C2DC70",
      "asnIdentifier": null,
      "position": 3007,
      "depth": 2,
      "listId": "2",
      "statementNotation": "3.DT.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use the  body to create and refine a vivid character by manipulating energy, gestures, \nshape, body language, and\nmovement; use voice to create and refine a vivid character by manipulating volume, pitch, tone, \nrate, and clarity; and  justify character choices.\n",
      "comments": [],
      "ancestorIds": [
        "4E0E4A5ED7F0420F99B59083A29C2AB6",
        "B431BE0156D04845830E92E6C8868B92"
      ],
      "parentId": "4E0E4A5ED7F0420F99B59083A29C2AB6"
    },
