    "B4619E014923438BB5BD2273751A8844": {
      "id": "B4619E014923438BB5BD2273751A8844",
      "asnIdentifier": null,
      "position": 4007,
      "depth": 2,
      "listId": "3",
      "statementNotation": "3.DT.CR.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integrate design elements that create an emotional impact or convey meaning by creating \npuppets, masks, and  costume    pieces to  help\n\ndefine characters and selecting or creating materials to  be used for scenery, props, costumes, and \nsound effects that support the story and add to  its emotional impact and meaning.\n",
      "comments": [],
      "ancestorIds": [
        "4E0E4A5ED7F0420F99B59083A29C2AB6",
        "B431BE0156D04845830E92E6C8868B92"
