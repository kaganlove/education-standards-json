      "asnIdentifier": null,
      "position": 7018,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.RE.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With\nprompting and support, create or interpret a drama/theatre  work (play plans, dramatic play, guided \ndrama, and/or in developmentally appropriate theatrical performances)  by asking questions, sharing \npersonal responses, and reflecting upon performances viewed.\n",
      "comments": [],
      "ancestorIds": [
        "664C8F1A07DB4887AE8C75DFE21DF9A2",
        "32D43984B6D54702B54B43B3B6DE9144"
