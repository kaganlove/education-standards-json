      "asnIdentifier": null,
      "position": 4006,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.CR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPrepare a unified drama/theatre  work for presentation  by demonstrating skills of \ndrama/theatre, which are\nalso \"skills of the  mind\": imagination, focus, concentration.\n",
      "comments": [],
      "ancestorIds": [
        "517099C9157D477EAE96797F38B1AD22",
        "8BE3120041044B508DC92CB269721E89"
      ],
      "parentId": "517099C9157D477EAE96797F38B1AD22"
