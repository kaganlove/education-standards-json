    "4F8195C801484B0B878E3D2A76DF0E1A": {
      "id": "4F8195C801484B0B878E3D2A76DF0E1A",
      "asnIdentifier": null,
      "position": 7013,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.PR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPerform a non­ exhibitionaI drama/theatre work (play plans, dramatic play, and \nguided drama)  with characters that are part of recognizable and shared human experiences (e.g. \nfamily, workers, community helpers, etc.).\n",
      "comments": [],
      "ancestorIds": [
        "F43AC274AF7740978165701954D3F94E",
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "F43AC274AF7740978165701954D3F94E"
    },
    "F43AC274AF7740978165701954D3F94E": {
      "id": "F43AC274AF7740978165701954D3F94E",
      "asnIdentifier": null,
      "position": 6013,
      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.PR.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nConvey meaning through the presentation of an artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "D126CE282C1E4FF586F2398FF17A5102"
    },
    "8BA81D0AB37D4C7289D667827D170A4B": {
      "id": "8BA81D0AB37D4C7289D667827D170A4B",
      "asnIdentifier": null,
      "position": 6012,
      "depth": 2,
      "listId": "2",
      "statementNotation": "PK.DT.PR.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make choices for multiple aspects of drama/theatre work (including design elements of \nplaying space).\n",
      "comments": [],
      "ancestorIds": [
        "D4F709DD6EA54E21B86903525472E334",
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "D4F709DD6EA54E21B86903525472E334"
    },
    "5BBB93EAC83344E396248F6FA5AEA77A": {
      "id": "5BBB93EAC83344E396248F6FA5AEA77A",
      "asnIdentifier": null,
      "position": 5012,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.PR.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop expertise through a personalization of techniques by assuming roles in a \nvariety of dramatic forms (e.g.: play plans, dramatic play, guided drama, pantomime, tableau, \npuppetry, story enactment, etc.) and by observing, listening, and responding to the work of others.\n",
      "comments": [],
      "ancestorIds": [
        "D4F709DD6EA54E21B86903525472E334",
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "D4F709DD6EA54E21B86903525472E334"
    },
    "D4F709DD6EA54E21B86903525472E334": {
      "id": "D4F709DD6EA54E21B86903525472E334",
      "asnIdentifier": null,
      "position": 5011,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.PR.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nDevelop and refine artistic techniques  and work for presentation.\n",
      "comments": [],
      "ancestorIds": [
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "D126CE282C1E4FF586F2398FF17A5102"
    },
    "7A4A87ABA9924B01827BE7800C482F44": {
      "id": "7A4A87ABA9924B01827BE7800C482F44",
      "asnIdentifier": null,
      "position": 4011,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.PR.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate that there are multiple choices for every aspect of drama/theatre work \n(movement, speaking voice, etc.) and select the most supportable choice for the moment through\nidentification of essential events.\n\n\n\n\n\n\n\n\n",
      "comments": [],
      "ancestorIds": [
        "7235AA8594E64DDC99E434ACFB1E61E4",
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "7235AA8594E64DDC99E434ACFB1E61E4"
    },
    "7235AA8594E64DDC99E434ACFB1E61E4": {
      "id": "7235AA8594E64DDC99E434ACFB1E61E4",
      "asnIdentifier": null,
      "position": 4010,
      "depth": 1,
      "listId": "1",
      "statementNotation": "DT.PR.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Select, analyze, and  interpret artistic work for presentation.\n",
      "comments": [],
      "ancestorIds": [
        "D126CE282C1E4FF586F2398FF17A5102"
      ],
      "parentId": "D126CE282C1E4FF586F2398FF17A5102"
    },
