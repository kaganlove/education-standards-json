      "id": "664C8F1A07DB4887AE8C75DFE21DF9A2",
      "asnIdentifier": null,
      "position": 7017,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.RE.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret intent and meaning in artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "32D43984B6D54702B54B43B3B6DE9144"
      ],
      "parentId": "32D43984B6D54702B54B43B3B6DE9144"
