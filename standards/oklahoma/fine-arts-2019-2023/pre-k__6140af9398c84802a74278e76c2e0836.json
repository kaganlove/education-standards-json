    },
    "D8309682FAE04914A40CF28E63E68451": {
      "id": "D8309682FAE04914A40CF28E63E68451",
      "asnIdentifier": null,
      "position": 7019,
      "depth": 2,
      "listId": "2",
      "statementNotation": "PK.DT.RE.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Discuss feelings about and reactions to  what was enacted, seen, heard, and felt in \nplay plans, dramatic play, guided drama, and/or in developmentally\n\nappropriate theatrical performances viewed.     \n",
      "comments": [],
      "ancestorIds": [
        "664C8F1A07DB4887AE8C75DFE21DF9A2",
        "32D43984B6D54702B54B43B3B6DE9144"
