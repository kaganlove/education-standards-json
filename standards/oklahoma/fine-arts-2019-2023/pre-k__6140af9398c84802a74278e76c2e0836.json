      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.RE.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nApply criteria  to evaluate artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "32D43984B6D54702B54B43B3B6DE9144"
      ],
      "parentId": "32D43984B6D54702B54B43B3B6DE9144"
