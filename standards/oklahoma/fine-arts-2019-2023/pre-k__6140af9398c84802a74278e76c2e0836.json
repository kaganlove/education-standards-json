    "9E70E89D85B048308C434C531F756402": {
      "id": "9E70E89D85B048308C434C531F756402",
      "asnIdentifier": null,
      "position": 10029,
      "depth": 2,
      "listId": "1",
      "statementNotation": "PK.DT.CN.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nWith\nprompting and support, identify pictures, grade­ appropriate multicultural stories, and real people \nor fictional characters as sources for  play plans, dramatic play or guided drama.\n",
      "comments": [],
      "ancestorIds": [
        "677F48F17F634D3C94105FCD3F4167F6",
        "8026A753077749548BC7AB6BAC24D783"
