    "BF04677F445E4D1E97C42C7EB1ACD6B3": {
      "id": "BF04677F445E4D1E97C42C7EB1ACD6B3",
      "asnIdentifier": null,
      "position": 4016,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.PR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "  Present a non-exhibitional drama/theatre work with a defined purpose or intent and \ncooperate with peers to  demonstrate an understanding  of main idea/theme\n\nthrough setting, costume and other\n    design elements.                                       \n",
      "comments": [],
      "ancestorIds": [
        "1A9CEAC7C38B40C28FA43EE98B5F1291",
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "1A9CEAC7C38B40C28FA43EE98B5F1291"
    },
    "1A9CEAC7C38B40C28FA43EE98B5F1291": {
      "id": "1A9CEAC7C38B40C28FA43EE98B5F1291",
      "asnIdentifier": null,
      "position": 4015,
      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.PR.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nConvey meaning through the presentation of an artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "CF762C93927042408AAE297394ABF61D"
    },
    "8643034DAA444D7FA083F8D686A57506": {
      "id": "8643034DAA444D7FA083F8D686A57506",
      "asnIdentifier": null,
      "position": 4014,
      "depth": 2,
      "listId": "2",
      "statementNotation": "4.DT.PR.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Propose the use of technical elements in a drama/theatre work to create the most \neffective emotional impact and to convey meaning.\n",
      "comments": [],
      "ancestorIds": [
        "8122B2055EA54DA18E10A4563D309D15",
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "8122B2055EA54DA18E10A4563D309D15"
    },
    "DEFED9EFB24A490E9FFCE9FECEEB2120": {
      "id": "DEFED9EFB24A490E9FFCE9FECEEB2120",
      "asnIdentifier": null,
      "position": 3014,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.PR.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Assume roles in a variety of dramatic forms and manipulate the body and voice while \nmaking choices for multiple aspects of drama/theatre work (including acting, design, design \nelements of playing space and audience space, playwriting, etc.) to convey meaning and create an \nemotional impact.\n",
      "comments": [],
      "ancestorIds": [
        "8122B2055EA54DA18E10A4563D309D15",
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "8122B2055EA54DA18E10A4563D309D15"
    },
    "8122B2055EA54DA18E10A4563D309D15": {
      "id": "8122B2055EA54DA18E10A4563D309D15",
      "asnIdentifier": null,
      "position": 3013,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.PR.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop and refine artistic techniques  and work for presentation.\n",
      "comments": [],
      "ancestorIds": [
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "CF762C93927042408AAE297394ABF61D"
    },
    "C7A03D20A11047A4AAE0823D29C716B7": {
      "id": "C7A03D20A11047A4AAE0823D29C716B7",
      "asnIdentifier": null,
      "position": 3012,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.PR.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate that there are multiple choices for  every drama/theatre work and select \nsupportable choices for story/plot, character, setting, design and production elements (e.g. \nplaying space, audience space, props, sound).\n",
      "comments": [],
      "ancestorIds": [
        "5E26C4F226474A83803B537AF605AB5A",
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "5E26C4F226474A83803B537AF605AB5A"
    },
    "5E26C4F226474A83803B537AF605AB5A": {
      "id": "5E26C4F226474A83803B537AF605AB5A",
      "asnIdentifier": null,
      "position": 3011,
      "depth": 1,
      "listId": "1",
      "statementNotation": "DT.PR.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nSelect, analyze, and  interpret artistic work for presentation.\n",
      "comments": [],
      "ancestorIds": [
        "CF762C93927042408AAE297394ABF61D"
      ],
      "parentId": "CF762C93927042408AAE297394ABF61D"
    },
