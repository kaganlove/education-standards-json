    "7070DD76798946DDB263D644FED25336": {
      "id": "7070DD76798946DDB263D644FED25336",
      "asnIdentifier": null,
      "position": 7024,
      "depth": 2,
      "listId": "3",
      "statementNotation": "4.DT.RE.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Observe and discuss how\ncharacters' circumstances and choices impact audience members' perspectives and reactions in a live \nor recorded drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "2D1C0EF0256F4704A8E7A9D5220D35EA",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "2D1C0EF0256F4704A8E7A9D5220D35EA"
    },
    "2470AC06F07142D18F90669D6D1CD021": {
      "id": "2470AC06F07142D18F90669D6D1CD021",
      "asnIdentifier": null,
      "position": 6024,
      "depth": 2,
      "listId": "2",
      "statementNotation": "4.DT.RE.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Ask questions about what is seen and  heard as a result of the design and execution of \ntechnical/production elements (e.g., costumes, props, sets, sound effects, etc.) in drama/theatre  \nwork.\n",
      "comments": [],
      "ancestorIds": [
        "2D1C0EF0256F4704A8E7A9D5220D35EA",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "2D1C0EF0256F4704A8E7A9D5220D35EA"
    },
    "2DD2A4BD051B46948DAC03F53600FDBD": {
      "id": "2DD2A4BD051B46948DAC03F53600FDBD",
      "asnIdentifier": null,
      "position": 6023,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.RE.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Decide as a small group or as an individual what makes a successful performance  by \nexploring the application of criteria for making informed evaluations and dramatic and theatrical \nchoices in a drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "2D1C0EF0256F4704A8E7A9D5220D35EA",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "2D1C0EF0256F4704A8E7A9D5220D35EA"
    },
    "2D1C0EF0256F4704A8E7A9D5220D35EA": {
      "id": "2D1C0EF0256F4704A8E7A9D5220D35EA",
      "asnIdentifier": null,
      "position": 6022,
      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.RE.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nApply criteria to evaluate artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "1B9D672F317542329194E909A8E59285"
    },
    "33756C5B0EB84CAB9B62B474F469D6E6": {
      "id": "33756C5B0EB84CAB9B62B474F469D6E6",
      "asnIdentifier": null,
      "position": 5022,
      "depth": 2,
      "listId": "3",
      "statementNotation": "4.DT.RE.2.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify personal aesthetics to  create and interpret a drama/theatre work by exploring \nthe meaning of a personal aesthetic and\n\nhow to  apply it to  a drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "19321A03841341379BDE7CAF69FDC87C",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "19321A03841341379BDE7CAF69FDC87C"
    },
    "10F242F192EE4370BF84BD27983A6405": {
      "id": "10F242F192EE4370BF84BD27983A6405",
      "asnIdentifier": null,
      "position": 5021,
      "depth": 2,
      "listId": "2",
      "statementNotation": "4.DT.RE.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare and contrast characters and events and describe the similarities and \ndifferences between them in dramatic play, guided drama, and/or in developmentally appropriate \ntheatrical performances viewed.\n",
      "comments": [],
      "ancestorIds": [
        "19321A03841341379BDE7CAF69FDC87C",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "19321A03841341379BDE7CAF69FDC87C"
    },
    "3C8038C1B451497CA27DBA1ACA3CC407": {
      "id": "3C8038C1B451497CA27DBA1ACA3CC407",
      "asnIdentifier": null,
      "position": 5020,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.RE.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use personal experience and background  knowledge to create or interpret a \ndrama/theatre  work by exploring, expressing, and justifying personal preferences about the content \nof dramatic play, guided drama, and/or developmentally appropriate theatrical performances viewed.\n",
      "comments": [],
      "ancestorIds": [
        "19321A03841341379BDE7CAF69FDC87C",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "19321A03841341379BDE7CAF69FDC87C"
    },
    "19321A03841341379BDE7CAF69FDC87C": {
      "id": "19321A03841341379BDE7CAF69FDC87C",
      "asnIdentifier": null,
      "position": 5019,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.RE.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret intent and meaning in artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "1B9D672F317542329194E909A8E59285"
    },
    "2566F819C2D6450F84910460EE8EEF4A": {
      "id": "2566F819C2D6450F84910460EE8EEF4A",
      "asnIdentifier": null,
      "position": 5018,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.RE.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate audience skills of observing attentively, responding appropriately, \nidentifying artistic choices made, and sharing personal responses in classroom dramatizations and \ndevelopmentally appropriate live or recorded theatrical performances.\n",
      "comments": [],
      "ancestorIds": [
        "62BFF85578F348D4BB5DD3E29F1AAD59",
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "62BFF85578F348D4BB5DD3E29F1AAD59"
    },
    "62BFF85578F348D4BB5DD3E29F1AAD59": {
      "id": "62BFF85578F348D4BB5DD3E29F1AAD59",
      "asnIdentifier": null,
      "position": 5017,
      "depth": 1,
      "listId": "1",
      "statementNotation": "DT.RE.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPerceive and analyze artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "1B9D672F317542329194E909A8E59285"
      ],
      "parentId": "1B9D672F317542329194E909A8E59285"
