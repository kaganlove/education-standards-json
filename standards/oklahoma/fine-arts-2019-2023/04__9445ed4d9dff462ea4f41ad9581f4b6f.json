    },
    "0BCDE5BB0BD54767ACB7F7BA8D3934B8": {
      "id": "0BCDE5BB0BD54767ACB7F7BA8D3934B8",
      "asnIdentifier": null,
      "position": 8031,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.CN.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nAdapt grade-appropriate stories, texts, pictures, real people or fictional characters \nand research Oklahoma  history and culture as sources for  guided drama, devised drama, and \ntheatrical works.\n",
      "comments": [],
      "ancestorIds": [
        "10FEA4BE79B14175974E7A9C56A7BC56",
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "10FEA4BE79B14175974E7A9C56A7BC56"
    },
    "10FEA4BE79B14175974E7A9C56A7BC56": {
      "id": "10FEA4BE79B14175974E7A9C56A7BC56",
      "asnIdentifier": null,
      "position": 8030,
      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.CN.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nResearch  and relate artistic ideas/works and societal, cultural, and historical context to deepen \nunderstanding.\n",
      "comments": [],
      "ancestorIds": [
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "575FC3B870294EB0898C778695D9C9A1"
    },
    "5A794D178E9F46CB8DF503B54CAF3EEE": {
      "id": "5A794D178E9F46CB8DF503B54CAF3EEE",
      "asnIdentifier": null,
      "position": 8029,
      "depth": 2,
      "listId": "2",
      "statementNotation": "4.DT.CN.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop connections\nbetween the history and culture of others to Oklahoma  history and culture, including the stories \nand storytelling contributions of the thirty-nine present day tribes that call Oklahoma  home.\n",
      "comments": [],
      "ancestorIds": [
        "45DAE66166B34D7388426A1E52C50840",
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "45DAE66166B34D7388426A1E52C50840"
    },
    "034D1DD6AD6C4016A19B5A4BE854B5C8": {
      "id": "034D1DD6AD6C4016A19B5A4BE854B5C8",
      "asnIdentifier": null,
      "position": 8028,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.CN.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nCreate drama/theatre works by connecting artistic ideas to personal, societal, \ncultural, or historical contexts.\n",
      "comments": [],
      "ancestorIds": [
        "45DAE66166B34D7388426A1E52C50840",
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "45DAE66166B34D7388426A1E52C50840"
    },
    "45DAE66166B34D7388426A1E52C50840": {
      "id": "45DAE66166B34D7388426A1E52C50840",
      "asnIdentifier": null,
      "position": 8027,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.CN.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nRelate artistic ideas and works with societal, cultural, and historical context to deepen \nunderstanding.\n",
      "comments": [],
      "ancestorIds": [
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "575FC3B870294EB0898C778695D9C9A1"
    },
    "EA16F27385FF4BC9A2C7F9F3073F1525": {
      "id": "EA16F27385FF4BC9A2C7F9F3073F1525",
      "asnIdentifier": null,
      "position": 7027,
      "depth": 2,
      "listId": "1",
      "statementNotation": "4.DT.CN.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify similarities between story elements and personal experiences, peer \nexperiences, and culture comparisons (i.e., Oklahoman culture compared with other cultures) in \ndramatic play or guided drama experiences, and/or in developmentally appropriate theatrical\nperformances viewed.\n",
      "comments": [],
      "ancestorIds": [
        "D340040C45FE49CABB4E84C14C496D5C",
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "D340040C45FE49CABB4E84C14C496D5C"
    },
    "D340040C45FE49CABB4E84C14C496D5C": {
      "id": "D340040C45FE49CABB4E84C14C496D5C",
      "asnIdentifier": null,
      "position": 7026,
      "depth": 1,
      "listId": "1",
      "statementNotation": "DT.CN.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Synthesize  and relate knowledge and personal experiences to art.\n",
      "comments": [],
      "ancestorIds": [
        "575FC3B870294EB0898C778695D9C9A1"
      ],
      "parentId": "575FC3B870294EB0898C778695D9C9A1"
    },
