    "0230CA4D03A64100A2733ACD96E4A6CD": {
      "id": "0230CA4D03A64100A2733ACD96E4A6CD",
      "asnIdentifier": null,
      "position": 11011,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.DT.PR.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPresent a drama/theatre work for a specific audience that employs research and \nanalysis grounded in the creative perspectives of the playwright, director, designer, and dramaturgy.\n",
      "comments": [],
      "ancestorIds": [
        "E7619AAF21644C2DB86DAEE83A51AFA7",
        "5FDBE5B3CC904C20B1C5444D2AE184D8"
