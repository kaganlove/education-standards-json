    "185BFF6B76864AD7A377F9565A044D64": {
      "id": "185BFF6B76864AD7A377F9565A044D64",
      "asnIdentifier": null,
      "position": 6007,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.CR.3.4 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret drama using scripted material by independently writing a one-act play with \ncorrect form and structure that includes full character development, believable dialogue, and logical    plot outcomes.\n",
      "comments": [],
      "ancestorIds": [
        "5A86D76B72A54D62B7CDFD82745EBDD7",
        "B4ADDD3BBB2A445093B16CB80ED07587"
