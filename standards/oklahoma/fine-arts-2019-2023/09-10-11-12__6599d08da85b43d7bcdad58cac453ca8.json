      "parentId": "5A86D76B72A54D62B7CDFD82745EBDD7"
    },
    "E85CBAD7315045EAB045D99D12EA0C10": {
      "id": "E85CBAD7315045EAB045D99D12EA0C10",
      "asnIdentifier": null,
      "position": 6006,
      "depth": 2,
      "listId": "3",
      "statementNotation": "III.CR.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integrate, refine, and apply\ntechnical and design elements to enhance the unified concept and create an emotional impact or \nconvey meaning in a devised or scripted drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "5A86D76B72A54D62B7CDFD82745EBDD7",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "5A86D76B72A54D62B7CDFD82745EBDD7"
    },
    "80BB8F1D6D95401FBAB3FE10F3D42D33": {
