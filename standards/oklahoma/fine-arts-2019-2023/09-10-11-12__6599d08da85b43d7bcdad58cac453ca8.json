      "asnIdentifier": null,
      "position": 15018,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.DT.RE.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nSynthesize evidence and appropriate criteria to revise personal work and interpret \nthe work of others when participating in or observing a drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "C5756EF689EB4248B3168318005E21C9",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "C5756EF689EB4248B3168318005E21C9"
    },
