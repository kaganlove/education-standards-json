      "id": "5D45C58AC0D74F3B9ABEAB924E272621",
      "asnIdentifier": null,
      "position": 12012,
      "depth": 1,
      "listId": "1",
      "statementNotation": "DT.RE.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Perceive and analyze artistic work.\n",
