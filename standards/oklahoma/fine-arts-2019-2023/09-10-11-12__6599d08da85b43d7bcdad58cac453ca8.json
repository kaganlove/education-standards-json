    "D32F14402DDD4FBA9B57CB4C53D593F7": {
      "id": "D32F14402DDD4FBA9B57CB4C53D593F7",
      "asnIdentifier": null,
      "position": 16020,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.DT.RE.3.4 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop respect by creating a personal code of theatre etiquette and ethics, \ndemonstrating awareness of professional ethics as a cast or crew member, and respecting theatre \npractitioners and conventions such as copyright law.\n",
      "comments": [],
      "ancestorIds": [
        "C5756EF689EB4248B3168318005E21C9",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "C5756EF689EB4248B3168318005E21C9"
    },
    "746CE3F497F742878E96138ADE5A7D1B": {
      "id": "746CE3F497F742878E96138ADE5A7D1B",
      "asnIdentifier": null,
      "position": 16019,
      "depth": 2,
      "listId": "3",
      "statementNotation": "III.DT.RE.3.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Establish an active relationship between audience and performer by \nevaluating/synthesizing evidence from audience responses to revise personal work and interpret the \nwork of others.\n",
      "comments": [],
      "ancestorIds": [
        "C5756EF689EB4248B3168318005E21C9",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "C5756EF689EB4248B3168318005E21C9"
    },
    "5D9749BB34544D9C9560CA3199D899E6": {
      "id": "5D9749BB34544D9C9560CA3199D899E6",
      "asnIdentifier": null,
      "position": 16018,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.DT.RE.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Analyze and evaluate varied aesthetic interpretations of technical and production \nelements for the same drama/theatre  work.\n",
      "comments": [],
      "ancestorIds": [
        "C5756EF689EB4248B3168318005E21C9",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "C5756EF689EB4248B3168318005E21C9"
    },
    "5D1DB9A79C304F738F8DE173EB258505": {
