    "83A6582305FF40418EDD2E53C47AEAC0": {
      "id": "83A6582305FF40418EDD2E53C47AEAC0",
      "asnIdentifier": null,
      "position": 4004,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.CR.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate collaborative and interdisciplinary   skills by   working\n\nto develop theatrical production concepts, discover artistic solutions, and make interpretive \nchoices in a drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "FC00C6742FB3441F80486CB1B1671657",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "FC00C6742FB3441F80486CB1B1671657"
    },
    "8FE4E0A64CF043F79EA323EDFEA72DF4": {
      "id": "8FE4E0A64CF043F79EA323EDFEA72DF4",
      "asnIdentifier": null,
      "position": 3004,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.CR.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Evolve a unified drama/theatre work by generating ideas from research and script \nanalysis and  applying meaningful cultural, literary, and historical influences to acting choices, \ntechnical choices, or directorial concepts.\n",
      "comments": [],
      "ancestorIds": [
        "FC00C6742FB3441F80486CB1B1671657",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "FC00C6742FB3441F80486CB1B1671657"
    },
