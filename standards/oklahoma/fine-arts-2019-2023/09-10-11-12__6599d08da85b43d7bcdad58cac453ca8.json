    "2B769D2FB7AB48659949BE1B2F50B2D7": {
      "id": "2B769D2FB7AB48659949BE1B2F50B2D7",
      "asnIdentifier": null,
      "position": 2003,
      "depth": 2,
      "listId": "3",
      "statementNotation": "III.CR.1.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a character authentic to the work by: applying fundamental vocal techniques such \nas knowledge of dialects and accents, the International Phonetic Alphabet, increased range and \ncontrol, intonation, and connotation; applying fundamental movement techniques and show increased \npoise and flexibility; and connecting and defending internal and external work to fully realize the \ncharacter.\n",
      "comments": [],
      "ancestorIds": [
        "99D5D0C4BC4E4D9DA43DFB08602E679B",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "99D5D0C4BC4E4D9DA43DFB08602E679B"
    },
