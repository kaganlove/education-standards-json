      "asnIdentifier": null,
      "position": 5006,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explore the connection to a character through mind/body/voice to create a vivid \ncharacter and apply appropriate acting techniques and styles in performances of characters from a \nvariety of dramatic genres and historical    periods.\n",
      "comments": [],
      "ancestorIds": [
        "5A86D76B72A54D62B7CDFD82745EBDD7",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "5A86D76B72A54D62B7CDFD82745EBDD7"
    },
