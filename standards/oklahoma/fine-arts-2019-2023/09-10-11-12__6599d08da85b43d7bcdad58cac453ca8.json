      "depth": 2,
      "listId": "2",
      "statementNotation": "III.CR.1.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Imagine technical elements for a unified drama/theatre concept  by developing and \nimplementing a major design element (e.g., sets, costumes, sound, etc.) for  a production.\n",
      "comments": [],
      "ancestorIds": [
        "99D5D0C4BC4E4D9DA43DFB08602E679B",
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "99D5D0C4BC4E4D9DA43DFB08602E679B"
    },
