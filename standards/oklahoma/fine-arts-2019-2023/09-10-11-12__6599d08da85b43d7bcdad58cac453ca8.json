      "asnIdentifier": null,
      "position": 3003,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.CR.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nOrganize, develop, and rehearse artistic ideas and work.\n",
      "comments": [],
      "ancestorIds": [
        "B4ADDD3BBB2A445093B16CB80ED07587"
      ],
      "parentId": "B4ADDD3BBB2A445093B16CB80ED07587"
