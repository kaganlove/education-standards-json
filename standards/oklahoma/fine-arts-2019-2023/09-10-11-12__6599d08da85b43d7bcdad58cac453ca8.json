      "asnIdentifier": null,
      "position": 13016,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.RE.2.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret how culture influences a performance  by demonstrating and articulating \nunderstandings of culture contexts to interpret a drama/theatre  work.\n",
      "comments": [],
      "ancestorIds": [
        "8BD843DF1E904A04B958A1F3633AB6F1",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "8BD843DF1E904A04B958A1F3633AB6F1"
    },
    "1C57199FE08A4AF993CB6F68F2AB0AC2": {
      "id": "1C57199FE08A4AF993CB6F68F2AB0AC2",
      "asnIdentifier": null,
      "position": 13015,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.RE.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use personal experience and background  knowledge to create or interpret a \ndrama/theatre  work by analyzing artistic choices developed from personal experience, textual \nevidence, and appropriate criteria to reinforce artistic choices when participating in or observing \na drama/theatre work.\n",
      "comments": [],
      "ancestorIds": [
        "8BD843DF1E904A04B958A1F3633AB6F1",
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "8BD843DF1E904A04B958A1F3633AB6F1"
    },
    "8BD843DF1E904A04B958A1F3633AB6F1": {
      "id": "8BD843DF1E904A04B958A1F3633AB6F1",
      "asnIdentifier": null,
      "position": 13014,
      "depth": 1,
      "listId": "2",
      "statementNotation": "DT.RE.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret intent and meaning in artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "32F3F88ED13A4BD7B34983B682FF1ED6"
    },
