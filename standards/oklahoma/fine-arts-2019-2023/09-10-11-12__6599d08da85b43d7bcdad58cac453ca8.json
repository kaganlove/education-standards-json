      "depth": 1,
      "listId": "3",
      "statementNotation": "DT.RE.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nApply criteria  to evaluate artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "32F3F88ED13A4BD7B34983B682FF1ED6"
      ],
      "parentId": "32F3F88ED13A4BD7B34983B682FF1ED6"
