      "asnIdentifier": null,
      "position": 4015,
      "depth": 1,
      "listId": "2",
      "statementNotation": "D.CR.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Organize and develop artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "7E95F093D4E947FE833EF081D49CA7EB"
      ],
      "parentId": "7E95F093D4E947FE833EF081D49CA7EB"
