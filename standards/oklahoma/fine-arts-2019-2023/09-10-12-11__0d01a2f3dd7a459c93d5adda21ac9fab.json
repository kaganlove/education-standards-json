      "asnIdentifier": null,
      "position": 5025,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.D.RE.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Analyze and interpret how the elements of dance, execution of dance movement \nprinciples, and context contribute to artistic expression across different genres, styles, or \ncultural movement practices. Use genre-specific dance terminology.\n",
      "comments": [],
      "ancestorIds": [
        "E3285FB1AEEC450CAF6A99BF9EEC3B92",
        "05BF3D4BB4BB489392EAE40CDF09E1A5"
      ],
      "parentId": "E3285FB1AEEC450CAF6A99BF9EEC3B92"
    },
