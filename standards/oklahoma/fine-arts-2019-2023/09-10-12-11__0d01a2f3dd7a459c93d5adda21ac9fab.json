      "depth": 1,
      "listId": "3",
      "statementNotation": "D.RE.3 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Apply criteria to evaluate artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "05BF3D4BB4BB489392EAE40CDF09E1A5"
      ],
      "parentId": "05BF3D4BB4BB489392EAE40CDF09E1A5"
    },
    "19BEDE6B6B094A5FA93B878653F57696": {
