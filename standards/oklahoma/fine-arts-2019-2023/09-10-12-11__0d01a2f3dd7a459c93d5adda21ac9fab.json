      "asnIdentifier": null,
      "position": 5030,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.CN.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nReview original choreography with respect to its content and context. Reflect and \nanalyze its relationship to personal perspectives and growth.\n",
      "comments": [],
      "ancestorIds": [
        "426A18B935EB4D58A62AA2ED84A5EA49",
        "C037A2A19C7E402A86D84EBDC82DE630"
