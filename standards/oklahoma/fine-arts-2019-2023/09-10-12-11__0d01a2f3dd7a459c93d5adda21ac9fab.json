    "823A31826BE14843A8514EA7980F9C84": {
      "id": "823A31826BE14843A8514EA7980F9C84",
      "asnIdentifier": null,
      "position": 4010,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.D.PR.3.4 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Work collaboratively to produce dance concerts in a variety of venues and design and \norganize the production elements that would be necessary to  fulfill the artistic intent of the \ndance works in each of the venues.\n",
      "comments": [],
      "ancestorIds": [
        "552B4F9FED0C4EC792BCA44579EB70B4",
        "38C961F602774B70BC8AF6A97590771F"
