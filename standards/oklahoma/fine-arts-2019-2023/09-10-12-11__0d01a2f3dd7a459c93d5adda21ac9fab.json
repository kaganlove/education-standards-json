      "asnIdentifier": null,
      "position": 5024,
      "depth": 1,
      "listId": "2",
      "statementNotation": "D.RE.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Construct meaningful interpretations of artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "05BF3D4BB4BB489392EAE40CDF09E1A5"
      ],
      "parentId": "05BF3D4BB4BB489392EAE40CDF09E1A5"
    },
    "7546B2A2F5634FC6B6B3EEA3ACA972A8": {
