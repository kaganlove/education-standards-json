      "asnIdentifier": null,
      "position": 5027,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.D.RE.3.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": " Define personal artistic preferences to critique a dance.\nConsider societal and personal values and a range of artistic expression.\nDiscuss perspectives with peers and\njustify views.                                                      \n",
      "comments": [],
      "ancestorIds": [
        "8389F63068334491B4366B6272A642FB",
        "05BF3D4BB4BB489392EAE40CDF09E1A5"
