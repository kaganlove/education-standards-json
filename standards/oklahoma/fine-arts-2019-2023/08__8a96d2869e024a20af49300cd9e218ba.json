    "189C2D61F4AB4866992D47E548CD178F": {
      "id": "189C2D61F4AB4866992D47E548CD178F",
      "asnIdentifier": null,
      "position": 5018,
      "depth": 2,
      "listId": "2",
      "statementNotation": "8.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nExperiment  with aspects of a recognized system to  document a section of a dance by using \nwords, symbols, or media technologies.\n",
      "comments": [],
      "ancestorIds": [
        "5B2FCA55005244BBB8F638C3630E51DB",
        "47C0CD9B0A554FE9875BD44E518BF7FB"
