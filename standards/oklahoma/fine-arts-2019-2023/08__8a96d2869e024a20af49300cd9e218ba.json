      "asnIdentifier": null,
      "position": 4004,
      "depth": 2,
      "listId": "1",
      "statementNotation": "8.D.PR.2.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nDesign body shapes in relation to other dancers, objects, and/or environment. Use \nappropriate performance focus in direct and indirect pathways.\n",
      "comments": [],
      "ancestorIds": [
        "019BD9B7033E43A1935A227C911E6BA3",
        "B72E20C511EC4C11A3CB2AC9C5D4A79D"
