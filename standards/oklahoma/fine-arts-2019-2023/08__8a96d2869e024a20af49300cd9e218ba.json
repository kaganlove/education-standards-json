    "DF64730A160041E78CEA3EEC04E7428A": {
      "id": "DF64730A160041E78CEA3EEC04E7428A",
      "asnIdentifier": null,
      "position": 5008,
      "depth": 2,
      "listId": "2",
      "statementNotation": "8.D.PR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Collaborate to design and execute production elements that would intensify and heighten \nthe artistic intent of a dance performed on a stage, in a different venue, or for different \naudiences. Use production terminology to explain reasons for choices.\n",
      "comments": [],
      "ancestorIds": [
        "B8D96E6342ED43B8A0F6B4D37C95553C",
        "B72E20C511EC4C11A3CB2AC9C5D4A79D"
