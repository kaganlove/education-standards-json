      "id": "BABAC3B0DBA3464AB3044024BC68C120",
      "asnIdentifier": null,
      "position": 5020,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.RE.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nPerceive and analyze artistic work.\n",
      "comments": [],
      "ancestorIds": [
        "48A6F531F1D04360BC555172F19F0337"
      ],
      "parentId": "48A6F531F1D04360BC555172F19F0337"
