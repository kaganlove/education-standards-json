    "2F1409AE9C58455690727473E22F7118": {
      "id": "2F1409AE9C58455690727473E22F7118",
      "asnIdentifier": null,
      "position": 5021,
      "depth": 2,
      "listId": "1",
      "statementNotation": "8.D.RE.1.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe and discuss patterns of movement and their relationships in dance in context of \nartistic intent.\n",
      "comments": [],
      "ancestorIds": [
        "BABAC3B0DBA3464AB3044024BC68C120",
        "48A6F531F1D04360BC555172F19F0337"
      ],
      "parentId": "BABAC3B0DBA3464AB3044024BC68C120"
    },
