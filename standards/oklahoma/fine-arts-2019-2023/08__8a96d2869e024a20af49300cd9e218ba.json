      "id": "C9618632E8D6420BA2DA3E7860E47BBC",
      "asnIdentifier": null,
      "position": 5010,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.CR.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nGenerate and conceptualize artistic ideas and work.\n",
      "comments": [],
      "ancestorIds": [
        "47C0CD9B0A554FE9875BD44E518BF7FB"
      ],
      "parentId": "47C0CD9B0A554FE9875BD44E518BF7FB"
    },
