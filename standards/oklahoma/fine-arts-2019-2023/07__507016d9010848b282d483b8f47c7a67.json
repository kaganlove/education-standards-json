      "asnIdentifier": null,
      "position": 4019,
      "depth": 2,
      "listId": "2",
      "statementNotation": "7.CR.3.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\n\nInvestigate a recognized system to  document a dance sequence by using words, symbols, or \nmedia technologies.\n",
      "comments": [],
      "ancestorIds": [
        "464723B20D2D4435BA8D0A3581F91191",
        "20D734C111D0465C98FBAFAF352D7C84"
