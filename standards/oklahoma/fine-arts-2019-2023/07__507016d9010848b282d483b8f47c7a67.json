      "id": "916556F489EB4E3192B602273360F4A7",
      "asnIdentifier": null,
      "position": 2005,
      "depth": 1,
      "listId": "2",
      "statementNotation": "D.PR.2 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nSelect, analyze, and interpret artistic work for presentation.\n",
      "comments": [],
      "ancestorIds": [
        "A0E18E562B8E4B8BA33534382ED67B86"
      ],
      "parentId": "A0E18E562B8E4B8BA33534382ED67B86"
    },
