      "id": "B584B5DB387B459D90403D629AF5689C",
      "asnIdentifier": null,
      "position": 3012,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.CR.1 ",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\nGenerate and conceptualize artistic ideas and work.\n",
      "comments": [],
      "ancestorIds": [
        "20D734C111D0465C98FBAFAF352D7C84"
      ],
      "parentId": "20D734C111D0465C98FBAFAF352D7C84"
    },
