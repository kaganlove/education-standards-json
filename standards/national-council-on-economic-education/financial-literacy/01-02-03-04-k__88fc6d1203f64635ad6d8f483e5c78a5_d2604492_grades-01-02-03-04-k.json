    "6771E5B6701C467AA71418744328B64B": {
      "asnIdentifier": "S2604573",
      "position": 30000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "4.",
      "description": "Borrowers who repay loans as promised show that they are worthy of getting credit in the future. A reputation for not repaying a loan as promised can result in higher interest charges on future loans, if loans are available at all.",
      "id": "6771E5B6701C467AA71418744328B64B",
      "ancestorIds": [
        "8731F4ADC94A43D1A86682542E960E90"
      ],
      "parentId": "8731F4ADC94A43D1A86682542E960E90"
    },
    "7533CE8B8E5C4126AD4151C0BA6C7B08": {
      "id": "7533CE8B8E5C4126AD4151C0BA6C7B08",
      "asnIdentifier": "S2604572",
      "position": 29000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "3.",
      "description": "By using credit to buy durable goods—such as cars, houses, and appliances—people are able to use the goods while paying for them.",
      "ancestorIds": [
        "8731F4ADC94A43D1A86682542E960E90"
      ],
      "parentId": "8731F4ADC94A43D1A86682542E960E90"
    },
    "525C5A6868924A25A6553DF831D2EFF7": {
      "id": "525C5A6868924A25A6553DF831D2EFF7",
      "asnIdentifier": "S2604571",
      "position": 28000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "2.",
      "description": "When people use credit, they receive something of value now and agree to repay the lender over time, or at some date in the future, with interest.",
      "ancestorIds": [
        "8731F4ADC94A43D1A86682542E960E90"
      ],
      "parentId": "8731F4ADC94A43D1A86682542E960E90"
    },
    "A8771D390F564D28A5D745B8F4C30C93": {
      "description": "Interest is the price the borrower pays for using someone else's money.",
      "id": "A8771D390F564D28A5D745B8F4C30C93",
      "asnIdentifier": "S2604570",
      "position": 27000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "1.",
      "ancestorIds": [
        "8731F4ADC94A43D1A86682542E960E90"
      ],
      "parentId": "8731F4ADC94A43D1A86682542E960E90"
    },
    "8731F4ADC94A43D1A86682542E960E90": {
      "asnIdentifier": "S2604569",
      "position": 26000,
      "depth": 0,
      "statementLabel": "Standard",
      "listId": "IV.",
      "description": "Using Credit",
      "id": "8731F4ADC94A43D1A86682542E960E90",
      "ancestorIds": [],
      "parentId": null
    },
    "C16289F7191944F7803A1DA23AF90C2A": {
      "description": "When people deposit money into a bank (or other financial institution), the bank may pay them interest. Banks attract savings by paying interest. People also deposit money into banks because banks are safe places to keep their savings.",
      "id": "C16289F7191944F7803A1DA23AF90C2A",
      "asnIdentifier": "S2604551",
      "position": 25000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "6.",
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "2414FD0E42694515B9F16AE618921DD0": {
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "5.",
      "description": "A savings plan helps people reach their savings goals.",
      "id": "2414FD0E42694515B9F16AE618921DD0",
      "asnIdentifier": "S2604550",
      "position": 24000,
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "F0BE3260131A45568265AC327E0E57FF": {
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "4.",
      "description": "People set savings goals as incentives to save. One savings goal might be to buy goods and services in the future.",
      "id": "F0BE3260131A45568265AC327E0E57FF",
      "asnIdentifier": "S2604549",
      "position": 23000,
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "3C8282A196E9452787C7922826810B8A": {
      "description": "People can choose to save money in many places—for example, at home in a piggy bank or at a commercial bank, credit union, or savings and loan.",
      "id": "3C8282A196E9452787C7922826810B8A",
      "asnIdentifier": "S2604548",
      "position": 22000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "3.",
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "6588E44ED1FD4353AE7657316B520758": {
      "statementLabel": "Benchmark",
      "listId": "2.",
      "description": "When people save money, they give up the opportunity to spend that money to buy things now in order to buy things later.",
      "id": "6588E44ED1FD4353AE7657316B520758",
      "asnIdentifier": "S2604547",
      "position": 21000,
      "depth": 1,
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "E7AC7BEA85C34833B13334531ACF0F52": {
      "description": "Income is saved, spent on goods and services, or used to pay taxes.",
      "id": "E7AC7BEA85C34833B13334531ACF0F52",
      "asnIdentifier": "S2604546",
      "position": 20000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "1.",
      "ancestorIds": [
        "5BD4ABCB363E4EDE939E6FFF4A381FB5"
      ],
      "parentId": "5BD4ABCB363E4EDE939E6FFF4A381FB5"
    },
    "5BD4ABCB363E4EDE939E6FFF4A381FB5": {
      "id": "5BD4ABCB363E4EDE939E6FFF4A381FB5",
