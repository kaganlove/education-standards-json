    "5100049E876E4B79AE3A8DCEBC3D079F": {
      "id": "5100049E876E4B79AE3A8DCEBC3D079F",
      "asnIdentifier": "S2604531",
      "position": 18000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "7.",
      "description": "Planning for spending can help people make informed choices. A budget is a plan for spending, saving, and managing income.",
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "C6D39D08D0E244D9921D65ECC6E704E7": {
      "description": "People's spending choices are influenced by prices as well as many other factors, including advertising, the spending choices of others, and peer pressure.",
      "id": "C6D39D08D0E244D9921D65ECC6E704E7",
      "asnIdentifier": "S2604530",
      "position": 17000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "6.",
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "4EF139B421CC49AB8AD7E118370F5028": {
      "statementLabel": "Benchmark",
      "listId": "5.",
      "description": "Informed decision making requires comparing the costs and benefits of spending alternatives. Costs are things that a decision maker gives up; benefits are things that a decision maker gains.",
      "id": "4EF139B421CC49AB8AD7E118370F5028",
      "asnIdentifier": "S2604529",
      "position": 16000,
      "depth": 1,
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "95BC8ED1066B4382B643840B8D2257C3": {
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "4.",
      "description": "Whenever people buy something, they incur an opportunity cost. Opportunity cost is the value of the next best alternative that is given up when a person makes a choice.",
      "id": "95BC8ED1066B4382B643840B8D2257C3",
      "asnIdentifier": "S2604528",
      "position": 15000,
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "48438BD581D34995A06906739CB2783B": {
      "id": "48438BD581D34995A06906739CB2783B",
      "asnIdentifier": "S2604527",
      "position": 14000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "3.",
      "description": "People spend a portion of their income on goods and services in order to increase their personal satisfaction or happiness.",
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "1F2D13689BB14A60985591AFC8626C8C": {
      "position": 13000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "2.",
      "description": "People make choices about what goods and services they buy because they can't have everything they want. This requires individuals to prioritize their wants.",
      "id": "1F2D13689BB14A60985591AFC8626C8C",
      "asnIdentifier": "S2604526",
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
    "EDF305017DEE492FA2F3CCB6C9A2656A": {
      "position": 12000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "1.",
      "description": "Economic wants are desires that can be satisfied by consuming a good, a service, or a leisure activity.",
      "id": "EDF305017DEE492FA2F3CCB6C9A2656A",
      "asnIdentifier": "S2604525",
      "ancestorIds": [
        "D11563AA7BFE470786C6933F166FCA0C"
      ],
      "parentId": "D11563AA7BFE470786C6933F166FCA0C"
    },
