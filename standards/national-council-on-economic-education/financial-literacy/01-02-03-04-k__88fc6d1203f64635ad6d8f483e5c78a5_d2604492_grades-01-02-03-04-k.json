      "asnIdentifier": "S2604503",
      "position": 9000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "8.",
      "description": "Income can be received from family or friends as money gifts or as an allowance for which no specified work may be required.",
      "ancestorIds": [
        "B33B4DCCD765497B9C6AE78DB6889FB3"
      ],
      "parentId": "B33B4DCCD765497B9C6AE78DB6889FB3"
    },
