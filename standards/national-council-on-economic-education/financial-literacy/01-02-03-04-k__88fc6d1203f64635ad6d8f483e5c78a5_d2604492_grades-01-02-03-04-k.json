      "asnIdentifier": "S2604502",
      "position": 8000,
      "depth": 1,
      "statementLabel": "Benchmark",
      "listId": "7.",
      "description": "Entrepreneurs are people who start new businesses. Starting a business is risky for entrepreneurs because they do not know if their new businesses will be successful and earn a profit.",
      "ancestorIds": [
        "B33B4DCCD765497B9C6AE78DB6889FB3"
      ],
      "parentId": "B33B4DCCD765497B9C6AE78DB6889FB3"
    },
