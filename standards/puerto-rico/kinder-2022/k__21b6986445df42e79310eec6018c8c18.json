      "ancestorIds": [
        "96D763F27830485FBDEACFC9D69F3143",
        "CF1F4926CF7149B6B028FAC89031F12F"
      ],
      "parentId": "96D763F27830485FBDEACFC9D69F3143"
    },
    "96D763F27830485FBDEACFC9D69F3143": {
      "id": "96D763F27830485FBDEACFC9D69F3143",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 1,
      "listId": "5",
      "statementNotation": "K.LLI.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Analiza cómo estructuras y patrones específicos del texto dan secuencia a las ideas o afectan el tono, mientras considera las\nrelaciones entre las partes del texto.",
      "comments": [],
      "ancestorIds": [
        "CF1F4926CF7149B6B028FAC89031F12F"
      ],
      "parentId": "CF1F4926CF7149B6B028FAC89031F12F"
    },
    "2EDAF8BE8FD84DF09A82CD8675E69246": {
