      "position": 1030,
      "depth": 1,
      "listId": "2",
      "statementNotation": "K.E.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Utiliza efectivamente una estructura de organización y múltiples fuentes de referencia para escribir textos informativos y\nexplicativos con el fin de comunicarse y establecer relaciones entre conceptos.",
      "comments": [],
      "ancestorIds": [
        "2464FEE52B324448AC8FA83940272E59"
      ],
      "parentId": "2464FEE52B324448AC8FA83940272E59"
    },
