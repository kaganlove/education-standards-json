      "depth": 1,
      "listId": "1",
      "statementNotation": "K.AO.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Participa en grupos variados en los que aporta y colabora de diferentes formas, intercambia ideas, desarrolla nueva\ncomprensión y resuelve problemas hacia una meta en común.",
      "comments": [],
      "ancestorIds": [
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "29AF889411FB40B6BE3FC7D73D91443A"
    },
