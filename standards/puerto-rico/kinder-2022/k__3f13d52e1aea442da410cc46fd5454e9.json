      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "6",
      "statementNotation": "K.AO.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Utiliza efectivamente los medios, los recursos visuales y las destrezas tecnológicas para crear y mejorar las\npresentaciones. ",
      "comments": [],
      "ancestorIds": [
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "29AF889411FB40B6BE3FC7D73D91443A"
    },
    "8C47A22A995848248C04819ABED65784": {
