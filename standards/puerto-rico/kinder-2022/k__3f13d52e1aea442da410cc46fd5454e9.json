      "statementNotation": "K.AO.1.1b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Participa en grupos pequeños o grandes para buscar información y dar su opinión\nsobre el tema. ",
      "comments": [],
      "ancestorIds": [
        "E233E7902AD04B308E6DF00AC095DD24",
        "7B7A4F99A3124CE8A45B105D18F18970",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "E233E7902AD04B308E6DF00AC095DD24"
    },
