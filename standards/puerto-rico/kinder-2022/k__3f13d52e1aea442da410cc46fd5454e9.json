      "id": "2CFC046DD832426EA469B8F58C900466",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "5",
      "statementNotation": "K.AO.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Presenta información, hallazgos y evidencia para que el receptor pueda seguir la línea de razonamiento, la\norganización, el desarrollo y el estilo apropiado para la tarea, el propósito y la audiencia. ",
      "comments": [],
      "ancestorIds": [
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "29AF889411FB40B6BE3FC7D73D91443A"
    },
