      "ancestorIds": [
        "67F191C8BE5140E8980A23030FF0546D",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "67F191C8BE5140E8980A23030FF0546D"
    },
    "67F191C8BE5140E8980A23030FF0546D": {
      "id": "67F191C8BE5140E8980A23030FF0546D",
      "asnIdentifier": null,
      "position": 1010,
      "depth": 1,
      "listId": "7",
      "statementNotation": "K.AO.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adapta el lenguaje a diferentes contextos, realiza tareas que fomentan la comunicación oral y utiliza medios\ninformativos para demostrar dominio del español formal e informal. ",
      "comments": [],
      "ancestorIds": [
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "29AF889411FB40B6BE3FC7D73D91443A"
    },
