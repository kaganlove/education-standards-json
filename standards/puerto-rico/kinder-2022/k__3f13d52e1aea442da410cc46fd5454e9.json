      "asnIdentifier": null,
      "position": 1005,
      "depth": 2,
      "listId": "1",
      "statementNotation": "K.AO.2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Confirma la comprensión de los textos leídos en voz alta o de información presentada oralmente\nmediante diálogo, preguntas y respuestas. ",
      "comments": [],
      "ancestorIds": [
        "BC07A7D0D3274B3F8868C383E2A74FEB",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "BC07A7D0D3274B3F8868C383E2A74FEB"
    },
    "BC07A7D0D3274B3F8868C383E2A74FEB": {
      "id": "BC07A7D0D3274B3F8868C383E2A74FEB",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "2",
      "statementNotation": "K.AO.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integra y evalúa la información presentada en diversos medios y formatos; utiliza gráficas y recursos tecnológicos\npara enriquecer la presentación",
      "comments": [],
      "ancestorIds": [
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "29AF889411FB40B6BE3FC7D73D91443A"
    },
