      "asnIdentifier": null,
      "position": 1007,
      "depth": 2,
      "listId": "1",
      "statementNotation": "K.AO.5.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Con apoyo del maestro, utiliza palabras, oraciones simples y detalles para describir personas,\nlugares, cosas y eventos conocidos. ",
      "comments": [],
      "ancestorIds": [
        "2CFC046DD832426EA469B8F58C900466",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "2CFC046DD832426EA469B8F58C900466"
    },
