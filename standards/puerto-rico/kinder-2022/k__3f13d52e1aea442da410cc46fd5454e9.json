      "id": "E233E7902AD04B308E6DF00AC095DD24",
      "asnIdentifier": null,
      "position": 1002,
      "depth": 2,
      "listId": "1",
      "statementNotation": "K.AO.1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Participa en conversaciones sobre temas y textos relacionados con el grado en grupos grandes y\npequeños.",
      "comments": [],
      "ancestorIds": [
        "7B7A4F99A3124CE8A45B105D18F18970",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "7B7A4F99A3124CE8A45B105D18F18970"
    },
    "7B7A4F99A3124CE8A45B105D18F18970": {
