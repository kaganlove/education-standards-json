      "id": "C9C33F53F1AD45199761D2AAF932EE5D",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 2,
      "listId": "1",
      "statementNotation": "K.AO.7.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Expresa, en voz alta y oraciones completas, pensamientos, ideas y sentimientos.",
