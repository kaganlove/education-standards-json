      "id": "F9CE82F860A94FA1BA876A048EE4E336",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 2,
      "listId": "1",
      "statementNotation": "K.AO.6.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Realiza dibujos que sirven de punto de partida para sus narraciones y su propia escritura.",
      "comments": [],
      "ancestorIds": [
        "D075925F00C545AF9ADBA2B5957AB5D7",
        "29AF889411FB40B6BE3FC7D73D91443A"
      ],
      "parentId": "D075925F00C545AF9ADBA2B5957AB5D7"
    },
