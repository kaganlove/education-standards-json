      "asnIdentifier": null,
      "position": 31000,
      "depth": 2,
      "listId": "c",
      "statementNotation": "ET2.3c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Knowledge gained from other fields of study has a direct effect on the development of technological products and systems.",
