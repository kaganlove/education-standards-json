      "asnIdentifier": null,
      "position": 33000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "ET3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "In order to recognize the changes in society caused by the use of technology, students should learn that",
      "comments": [],
      "ancestorIds": [
        "298E21C2A3B94F8E99C2BB3B2E7DC5A7"
      ],
      "parentId": "298E21C2A3B94F8E99C2BB3B2E7DC5A7"
    },
    "298E21C2A3B94F8E99C2BB3B2E7DC5A7": {
      "id": "298E21C2A3B94F8E99C2BB3B2E7DC5A7",
      "asnIdentifier": null,
      "position": 32000,
      "depth": 0,
      "listId": "3",
      "statementNotation": "ET3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will develop an understanding of the cultural, social, economic, and political effects of technology, the effects of technology on the environment, the role of society in the development and use of technology, and the influence of technology on history. ",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
