      "position": 25021,
      "depth": 1,
      "listId": "Standard 5",
      "statementNotation": "S3.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Redesign the prototype by repeating the design process in order to further optimize the design. (For example: Learn from failed attempts and identify areas for improvement from testing, Reconsider any discarded ideas, Look for mathematical relationships and use them to identify the factors that affect the design the most, Repeat the steps of the design process until the prototype meets the requirements) ",
      "comments": [],
      "ancestorIds": [
        "11D1BC6D31314956BFD655ACB4A68E51"
      ],
      "parentId": "11D1BC6D31314956BFD655ACB4A68E51"
