    "A0155CC3FA8E45188B9BA70797601B66": {
      "id": "A0155CC3FA8E45188B9BA70797601B66",
      "asnIdentifier": null,
      "position": 27025,
      "depth": 1,
      "listId": "Standard 1",
      "statementNotation": "S4.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "In order to understand the effects of engineering on society, students should learn that\nengineers have improved the quality of life by introducing revolutionary technologies such as: Clean water systems, Transportation & infrastructure, Medicines & biotechnology, Electronics, Energy.",
      "comments": [],
      "ancestorIds": [
        "29D84B0139354463BA95939F676FB1C1"
      ],
      "parentId": "29D84B0139354463BA95939F676FB1C1"
    },
    "29D84B0139354463BA95939F676FB1C1": {
      "id": "29D84B0139354463BA95939F676FB1C1",
      "asnIdentifier": null,
      "position": 26025,
      "depth": 0,
      "listId": "Strand 4",
      "statementNotation": "S4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will develop an understanding of the cultural, environmental, economic, and\npolitical effects of engineering, and the impacts of technology throughout history.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "B4E0EF265BA3460DB2AF1420E876BB24": {
