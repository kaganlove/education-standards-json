      "asnIdentifier": null,
      "position": 1019,
      "depth": 2,
      "listId": "1",
      "statementNotation": "5.SP4.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explain probable causes and effects of events and developments in United States history from the revolutionary period to the rise of industry\nand urbanization.",
      "comments": [],
      "ancestorIds": [
        "362A134B0A3E4425A02B2053C1549A31",
        "9E9DFC7CFC354D91AB1B59673827232E"
