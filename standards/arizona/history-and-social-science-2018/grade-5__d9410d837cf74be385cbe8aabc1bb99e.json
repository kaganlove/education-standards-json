      "depth": 2,
      "listId": "6",
      "statementNotation": "5.SP3.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Construct and present arguments using claims and evidence from multiple sources.",
      "comments": [],
      "ancestorIds": [
        "CD8028C755994DAEB8823CE61F830FE5",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "CD8028C755994DAEB8823CE61F830FE5"
    },
