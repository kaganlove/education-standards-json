      "position": 1013,
      "depth": 2,
      "listId": "3",
      "statementNotation": "5.SP3.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare information provided by multiple sources about events and developments in the United States.",
      "comments": [],
      "ancestorIds": [
        "CD8028C755994DAEB8823CE61F830FE5",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
