      "id": "1454BBFC0A3945AA84B98D7D2ED69FAC",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 2,
      "listId": "1",
      "statementNotation": "5.SP2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explain why individuals and groups during the same historical period differed in their perspectives ",
      "comments": [],
      "ancestorIds": [
        "D850DB6B7E9F43608886FBD67279A849",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "D850DB6B7E9F43608886FBD67279A849"
    },
    "D850DB6B7E9F43608886FBD67279A849": {
