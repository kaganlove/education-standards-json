    "E930EE7A7DD944259E3259D354C67D32": {
      "id": "E930EE7A7DD944259E3259D354C67D32",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 2,
      "listId": "1",
      "statementNotation": "5.SP3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop compelling and supporting questions about the United States that are open to different interpretations.",
      "comments": [],
      "ancestorIds": [
        "CD8028C755994DAEB8823CE61F830FE5",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "CD8028C755994DAEB8823CE61F830FE5"
