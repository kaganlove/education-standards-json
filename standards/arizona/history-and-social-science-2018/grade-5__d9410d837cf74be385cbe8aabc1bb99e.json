      "asnIdentifier": null,
      "position": 1015,
      "depth": 2,
      "listId": "5",
      "statementNotation": "5.SP3.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use information about a historical source including the author, date, place of origin, intended audience, and purpose to judge the extent to\nwhich the source is useful for studying a topic and evaluate the credibility of the source.",
      "comments": [],
      "ancestorIds": [
        "CD8028C755994DAEB8823CE61F830FE5",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "CD8028C755994DAEB8823CE61F830FE5"
    },
