      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "SP2",
      "statementNotation": "5.SP2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Thinking within the discipline involves the ability to identify, compare, and evaluate multiple perspectives about a given event to draw conclusions since\nthere are multiple points of view about events and issues.",
      "comments": [],
      "ancestorIds": [
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "9E9DFC7CFC354D91AB1B59673827232E"
    },
    "A4E1B17063FC4524AC7D9DC2E929E48D": {
