      "position": 1020,
      "depth": 3,
      "listId": "-",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Events include but are not limited to the American Revolution, Constitutional Convention, Civil War, Reconstruction, westward expansion,\nindustrialism, and urbanization",
      "comments": [],
      "ancestorIds": [
        "6D0301E6572A4EEAA151E714E7720800",
        "362A134B0A3E4425A02B2053C1549A31",
        "9E9DFC7CFC354D91AB1B59673827232E"
