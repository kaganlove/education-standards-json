      "asnIdentifier": null,
      "position": 1010,
      "depth": 1,
      "listId": "SP3",
      "statementNotation": "5.SP3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Historians and Social Scientists gather, interpret, and use evidence to develop claims and answer historical, economic, geographical, and political\nquestions and communicate their conclusions.",
      "comments": [],
      "ancestorIds": [
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "9E9DFC7CFC354D91AB1B59673827232E"
    },
    "D46083594EE2487E83355D7DE12858EB": {
      "id": "D46083594EE2487E83355D7DE12858EB",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 3,
      "listId": "-",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Key issues and events can include but are not limited to federalism, constitutional interpretation, individual liberties, slavery, Jim Crow Laws\nand segregation, secession, westward expansion, Indian boarding schools, immigration, Manifest Destiny, worker’s rights, and women’s rights",
      "comments": [],
      "ancestorIds": [
        "1454BBFC0A3945AA84B98D7D2ED69FAC",
        "D850DB6B7E9F43608886FBD67279A849",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "1454BBFC0A3945AA84B98D7D2ED69FAC"
    },
    "3ED8C3CB80D640C48739F8D71B9A8C38": {
      "id": "3ED8C3CB80D640C48739F8D71B9A8C38",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 3,
      "listId": "-",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Key individuals and groups can include but are not limited to a loyalist and patriots, federalist and anti-federalist, Hamilton and Jefferson,\nabolitionists and slave owners, Abraham Lincoln and John C. Calhoun, southerners and northerners, labor and business, nativists and\nimmigrants, and American Indians and settlers",
      "comments": [],
      "ancestorIds": [
        "1454BBFC0A3945AA84B98D7D2ED69FAC",
        "D850DB6B7E9F43608886FBD67279A849",
        "9E9DFC7CFC354D91AB1B59673827232E"
      ],
      "parentId": "1454BBFC0A3945AA84B98D7D2ED69FAC"
    },
