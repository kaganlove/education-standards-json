      "asnIdentifier": null,
      "position": 46000,
      "depth": 2,
      "listId": "1",
      "statementNotation": "III.CSD.7.01",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify ways in which Christians actively participate in society and work for the common good through communion, solidarity, stewardship and service to others, especially by practicing the\r\nCorporal & Spiritual Works of Mercy.\r\n",
      "comments": [],
      "ancestorIds": [
        "09838E89D433459EA327FB5740ECC089",
        "1586EFB7CE584BE3937CC1343960E80D"
