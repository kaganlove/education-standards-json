    "1174A8E33CC048C89623CF7DF647B347": {
      "id": "1174A8E33CC048C89623CF7DF647B347",
      "asnIdentifier": null,
      "position": 43000,
      "depth": 2,
      "listId": "5",
      "statementNotation": "III.MOR.7.05",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize that the Incarnation is foundational for understanding that each human person has an innate dignity and right to life from natural\r\nconception to natural death.\r\n",
      "comments": [],
      "ancestorIds": [
        "CE60CC86CAB94A328B00C00F06F59AD8",
        "1586EFB7CE584BE3937CC1343960E80D"
