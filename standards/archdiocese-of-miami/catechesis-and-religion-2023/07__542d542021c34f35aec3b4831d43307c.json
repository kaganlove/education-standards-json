      "asnIdentifier": null,
      "position": 47000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.CSD.7.02",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Articulate how the innate dignity of\r\nevery human person guarantees them rights and responsibilities within society.\r\n\r\n",
      "comments": [],
      "ancestorIds": [
        "09838E89D433459EA327FB5740ECC089",
        "1586EFB7CE584BE3937CC1343960E80D"
      ],
