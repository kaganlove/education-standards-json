      "asnIdentifier": null,
      "position": 42000,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.MOR.7.04",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Distinguish between the theological virtues as freely given by God in Baptism and the cardinal virtues which grow in us by human effort and habitually\r\nchoosing to do the good.\r\n",
      "comments": [],
      "ancestorIds": [
        "CE60CC86CAB94A328B00C00F06F59AD8",
        "1586EFB7CE584BE3937CC1343960E80D"
      ],
      "parentId": "CE60CC86CAB94A328B00C00F06F59AD8"
