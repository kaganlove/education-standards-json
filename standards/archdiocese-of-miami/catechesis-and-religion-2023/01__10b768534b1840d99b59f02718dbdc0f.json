      "depth": 2,
      "listId": "3",
      "statementNotation": "IV.PRA.1.03",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize and practice the following types of prayer: blessing and adoration, petition, intercession, thanksgiving, & praise.\r\n",
      "comments": [],
      "ancestorIds": [
        "78F3CA25899E4CAEA1694750556D6D7D"
      ],
      "parentId": null
    },
    "2C3E5F7E6EFD4F7C955F48D684E34A17": {
