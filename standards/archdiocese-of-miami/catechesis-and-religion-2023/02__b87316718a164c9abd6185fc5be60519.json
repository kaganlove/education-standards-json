      "asnIdentifier": null,
      "position": 41000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "III.MOR.2.02",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Know that conscience is God's voice inside you where you distinguish right from wrong and take responsibility for your actions.\r\n",
