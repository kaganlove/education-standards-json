      "asnIdentifier": null,
      "position": 43000,
      "depth": 2,
      "listId": "3",
      "statementNotation": "III.MOR.8.03",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explain why both abortion and the use of contraception are contrary to human dignity and our understanding of human sexuality.\r\n",
      "comments": [],
      "ancestorIds": [
        "E311237730BC4C7BB6751E0664A734A8",
        "18EF1D81B6994AA5BD580193C2A0039D"
      ],
      "parentId": "E311237730BC4C7BB6751E0664A734A8"
    },
