    "39DC1EC4F2E84BADA514F6280F2CD487": {
      "id": "39DC1EC4F2E84BADA514F6280F2CD487",
      "asnIdentifier": null,
      "position": 50000,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.CSD.8.04",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explain causes of vulnerability (age, race, disability, etc.) and ways that we can act morally correct in situations\r\nwhere vulnerable people are present.\r\n",
      "comments": [],
      "ancestorIds": [
        "55DC3DAA25474D7B884646707E20C6A1",
        "18EF1D81B6994AA5BD580193C2A0039D"
