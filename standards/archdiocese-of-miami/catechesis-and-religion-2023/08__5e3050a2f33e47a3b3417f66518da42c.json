      "asnIdentifier": null,
      "position": 60000,
      "depth": 2,
      "listId": "3",
      "statementNotation": "IV.PRA.8.03",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand that the simple prayer \"Come Holy Spirit\" can be said at the beginning and the end of every important action.",
