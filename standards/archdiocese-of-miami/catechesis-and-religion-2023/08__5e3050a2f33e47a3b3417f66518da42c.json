    },
    "19CEA921B9504CCEAE2B24D7CB8FF1D6": {
      "id": "19CEA921B9504CCEAE2B24D7CB8FF1D6",
      "asnIdentifier": null,
      "position": 44000,
      "depth": 2,
      "listId": "4",
      "statementNotation": "III.MOR.8.04",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explain how an inappropriate pursuit of wealth, honor, power, or pleasure leads to sin while pursuing them for the good of others under the guidance of the Holy Spirit and for the glory of God leads to holiness and virtue; give examples for\r\neach.\r\n",
      "comments": [],
      "ancestorIds": [
        "E311237730BC4C7BB6751E0664A734A8",
        "18EF1D81B6994AA5BD580193C2A0039D"
      ],
      "parentId": "E311237730BC4C7BB6751E0664A734A8"
    },
    "3F45BF16D8794EB3BBB9E7681E0224B1": {
