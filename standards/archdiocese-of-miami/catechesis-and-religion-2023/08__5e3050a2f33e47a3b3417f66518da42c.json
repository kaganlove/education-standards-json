      "position": 59000,
      "depth": 2,
      "listId": "2",
      "statementNotation": "IV.PRA.8.02",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Examine our relationship with God through personal prayer and identify how that relationship leads to greater openness to hear God’s call and\r\ndirection in our lives.\r\n",
      "comments": [],
      "ancestorIds": [
        "45349B89898B4EC9AF6BADB49B187865"
      ],
      "parentId": null
    },
