    "357B5085A48045668333219AC5F82E1C": {
      "id": "357B5085A48045668333219AC5F82E1C",
      "asnIdentifier": null,
      "position": 45000,
      "depth": 2,
      "listId": "5",
      "statementNotation": "III.MOR.8.05",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Illustrate how a disciple of Christ is called to live in the world but not of the world since our true home is in heaven,\nand give examples of how our American society both supports and opposes our\r\nChristian Life.\r\n",
      "comments": [],
      "ancestorIds": [
        "E311237730BC4C7BB6751E0664A734A8",
        "18EF1D81B6994AA5BD580193C2A0039D"
      ],
