      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "2",
      "statementNotation": "2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Child recognizes the number of objects in a small set.",
      "comments": [],
      "ancestorIds": [
        "32A2ABB2E81E48DC8F0661C1F7732854"
      ],
      "parentId": "32A2ABB2E81E48DC8F0661C1F7732854"
    },
    "4051E71BC9044C028F87F9499DFDEED4": {
      "id": "4051E71BC9044C028F87F9499DFDEED4",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 2,
      "listId": "b",
      "statementNotation": "1.b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With support, understands that counting\nwords are separate words, such as “one,”\n“two,” “three” versus “onetwothree.” ",
      "comments": [],
