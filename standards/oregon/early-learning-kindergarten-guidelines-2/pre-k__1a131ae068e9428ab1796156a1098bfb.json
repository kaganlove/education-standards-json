    "161875EC5AF9453DBC1AA058C2A253D3": {
      "id": "161875EC5AF9453DBC1AA058C2A253D3",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 2,
      "listId": "a",
      "statementNotation": "5.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With support, begins to\nunderstand that a written numeral\nrepresents a quantity and may\ndraw objects or use informal\nsymbols to represent numbers. ",
      "comments": [],
      "ancestorIds": [
        "F9DB114077AC4D9D95F323B72092D0B0",
        "32A2ABB2E81E48DC8F0661C1F7732854"
      ],
      "parentId": "F9DB114077AC4D9D95F323B72092D0B0"
    },
    "F9DB114077AC4D9D95F323B72092D0B0": {
      "id": "F9DB114077AC4D9D95F323B72092D0B0",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 1,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Child associates a quantity with written numerals and begins to write numbers. ",
      "comments": [],
      "ancestorIds": [
        "32A2ABB2E81E48DC8F0661C1F7732854"
      ],
      "parentId": "32A2ABB2E81E48DC8F0661C1F7732854"
    },
    "2AB9F0725006460D97252BA7E9A08C69": {
