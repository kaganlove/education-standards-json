      "asnIdentifier": null,
      "position": 1016,
      "depth": 1,
      "listId": "6",
      "statementNotation": "6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Child understands addition as adding to and understands subtraction as taking away from. ",
      "comments": [],
      "ancestorIds": [
        "9AF205F3CD3F4AFDBCEF51DDC923F96C"
      ],
      "parentId": "9AF205F3CD3F4AFDBCEF51DDC923F96C"
