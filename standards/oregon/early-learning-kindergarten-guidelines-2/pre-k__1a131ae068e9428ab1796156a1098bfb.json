    "38F7E63FB15A49469C0B5EBFA9855CB0": {
      "id": "38F7E63FB15A49469C0B5EBFA9855CB0",
      "asnIdentifier": null,
      "position": 1019,
      "depth": 2,
      "listId": "a",
      "statementNotation": "7.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With support, recognizes a simple\npattern, and with trusted adult\nassistance, fills in the missing element\nof a pattern, such as red, blue, red,\nblue, ___, blue. Duplicates and extends\nABABAB patterns.",
      "comments": [],
      "ancestorIds": [
        "1E647FDEE8A74836AB5556AC760E1725",
        "9AF205F3CD3F4AFDBCEF51DDC923F96C"
