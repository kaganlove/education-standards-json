      "position": 22000,
      "depth": 2,
      "listId": "3.4",
      "statementNotation": "7.LS.3.4",
      "statementLabel": null,
      "description": "Students know that evidence from geologic layers and radioactive dating indicates Earth is approximately six to ten thousand years old and that life on this planet has existed for more than six thousand years. ",
      "ancestorIds": [
        "07D6629FD4D243678AD151092F4BD733",
        "FDA3550D65B347F0BF1E378DE16FF903"
      ],
      "parentId": "07D6629FD4D243678AD151092F4BD733"
    },
