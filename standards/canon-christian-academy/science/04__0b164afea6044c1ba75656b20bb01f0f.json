    "6246B7DD2E2345FAB7D8B0C847F51FC9": {
      "id": "6246B7DD2E2345FAB7D8B0C847F51FC9",
      "asnIdentifier": null,
      "position": 1026,
      "depth": 2,
      "listId": "5.3",
      "statementNotation": "3.ES.5.3",
      "statementLabel": null,
      "description": "Students know moving water erodes landforms, reshaping the land by taking it away from some places and depositing it as pebbles, sand, silt, and mud in other places (weathering, transport, and deposition). ",
      "ancestorIds": [
        "B71F765DBCF84B3FBA058DA41F3972AA",
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "B71F765DBCF84B3FBA058DA41F3972AA"
    },
    "1920D083A28A4E77BDD9C8D8F9C0517A": {
      "id": "1920D083A28A4E77BDD9C8D8F9C0517A",
      "asnIdentifier": null,
      "position": 1025,
      "depth": 2,
      "listId": "5.2",
      "statementNotation": "3.ES.5.2",
      "statementLabel": null,
      "description": "Students know natural processes, including freezing and thawing and the growth of roots, cause rocks to break down into smaller pieces.",
      "ancestorIds": [
        "B71F765DBCF84B3FBA058DA41F3972AA",
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "B71F765DBCF84B3FBA058DA41F3972AA"
    },
    "05D54C29EA5048E0B08E3CA8FF22C637": {
      "id": "05D54C29EA5048E0B08E3CA8FF22C637",
      "asnIdentifier": null,
      "position": 1024,
      "depth": 2,
      "listId": "5.1",
      "statementNotation": "3.ES.5.1",
      "statementLabel": null,
      "description": "Students know some changes in the earth are due to slow processes, such as erosion, and some changes are due to rapid processes, such as landslides, volcanic eruptions, and earthquakes.",
      "ancestorIds": [
        "B71F765DBCF84B3FBA058DA41F3972AA",
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "B71F765DBCF84B3FBA058DA41F3972AA"
    },
    "B71F765DBCF84B3FBA058DA41F3972AA": {
      "id": "B71F765DBCF84B3FBA058DA41F3972AA",
      "asnIdentifier": null,
      "position": 1023,
      "depth": 1,
      "listId": "5.0",
      "statementNotation": "3.ES.5.0",
      "statementLabel": null,
      "description": "Waves, wind, water, and ice shape and reshape Earth's land surface. As a basis for understanding this concept:",
      "ancestorIds": [
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "5F977F490096491FB99BF5EA69EF1F68"
    },
    "66A19B36BD08468CBE91DF490329D3E7": {
      "id": "66A19B36BD08468CBE91DF490329D3E7",
      "asnIdentifier": null,
      "position": 1022,
      "depth": 2,
      "listId": "4.2",
      "statementNotation": "3.ES.4.2",
      "statementLabel": null,
      "description": "Students know how to identify common rock-forming minerals (including quartz, calcite, feldspar, mica, and hornblende) and ore minerals by using a table of diagnostic properties.",
      "ancestorIds": [
        "22A5D8203F704FB7A21F8A4A629EE7F7",
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "22A5D8203F704FB7A21F8A4A629EE7F7"
    },
    "DDE14E0AE5634D6EA44FB6D69D783BA1": {
