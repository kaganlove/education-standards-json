      "asnIdentifier": null,
      "position": 1021,
      "depth": 2,
      "listId": "4.1",
      "statementNotation": "3.ES.4.1",
      "statementLabel": null,
      "description": "Students know how to differentiate among igneous, sedimentary, and metamorphic rocks by referring to their properties and methods of formation (the rock cycle).",
      "ancestorIds": [
        "22A5D8203F704FB7A21F8A4A629EE7F7",
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "22A5D8203F704FB7A21F8A4A629EE7F7"
    },
    "22A5D8203F704FB7A21F8A4A629EE7F7": {
