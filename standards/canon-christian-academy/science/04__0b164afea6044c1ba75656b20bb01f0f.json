      "asnIdentifier": null,
      "position": 1020,
      "depth": 1,
      "listId": "4.0",
      "statementNotation": "3.ES.4.0",
      "statementLabel": null,
      "description": "The properties of rocks and minerals reflect the processes that formed them. As a basis for understanding this concept:",
      "ancestorIds": [
        "5F977F490096491FB99BF5EA69EF1F68"
      ],
      "parentId": "5F977F490096491FB99BF5EA69EF1F68"
