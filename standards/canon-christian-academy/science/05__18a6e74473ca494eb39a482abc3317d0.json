      "position": 1003,
      "depth": 2,
      "listId": "1.2",
      "statementNotation": "5.PS.1.2",
      "statementLabel": null,
      "description": "Students know all matter is made of atoms, which may combine to form molecules. ",
      "ancestorIds": [
        "6BECE11E603647878BEC04B0EEFA9F49",
        "584489DF1D50444092B7BF515DF65450"
      ],
      "parentId": "6BECE11E603647878BEC04B0EEFA9F49"
