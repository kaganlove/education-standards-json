      "statementNotation": "LW-PB3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Light can be absorbed, reflected, or transmitted by\na material; if the light is reflected or transmitted, the energy is not absorbed and\nthe material will not change.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "0938FBB7D0094C419B6E1DF8E248766F": {
