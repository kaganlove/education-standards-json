    "4E48B007D62D4B77B56A9FBBA039EA6E": {
      "id": "4E48B007D62D4B77B56A9FBBA039EA6E",
      "asnIdentifier": null,
      "position": 1022,
      "depth": 1,
      "listId": "9",
      "statementNotation": "WHST.6-8.9",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Draw evidence from informational texts to support analysis, reflection, and research. ",
      "comments": [],
      "ancestorIds": [
        "9E5C320867484F67BD543235041B5B75"
      ],
      "parentId": "9E5C320867484F67BD543235041B5B75"
    },
    "8E36983196584F39BFA157ABD7EB56FE": {
      "id": "8E36983196584F39BFA157ABD7EB56FE",
      "asnIdentifier": null,
      "position": 1021,
      "depth": 1,
      "listId": "8",
      "statementNotation": "WHST.6-8.8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Gather relevant information from multiple print and digital sources, using search terms effectively;\nassess the credibility and accuracy of each source; and quote or paraphrase the data and conclusions\nof others while avoiding plagiarism and following a standard format for citation.",
      "comments": [],
      "ancestorIds": [
        "9E5C320867484F67BD543235041B5B75"
      ],
      "parentId": "9E5C320867484F67BD543235041B5B75"
    },
    "0E964F35A0714C38B66F39D5D79C8878": {
      "id": "0E964F35A0714C38B66F39D5D79C8878",
      "asnIdentifier": null,
      "position": 1020,
      "depth": 1,
      "listId": "7",
      "statementNotation": "WHST.6-8.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Conduct short research projects to answer a question (including a self-generated question), drawing on\nseveral sources and generating additional related, focused questions that allow for multiple avenues of\nexploration.",
      "comments": [],
      "ancestorIds": [
        "9E5C320867484F67BD543235041B5B75"
      ],
      "parentId": "9E5C320867484F67BD543235041B5B75"
    },
