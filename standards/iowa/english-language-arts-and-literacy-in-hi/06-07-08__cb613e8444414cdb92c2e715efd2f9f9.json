      "asnIdentifier": null,
      "position": 1016,
      "depth": 1,
      "listId": "4",
      "statementNotation": "WHST.6-8.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Produce clear and coherent writing in which the development, organization, and style are appropriate to\ntask, purpose, and audience.",
      "comments": [],
      "ancestorIds": [
        "8D6FB8E47D23470F9792EDA27CBEA1B6"
