    },
    "25FA301DD93643A386362B44C5F954AC": {
      "id": "25FA301DD93643A386362B44C5F954AC",
      "asnIdentifier": null,
      "position": 1018,
      "depth": 1,
      "listId": "6",
      "statementNotation": "WHST.6-8.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use technology to produce and publish writing to interact and collaborate with others through a variety\nof media to present the relationships between information and ideas efficiently. ",
      "comments": [],
      "ancestorIds": [
        "8D6FB8E47D23470F9792EDA27CBEA1B6"
      ],
      "parentId": "8D6FB8E47D23470F9792EDA27CBEA1B6"
    },
    "6ED1EF6A6A50490A92CEBD1E87FAAC45": {
      "id": "6ED1EF6A6A50490A92CEBD1E87FAAC45",
      "asnIdentifier": null,
      "position": 1017,
      "depth": 1,
      "listId": "5",
      "statementNotation": "WHST.6-8.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With some guidance and support from peers and adults, develop and strengthen writing as needed by\nplanning, revising, editing, rewriting, or trying a new approach, focusing on how well purpose and\naudience have been addressed.",
      "comments": [],
      "ancestorIds": [
        "8D6FB8E47D23470F9792EDA27CBEA1B6"
      ],
      "parentId": "8D6FB8E47D23470F9792EDA27CBEA1B6"
    },
