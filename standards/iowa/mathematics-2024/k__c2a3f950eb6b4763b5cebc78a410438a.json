      "depth": 3,
      "listId": "e",
      "statementNotation": "K.CC.B.4.e",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Each successive number name refers to a quantity that is one\nlarger.",
      "comments": [],
      "ancestorIds": [
