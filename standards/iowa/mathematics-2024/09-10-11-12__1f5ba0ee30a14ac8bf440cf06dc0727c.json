    },
    "8112160A596340E78A484F2EE2BFD66B": {
      "id": "8112160A596340E78A484F2EE2BFD66B",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 3,
      "listId": "1",
      "statementNotation": "G.G-CO.C.9",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Prove theorems about lines and angles. Theorems include\nvertical angles are congruent; when a transversal crosses parallel lines,\nalternate interior angles are congruent and corresponding angles are\ncongruent; points on a perpendicular bisector of a line segment are\nexactly those equidistant from the segment’s endpoints.",
      "comments": [],
      "ancestorIds": [
        "B5988C6988A847F8B7100AED228D8B59",
        "9DCC871E98F448BCAA6AE852EAECB80D",
        "F17575AB54E74019AC44475EE3FBABA1"
