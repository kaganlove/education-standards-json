    "26736E4F4BFC45288B649D4802999FA2": {
      "id": "26736E4F4BFC45288B649D4802999FA2",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 3,
      "listId": "2",
      "statementNotation": "G.G-CO.C.10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Prove theorems about triangles. Theorems include\nmeasures of interior angles of a triangle sum to 180; base angles of\nisosceles triangles are congruent; the segment joining midpoints of two\nsides of a triangle is parallel to the third side and half the length; the\nmedians of a triangle meet at a point.",
      "comments": [],
      "ancestorIds": [
        "B5988C6988A847F8B7100AED228D8B59",
        "9DCC871E98F448BCAA6AE852EAECB80D",
        "F17575AB54E74019AC44475EE3FBABA1"
