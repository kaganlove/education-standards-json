    "C3D2A00F0ACE4E488005170450A1B420": {
      "id": "C3D2A00F0ACE4E488005170450A1B420",
      "asnIdentifier": null,
      "position": 1068,
      "depth": 3,
      "listId": "1",
      "statementNotation": "G.S-CP.B.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Find the conditional probability of 𝐴𝐴 given 𝐵𝐵 as the fraction\nof 𝐵𝐵′𝑠𝑠 outcomes that also belong to 𝐴𝐴 and interpret the answer in terms\nof the model. ★",
      "comments": [],
      "ancestorIds": [
        "0984285077C948F39D0BB6A50B21FA78",
        "73D63E11764A4C5FB45BC45DF705CE98",
        "B6CBE6C764F14E448B19585DAB5DF045"
      ],
      "parentId": "0984285077C948F39D0BB6A50B21FA78"
    },
    "0984285077C948F39D0BB6A50B21FA78": {
