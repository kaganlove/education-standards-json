      "depth": 1,
      "listId": "12",
      "statementNotation": "6.2.PS.12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The child identifies the syllables in his or her name and in familiar objects or words by clapping and segmenting the\nsyllables (phonological awareness – segmenting syllables).",
      "comments": [],
      "ancestorIds": [
        "A96E20CD8C7A46D59D4A40F34D0B2899"
      ],
      "parentId": "A96E20CD8C7A46D59D4A40F34D0B2899"
    },
