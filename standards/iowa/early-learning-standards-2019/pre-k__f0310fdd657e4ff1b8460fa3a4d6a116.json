      "asnIdentifier": null,
      "position": 28000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "6.3.PS.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The child attempts to communicate with others using scribbles, shapes, pictures, letter-like forms and/or letters in writing. ",
      "comments": [],
      "ancestorIds": [
        "45FCAD98433E4946B3D2AA0DB7FCABB4"
      ],
      "parentId": "45FCAD98433E4946B3D2AA0DB7FCABB4"
