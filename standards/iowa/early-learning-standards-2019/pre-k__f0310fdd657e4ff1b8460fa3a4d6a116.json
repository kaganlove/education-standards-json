      "position": 24000,
      "depth": 1,
      "listId": "11",
      "statementNotation": "6.2.PS.11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The child  identifies the beginning sound in words, such as identifying two words that start with the same sound (phonological awareness – alliteration).",
      "comments": [],
      "ancestorIds": [
        "A96E20CD8C7A46D59D4A40F34D0B2899"
      ],
      "parentId": "A96E20CD8C7A46D59D4A40F34D0B2899"
    },
