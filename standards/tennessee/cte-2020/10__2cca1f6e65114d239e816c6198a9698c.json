    "E0A063B3C77E4127859210C6EFDC2142": {
      "id": "E0A063B3C77E4127859210C6EFDC2142",
      "asnIdentifier": null,
      "position": 41000,
      "depth": 1,
      "listId": "20",
      "statementNotation": "20",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Examine the crime control and due process models of criminal justice and how they relate, connecting their application to the concepts of crime, victimization, and criminal behavior along with the powers and restrictions that pertain to law enforcement. Address the significance of the Broken Windows Theory as it relates to criminal behavior.",
      "comments": [],
      "ancestorIds": [
        "14CC4976FC8E41C7B3C177A5ADB81C53"
      ],
      "parentId": "14CC4976FC8E41C7B3C177A5ADB81C53"
