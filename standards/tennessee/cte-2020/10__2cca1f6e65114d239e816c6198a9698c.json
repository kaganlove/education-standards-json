    "7C29F03626264562BC3CDBE38313A880": {
      "id": "7C29F03626264562BC3CDBE38313A880",
      "asnIdentifier": null,
      "position": 42000,
      "depth": 1,
      "listId": "21",
      "statementNotation": "21",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Explore the rights of the accused guaranteed by the United States Constitution in Amendments 1, 4, 5, 6, 7, 8 and 14. Review the following cases and determine effects on law enforcement policy, and corrections policy (search and seizure, exclusionary rule, Miranda, and rights of incarcerated individuals). Summarize the processing of an offender through the criminal justice system citing laws, procedures, and policies that protect the offender’s rights.",
      "comments": [],
      "ancestorIds": [
        "14CC4976FC8E41C7B3C177A5ADB81C53"
      ],
      "parentId": "14CC4976FC8E41C7B3C177A5ADB81C53"
