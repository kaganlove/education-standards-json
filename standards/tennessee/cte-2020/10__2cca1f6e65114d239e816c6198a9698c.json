    "193B61DD931241AE83F7E39ADCE67425": {
      "id": "193B61DD931241AE83F7E39ADCE67425",
      "asnIdentifier": null,
      "position": 39000,
      "depth": 1,
      "listId": "19",
      "statementNotation": "19",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Research landmark juvenile cases and the rights of juveniles. Use research findings to communicate the similarities and differences between the juvenile justice system and the adult system. Discuss pros and cons of each system.",
      "comments": [],
      "ancestorIds": [
        "2C3D5DB052E54085A457B9591A95DDE4"
