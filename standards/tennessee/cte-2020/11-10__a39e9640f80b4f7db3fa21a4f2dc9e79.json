    "FF510EF682E04BBDAECC1C233209A681": {
      "id": "FF510EF682E04BBDAECC1C233209A681",
      "asnIdentifier": null,
      "position": 1066,
      "depth": 1,
      "listId": "26",
      "statementNotation": "26",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a patient health education plan for a real or imagined person that describes\nrecommended preventive measures, signs and symptoms of exacerbation of\ndisease/disorder/injury, pharmacological needs, and support systems to ensure safe and\nspeedy recovery. Incorporate and properly cite information from at least three authoritative\nsources such as textbooks, digital or print healthcare journals, or interviews with related\nhealthcare professionals. Examples of possible topics include effective heat loss and heat\nillness prevention, work back injury prevention, reaching and maintaining optimal weight,\nsafe and effective physical activity, and use of pet, recreation, or music therapy in autistic\nchildren. ",
      "comments": [],
      "ancestorIds": [
        "E02F96B459624F0C910B3D542291547C"
      ],
      "parentId": "E02F96B459624F0C910B3D542291547C"
    },
