    "954370D1AEAA4C12888A194D2BC15113": {
      "id": "954370D1AEAA4C12888A194D2BC15113",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "4",
      "statementNotation": "4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Summarize the Health Insurance Portability and Accountability Act (HIPAA) and other legal\ndirectives regarding medical treatment and analyze their impact on patient rights. Include\nconfidential information shared concerning minor athletes and/or patients with someone\nother than parents.",
      "comments": [],
      "ancestorIds": [
        "0CC0C4141C0147FDAF00C55B1E2AD856"
      ],
      "parentId": "0CC0C4141C0147FDAF00C55B1E2AD856"
    },
    "18F8C0E3151F435A9BDD2BEC419C6C75": {
      "id": "18F8C0E3151F435A9BDD2BEC419C6C75",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "3",
      "statementNotation": "3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare and contrast the specific laws and ethical issues that impact relationships among\npatients/clients and the healthcare professional, and debate these issues in an oral or\nwritten format. Include issues such as codes and standards of practice. ",
      "comments": [],
      "ancestorIds": [
        "0CC0C4141C0147FDAF00C55B1E2AD856"
      ],
      "parentId": "0CC0C4141C0147FDAF00C55B1E2AD856"
    },
    "0AADD07F7ED649E7AD7D482B6CE364A3": {
      "id": "0AADD07F7ED649E7AD7D482B6CE364A3",
      "asnIdentifier": null,
      "position": 1002,
      "depth": 1,
      "listId": "2",
      "statementNotation": "2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate and compare the range of skills, competencies, and professional traits required\nfor careers in the Rehabilitation careers pathway. Using real-time and projected labor\nmarket data, identify local and national employment opportunities and determine areas of\ngrowth in rehabilitation careers. ",
      "comments": [],
      "ancestorIds": [
        "0CC0C4141C0147FDAF00C55B1E2AD856"
      ],
      "parentId": "0CC0C4141C0147FDAF00C55B1E2AD856"
    },
