    "BD32C305BEEC489ABB0BE1D1723AB57D": {
      "id": "BD32C305BEEC489ABB0BE1D1723AB57D",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 1,
      "listId": "8",
      "statementNotation": "8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Evaluate factors that contribute to effective patient/client communication, demonstrating\nsensitivity to barriers, cultural differences, and special needs individuals. Apply effective\npractices within a lab/clinical setting. ",
      "comments": [],
      "ancestorIds": [
        "7A0DA3DBDC8F4622B278C61FF696EA5C"
      ],
      "parentId": "7A0DA3DBDC8F4622B278C61FF696EA5C"
    },
    "226D6D0AD01D4935866F93982FBA77AC": {
      "id": "226D6D0AD01D4935866F93982FBA77AC",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "7",
      "statementNotation": "7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Gather information on the history and development of physical therapy, occupational\ntherapy, speech therapy, and athletic training, including but not limited to significant\nchanges in the profession, major contributors to the field, and impactful practices that were\ndeveloped. Document findings from print and digital professional journals, rehabilitation\ncareer related websites, and textbooks in an oral, visual, digital, or paper product with\nproper citations.",
      "comments": [],
      "ancestorIds": [
        "7A0DA3DBDC8F4622B278C61FF696EA5C"
      ],
      "parentId": "7A0DA3DBDC8F4622B278C61FF696EA5C"
    },
    "C091D6719EAD42B899A0C1196D7C0DA7": {
      "id": "C091D6719EAD42B899A0C1196D7C0DA7",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "6",
      "statementNotation": "6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate current issues and practices surrounding assessment and treatment of clients\nseeking rehabilitation services such as athletes, military personnel, or patients recovering\nfrom surgery or trauma. Demonstrate understanding and application of major legislation\nand policy affecting patient/client interaction by determining the central idea or conclusion\nof a text. Construct an argumentative essay explaining the identified issue, any legislation\nand outcomes. Include both claims and counterclaims equally. ",
      "comments": [],
      "ancestorIds": [
        "7A0DA3DBDC8F4622B278C61FF696EA5C"
      ],
      "parentId": "7A0DA3DBDC8F4622B278C61FF696EA5C"
    },
    "968A3BD9CFB74E97ADB3917805725263": {
      "id": "968A3BD9CFB74E97ADB3917805725263",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Calculate the costs of a range of health insurance plans, including deductibles, co-pays,\nPPO’s and HMO’s. For a selected disease/disorder/injury, predict the total cost (including but\nnot limited to the diagnostics, procedures, and medications involved), allowable\nreimbursement, and actual reimbursement under each of these plans for the course of the\ntreatment. ",
      "comments": [],
      "ancestorIds": [
        "7A0DA3DBDC8F4622B278C61FF696EA5C"
      ],
      "parentId": "7A0DA3DBDC8F4622B278C61FF696EA5C"
    },
