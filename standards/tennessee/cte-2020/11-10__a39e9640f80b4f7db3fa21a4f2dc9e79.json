    "3D25C53A706E446AB8C3A4F78B43D632": {
      "id": "3D25C53A706E446AB8C3A4F78B43D632",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 1,
      "listId": "11",
      "statementNotation": "11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare and contrast physiological responses of patients of differing ages, current health\nstatus, and presence of acute and/or chronic diseases. For example, compare the response\nof a healthy elderly patient with a fractured femur to an overweight adolescent with the\nsame fracture. Explain how one would differentiate treatment to meet varying conditions.",
      "comments": [],
      "ancestorIds": [
        "A64D69C64B8542B7BE407F3CD48F0C00"
      ],
      "parentId": "A64D69C64B8542B7BE407F3CD48F0C00"
    },
    "6450F53B28A743F9AFBE0C528606EF7C": {
      "id": "6450F53B28A743F9AFBE0C528606EF7C",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 1,
      "listId": "10",
      "statementNotation": "10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate the basic principles of kinesiology and relate in an informational paper, brochure,\nor presentation the connection to disease/disorder prevention. Address at minimum:\nmovements of joints and bones, planes, directional terms, body motions, motions between\njoint articular surfaces, mechanisms of joints and biomechanical levers.",
      "comments": [],
      "ancestorIds": [
        "A64D69C64B8542B7BE407F3CD48F0C00"
      ],
      "parentId": "A64D69C64B8542B7BE407F3CD48F0C00"
    },
