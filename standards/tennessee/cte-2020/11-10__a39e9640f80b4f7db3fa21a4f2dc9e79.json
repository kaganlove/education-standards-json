    "F51DBDACF6CB4514B90D52237A693C63": {
      "id": "F51DBDACF6CB4514B90D52237A693C63",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 1,
      "listId": "12",
      "statementNotation": "12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe the physiological and pathological processes of trauma, wound healing, and tissue\nrepair, and evaluate their implications on the development, progression, and\nimplementation of a therapeutic exercise regimen. For example, examine a post-operative\ncardiac patient undergoing cardiac rehabilitation. ",
      "comments": [],
      "ancestorIds": [
        "A64D69C64B8542B7BE407F3CD48F0C00"
      ],
      "parentId": "A64D69C64B8542B7BE407F3CD48F0C00"
