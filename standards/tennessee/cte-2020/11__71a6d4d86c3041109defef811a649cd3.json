    "C29248D63F6D40EC8D700B5BAA757975": {
      "id": "C29248D63F6D40EC8D700B5BAA757975",
      "asnIdentifier": null,
      "position": 54000,
      "depth": 1,
      "listId": "17",
      "statementNotation": "17",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Summarize standards of proof and their application to adult and juvenile cases. Synthesize information in a mock investigation combined with relevant common and statutory law to create a warrant application that demonstrates probable cause. Survey court transcripts and verify the reasonableness of searches and seizures as interpreted by the 4th Amendment.",
      "comments": [],
      "ancestorIds": [
        "B03853D1871B4FAF8C4476E449C35322"
      ],
      "parentId": "B03853D1871B4FAF8C4476E449C35322"
