    "96FF87B10F974A38806D1C8046E33755": {
      "id": "96FF87B10F974A38806D1C8046E33755",
      "asnIdentifier": null,
      "position": 55000,
      "depth": 1,
      "listId": "18",
      "statementNotation": "18",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe the criminal process, and the types of criminal defenses related to the situation in which each is used. Use appropriate legal terminology including, but not limited to mens rea, actus reus, mala prohibita, and mala in se.",
      "comments": [],
      "ancestorIds": [
        "B03853D1871B4FAF8C4476E449C35322"
