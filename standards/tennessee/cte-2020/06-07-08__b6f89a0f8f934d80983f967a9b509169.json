    "8B414AB5E19647D99575CFF25D028C5C": {
      "id": "8B414AB5E19647D99575CFF25D028C5C",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "6",
      "statementNotation": "6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Apply knowledge of personal academic strengths and weaknesses to know when and how to\nask for help, when needed. Understand what resources are available to positively influence\nschool performance. Identify strategies for improving educational achievement and\nperformance. ",
      "comments": [],
      "ancestorIds": [
        "F1724777F71444B68DDB7E30640A1B6F"
      ],
      "parentId": "F1724777F71444B68DDB7E30640A1B6F"
    },
    "D8E1945C038D4CDB9B239C85E9436EDE": {
      "id": "D8E1945C038D4CDB9B239C85E9436EDE",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Evaluate current academic progress and how it aligns to future career goals. Identify and\narticulate academic strengths and areas for improvement. ",
      "comments": [],
      "ancestorIds": [
        "F1724777F71444B68DDB7E30640A1B6F"
      ],
      "parentId": "F1724777F71444B68DDB7E30640A1B6F"
    },
