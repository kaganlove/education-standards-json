    "AADA7B50FF4945F78FB779F7A2DB674B": {
      "id": "AADA7B50FF4945F78FB779F7A2DB674B",
      "asnIdentifier": null,
      "position": 1018,
      "depth": 2,
      "listId": "11",
      "statementNotation": "11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Analyze results from modeling and\nappropriately determine when it is\nnecessary to revise questions. Justify\nrevisions with evidence.",
      "comments": [],
      "ancestorIds": [
        "72BDCB0C5523433698427502D4D2E1D3",
        "D9E8825330FB459D85EAA66ACC541546"
      ],
      "parentId": "72BDCB0C5523433698427502D4D2E1D3"
    },
    "AEDE59F25C304FBA8464F80C61BD5296": {
      "id": "AEDE59F25C304FBA8464F80C61BD5296",
      "asnIdentifier": null,
      "position": 1017,
      "depth": 2,
      "listId": "10",
      "statementNotation": "10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use mathematics and technology to\ndevelop multiple models to predict an\noccurrence in the natural world.\nCompare and contrast the recorded\nobservations from each model. For\nexample, computer modeling can be\nused to analyze current atmospheric\nconditions to predict the weather in\ndays ahead.",
      "comments": [],
      "ancestorIds": [
        "72BDCB0C5523433698427502D4D2E1D3",
        "D9E8825330FB459D85EAA66ACC541546"
      ],
      "parentId": "72BDCB0C5523433698427502D4D2E1D3"
    },
    "710305CB82D143BC8379B69118F376C5": {
      "id": "710305CB82D143BC8379B69118F376C5",
      "asnIdentifier": null,
      "position": 1016,
      "depth": 2,
      "listId": "9",
      "statementNotation": "9",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create models to illustrate questions\nand represent processes or systems\nthat are justified by scientific evidence.\nFor example, models can be diagrams,\ndrawings, or scaled down physical\nrepresentations.",
      "comments": [],
      "ancestorIds": [
        "72BDCB0C5523433698427502D4D2E1D3",
        "D9E8825330FB459D85EAA66ACC541546"
      ],
      "parentId": "72BDCB0C5523433698427502D4D2E1D3"
    },
