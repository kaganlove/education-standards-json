  "standards": {
    "F9117BFA87484A3FB84051629EEC3C6A": {
      "id": "F9117BFA87484A3FB84051629EEC3C6A",
      "asnIdentifier": null,
      "position": 1051,
      "depth": 2,
      "listId": "30",
      "statementNotation": "30",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify and explain the intended use of safety equipment available in the classroom. For\nexample, demonstrate how to properly inspect, use, and maintain safe operating\nprocedures with tools and equipment. Incorporate safety procedures and complete safety\ntest with 100 percent accuracy.",
      "comments": [],
      "ancestorIds": [
        "B6852F9B3EC0421EBFEB99D467555E50"
      ],
      "parentId": null
    },
    "8C369F7C8B344AEFBDF9504893EC091D": {
      "id": "8C369F7C8B344AEFBDF9504893EC091D",
      "asnIdentifier": null,
      "position": 1050,
      "depth": 2,
      "listId": "29",
      "statementNotation": "29",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Accurately read and interpret safety rules, including but not limited to rules published by the\nNational Science Teachers Association (NSTA), rules pertaining to electrical safety,\nOccupational Safety and Health Administration (OSHA) guidelines, and state and national\ncode requirements. Be able to distinguish between the rules and explain why certain rules\napply. ",
      "comments": [],
      "ancestorIds": [
        "B6852F9B3EC0421EBFEB99D467555E50"
      ],
      "parentId": null
    },
