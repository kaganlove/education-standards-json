      "depth": 2,
      "listId": "26",
      "statementNotation": "26",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop an optimal design solution that\nis justified by data analysis and\nscientific knowledge, and meets ethical\nand design criteria and constraints.",
      "comments": [],
