    "AB2BA11FD95A4DDD8615930A60D07B81": {
      "id": "AB2BA11FD95A4DDD8615930A60D07B81",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 2,
      "listId": "8",
      "statementNotation": "8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a design brief that will guide a\ndesign process and follow responsible\nethical practices. For example, the\ndesign brief should outline a problem\ndefinition, design statement, criteria,\nconstraints, and deliverables.",
      "comments": [],
      "ancestorIds": [
        "0F7D355CF3EF434FBAC2CF317640F501",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "0F7D355CF3EF434FBAC2CF317640F501"
    },
    "92D9748B15AF491F87F7D61D88727FF9": {
      "id": "92D9748B15AF491F87F7D61D88727FF9",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 2,
      "listId": "7",
      "statementNotation": "7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Brainstorm for several problem\nsolutions, then conduct research using\nvarious sources (e.g., articles, end-uses,\ntextbooks) to generate more solution\nideas. Justify ideas using evidence from\nthe sources.",
      "comments": [],
      "ancestorIds": [
        "0F7D355CF3EF434FBAC2CF317640F501",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "0F7D355CF3EF434FBAC2CF317640F501"
    },
    "B3A51CE6CCDF48A79417FEA200E8C1FE": {
      "id": "B3A51CE6CCDF48A79417FEA200E8C1FE",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 2,
      "listId": "6",
      "statementNotation": "6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Ask clear, relevant questions that lead\nto defining a design problem. For\nexample, questions should be testable\nand explore the requirements of a\nproblem solution, but not define the\nmethodology to solve the problem.",
      "comments": [],
      "ancestorIds": [
        "0F7D355CF3EF434FBAC2CF317640F501",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "0F7D355CF3EF434FBAC2CF317640F501"
    },
    "0F7D355CF3EF434FBAC2CF317640F501": {
      "id": "0F7D355CF3EF434FBAC2CF317640F501",
      "asnIdentifier": null,
      "position": 1010,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Engineering Path",
      "comments": [],
      "ancestorIds": [
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "7EA0B02E753B4AADBABBDF4F91D41252"
    },
    "2D9EB5D989664A4082572A0AE59DAAF5": {
      "id": "2D9EB5D989664A4082572A0AE59DAAF5",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 2,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop an original proposal as would\na natural or social scientist that will\nguide the scientific inquiry and follow\nresponsible ethical practices. For\nexample, the proposal should outline\nthe reason for the research interest,\nhypothesis, methodology, data\nanalysis, importance of study, and\ndeliverables.",
      "comments": [],
      "ancestorIds": [
        "D7DFD0B200F74BB4AFCDF585E39CE103",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "D7DFD0B200F74BB4AFCDF585E39CE103"
    },
    "CBBF2A58963F4932BA79FD5EB2DE49F1": {
      "id": "CBBF2A58963F4932BA79FD5EB2DE49F1",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 2,
      "listId": "4",
      "statementNotation": "4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Research various sources (e.g., articles,\nend-uses, textbooks) and identify one\nor more questions that will guide a\nscientific investigation. For example,\nquestions should be relevant, testable,\nand based on current scientific\nknowledge.",
      "comments": [],
      "ancestorIds": [
        "D7DFD0B200F74BB4AFCDF585E39CE103",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
      "parentId": "D7DFD0B200F74BB4AFCDF585E39CE103"
    },
    "ABD118DDC4C74D939E8C42CF21B68F32": {
      "id": "ABD118DDC4C74D939E8C42CF21B68F32",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 2,
      "listId": "3",
      "statementNotation": "3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Engage in scientific inquiry by\nbrainstorming for questions to\nunderstand how a certain\nphenomenon in the natural world\nworks, to understand why a\nphenomenon occurs, or to determine\nthe validity of a theory.",
      "comments": [],
      "ancestorIds": [
        "D7DFD0B200F74BB4AFCDF585E39CE103",
        "7EA0B02E753B4AADBABBDF4F91D41252"
      ],
