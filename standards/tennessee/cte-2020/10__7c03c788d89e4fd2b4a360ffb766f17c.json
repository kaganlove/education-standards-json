    "8E4A0876D3FE44F086F621E43A3BBB23": {
      "id": "8E4A0876D3FE44F086F621E43A3BBB23",
      "asnIdentifier": null,
      "position": 1029,
      "depth": 2,
      "listId": "18",
      "statementNotation": "18",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Outline testing procedures that identify\ntype of data (e.g., number of trials, cost,\nrisk, and time) that is needed to\nproduce reliable measurements and\nthe specifications (e.g., effectiveness,\nefficiency, and durability) to determine\nwhether a design has exceeded or\nfailed expectations.",
      "comments": [],
      "ancestorIds": [
        "3F249DF5B91643FC82558FC6545F87ED",
        "0F079CC09BDD4B8AB4C574C52FC8D326"
      ],
      "parentId": "3F249DF5B91643FC82558FC6545F87ED"
    },
