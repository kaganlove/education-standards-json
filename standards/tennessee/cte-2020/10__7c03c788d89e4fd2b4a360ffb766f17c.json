      "asnIdentifier": null,
      "position": 1021,
      "depth": 2,
      "listId": "13",
      "statementNotation": "13",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify and sketch at least three\nalternative solutions, to a problem, that\nconsider analyses such as mechanical\nand electrical systems. For example,\ncomputer modeling can be used to\nanalyze the effect of stress and strain\non a beam.",
      "comments": [],
      "ancestorIds": [
        "74420D6127D846EAAE0191E9636AD752",
        "D9E8825330FB459D85EAA66ACC541546"
      ],
