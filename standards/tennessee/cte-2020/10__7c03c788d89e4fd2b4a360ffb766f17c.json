      "parentId": null
    },
    "F2FF9FEF618341BA96654AE35F2A7FB6": {
      "id": "F2FF9FEF618341BA96654AE35F2A7FB6",
      "asnIdentifier": null,
      "position": 1048,
      "depth": 2,
      "listId": "28",
      "statementNotation": "28",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a design document to\ncommunicate the final design solution\nand how well it meets the design\ncriteria and constraints. For example,\nthe design document can include\ncharts, graphs, calculations, engineering\ndrawings, as well as information\nregarding marketing, distribution, and\nsales. For example, conduct a STEM\nforum, present engineering design\nbriefs, and provide evidence to support\narguments for or against design\nsolutions.",
      "comments": [],
      "ancestorIds": [
        "0721F271362A4EC3AB9D717EE84F23A4",
        "A93DE5A43FD449F9A4C0D77958FA7388"
      ],
      "parentId": "0721F271362A4EC3AB9D717EE84F23A4"
    },
    "0721F271362A4EC3AB9D717EE84F23A4": {
      "id": "0721F271362A4EC3AB9D717EE84F23A4",
      "asnIdentifier": null,
      "position": 1047,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Engineering Path ",
      "comments": [],
