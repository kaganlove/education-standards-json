      "statementLabel": null,
      "description": "Team Project",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "845E5FCF984E44638298C7299D005683": {
      "id": "845E5FCF984E44638298C7299D005683",
      "asnIdentifier": null,
      "position": 1066,
      "depth": 1,
      "listId": "24",
      "statementNotation": "24",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use computer tools, such as spreadsheet software (e.g., Microsoft Excel), analytical/scientific\nsoftware (e.g., MATLAB), and/or programming software (e.g., Microsoft Visual Basic) to solve\nat least one problem from the content described in the standards above. Examples may\ninclude the use of spreadsheets to input data from experimental tests and create graphs for\npresentation, or the use of MATLAB to solve a system of equations.",
      "comments": [],
      "ancestorIds": [
        "F2B66F97DBBF48EDA4D575CE64182DAC"
      ],
      "parentId": "F2B66F97DBBF48EDA4D575CE64182DAC"
    },
    "F2B66F97DBBF48EDA4D575CE64182DAC": {
      "id": "F2B66F97DBBF48EDA4D575CE64182DAC",
