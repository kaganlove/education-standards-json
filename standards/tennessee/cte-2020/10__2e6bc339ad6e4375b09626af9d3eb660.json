    "CA58DC1A96A04E6785B5310F9C2BE4F7": {
      "id": "CA58DC1A96A04E6785B5310F9C2BE4F7",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "4",
      "statementNotation": "4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Research the postsecondary institutions (colleges of applied technology, community\ncolleges, and four-year universities) in Tennessee and other states that offer engineering or\nengineering technology programs. Write an informative paper or develop an infographic\nidentifying admissions criteria, the postsecondary programs of study, and the secondary courses that will prepare individuals to be successful in a postsecondary engineering or\nengineering technology program.",
      "comments": [],
      "ancestorIds": [
        "EFA271830985450AB91D58236858F6B7"
      ],
      "parentId": "EFA271830985450AB91D58236858F6B7"
