    "0E7E26D0160C46C09CABE99D72548F0B": {
      "id": "0E7E26D0160C46C09CABE99D72548F0B",
      "asnIdentifier": null,
      "position": 1034,
      "depth": 1,
      "listId": "14",
      "statementNotation": "14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Apply skills learned in Supply Chain Management courses to analyze a case study in which\nthe supply chain for a particular product or company was disrupted. In a written paper or\npresentation, describe what went wrong and how management addressed the problem.\nDiscuss whether or not the issue was resolved and the impact it had on either the supply chain or the industry as a whole. For example, analyze the 2002 Long Beach Port Strike and\ndemonstrate through graphic representations and narrative writing how the strike impacted\na range of manufacturers, retailers, and consumers in multiple locations.",
      "comments": [],
      "ancestorIds": [
        "150E85200C834E918E52E6ACDD0D0E4F"
      ],
      "parentId": "150E85200C834E918E52E6ACDD0D0E4F"
    },
    "D8A8792AF63749F784513E9C45B4E675": {
      "id": "D8A8792AF63749F784513E9C45B4E675",
      "asnIdentifier": null,
      "position": 1033,
      "depth": 1,
      "listId": "13",
      "statementNotation": "13",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a written profile of how a local business coordinates and controls the order cycle\nand associated information systems of scheduling, cost analysis, documentation\nconfirmation, packing lists, MSDS, product seals, packaging types, packaging labels, and\nrouting issues. Include a description of the performance metrics used to monitor the quality,\nquantity, cost, and efficiency of the movement and storage of goods.",
      "comments": [],
      "ancestorIds": [
        "150E85200C834E918E52E6ACDD0D0E4F"
      ],
      "parentId": "150E85200C834E918E52E6ACDD0D0E4F"
    },
    "ADBBB06008044BCA94267F5B7FF966BE": {
      "id": "ADBBB06008044BCA94267F5B7FF966BE",
      "asnIdentifier": null,
      "position": 1032,
      "depth": 1,
      "listId": "12",
      "statementNotation": "12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Simulate the work of a warehouse manager or logistician by planning for the shipment of a\nproduct. Given a set of constraints, such as a specified timetable, destination, quantity, or\nother factor, determine the number of pallets needed and assign dock doors to\naccommodate the appropriate number of loads. ",
      "comments": [],
      "ancestorIds": [
        "150E85200C834E918E52E6ACDD0D0E4F"
      ],
      "parentId": "150E85200C834E918E52E6ACDD0D0E4F"
    },
