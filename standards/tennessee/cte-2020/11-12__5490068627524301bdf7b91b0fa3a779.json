    "B6E57ECC05964097BF4B480FEF2FDF3F": {
      "id": "B6E57ECC05964097BF4B480FEF2FDF3F",
      "asnIdentifier": null,
      "position": 1046,
      "depth": 2,
      "listId": "f",
      "statementNotation": "18.f",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate the modes of transportation to be used to ship materials and develop\nguidelines for when each should be used. Consider truck, rail, air transport, maritime\ntransport, intermodal, and outsourcing as methods of moving product.",
      "comments": [],
      "ancestorIds": [
        "1ABE413BD4E24664996E37F3F49AB2A4",
        "51E409DA31D3457F9FB1DF1E82D973D1"
