      "id": "20206FFF3EF149ABB92B190A8634ABA6",
      "asnIdentifier": null,
      "position": 1031,
      "depth": 1,
      "listId": "11",
      "statementNotation": "11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create a flow chart for the processing of incoming goods and materials using standardized\nindustry protocols and procedures. Include processes for dealing with damaged, incorrect,\nand incomplete orders.",
      "comments": [],
      "ancestorIds": [
        "150E85200C834E918E52E6ACDD0D0E4F"
      ],
      "parentId": "150E85200C834E918E52E6ACDD0D0E4F"
    },
