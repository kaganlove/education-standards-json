    "1D0091E96E724C2A9975F3DDF4F91362": {
      "id": "1D0091E96E724C2A9975F3DDF4F91362",
      "asnIdentifier": null,
      "position": 1037,
      "depth": 1,
      "listId": "3",
      "statementNotation": "5.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Ethical Practices: Examine the progression of ethical practices in the textile and\napparel industry, using historical records found in textbooks or other sources.\nEvaluate current ethical issues affecting the fashion industry, included but not limited to:\nknockoff products/designer forgeries, shoplifting, sweatshop labor, provocative\nadvertising, and body image. Compose an argument, including development of claim(s)\nand counterclaim(s), debating the sociological and economic impacts of these issues on\nthe fashion industry. ",
      "comments": [],
      "ancestorIds": [
        "1AA6164AD3B6415783A68BBC8028E190"
      ],
      "parentId": "1AA6164AD3B6415783A68BBC8028E190"
    },
    "3AC20AD5E1CF4C69AC058BB9E7D2F9C2": {
      "id": "3AC20AD5E1CF4C69AC058BB9E7D2F9C2",
      "asnIdentifier": null,
      "position": 1036,
      "depth": 1,
      "listId": "2",
      "statementNotation": "5.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Textile Products: Analyze research from multiple sources such as technical journals to\nsummarize the typical products made out of textiles in the apparel industry. Compare and contrast determining factors for textile suitability and applications for a variety of\ncommercial purposes.",
      "comments": [],
      "ancestorIds": [
        "1AA6164AD3B6415783A68BBC8028E190"
      ],
      "parentId": "1AA6164AD3B6415783A68BBC8028E190"
    },
    "11E22D53A98D4609BC7A1F70FA8DA472": {
      "id": "11E22D53A98D4609BC7A1F70FA8DA472",
      "asnIdentifier": null,
      "position": 1035,
      "depth": 1,
      "listId": "1",
      "statementNotation": "5.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "History of Textiles: Examine the historical development of processes and techniques in\ntextile design and production. Using descriptive text, summarize the evolution of trends in\ntextile colors, textures, and prints used in fashion design. Identify textile technologies that\nhave influenced apparel design, production, merchandising, and sales.",
      "comments": [],
      "ancestorIds": [
        "1AA6164AD3B6415783A68BBC8028E190"
      ],
      "parentId": "1AA6164AD3B6415783A68BBC8028E190"
