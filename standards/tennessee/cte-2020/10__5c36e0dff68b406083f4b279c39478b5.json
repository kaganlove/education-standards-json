      "asnIdentifier": null,
      "position": 1044,
      "depth": 1,
      "listId": "1",
      "statementNotation": "7.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Garment Construction: Demonstrate basic garment construction skills and techniques.\nDemonstrate proficiency in proper pressing, fitting, alteration, finishing, and\nembellishment for quality garment construction. Demonstrate the appropriate use,\nselection, and maintenance of equipment, tools, and sewing supplies for the construction\nof apparel.",
      "comments": [],
      "ancestorIds": [
        "09BE7DB929124E11A88C108C80EEDD44"
      ],
      "parentId": "09BE7DB929124E11A88C108C80EEDD44"
