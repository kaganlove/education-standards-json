    "075D5AC7354D4B1EA67045E0B915706D": {
      "id": "075D5AC7354D4B1EA67045E0B915706D",
      "asnIdentifier": null,
      "position": 1042,
      "depth": 1,
      "listId": "4",
      "statementNotation": "6.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Industry Standards: Review informational resources identifying textile and apparel\nindustry standards that promote quality control in apparel manufacturing. Compose\nan informative essay citing specific textual evidence that critiques the effectiveness and\nimplications of these standards on product quality, cost, and supply chain dynamics. ",
      "comments": [],
      "ancestorIds": [
        "27C68F30524C4E79959688698227246D"
      ],
      "parentId": "27C68F30524C4E79959688698227246D"
    },
    "DBBD2331DAAF4DFFB0C8582017A238F3": {
      "id": "DBBD2331DAAF4DFFB0C8582017A238F3",
      "asnIdentifier": null,
      "position": 1041,
      "depth": 1,
      "listId": "3",
      "statementNotation": "6.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Textile Fabrication Process: Identify common fabrics and examine basic textile\nfabrication processes. Research and summarize technical sources such as industry\nmanuals or manufacturers’ quality control protocol to develop a list of general instructions\nfor the selection, evaluation, use, and care of fabrics used in fashion design. ",
      "comments": [],
      "ancestorIds": [
        "27C68F30524C4E79959688698227246D"
      ],
      "parentId": "27C68F30524C4E79959688698227246D"
    },
    "8E754B52CE5C4B0380ADC9D299404CED": {
      "id": "8E754B52CE5C4B0380ADC9D299404CED",
      "asnIdentifier": null,
      "position": 1040,
      "depth": 1,
      "listId": "2",
      "statementNotation": "6.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Natural and Synthetic Fibers: Analyze characteristics of natural and synthetic fibers.\nCompare and contrast natural and synthetic textiles with regards to performance,\ndying, printing, and finishing processes. Determine the suitability of various textiles for\nspecific applications.",
      "comments": [],
      "ancestorIds": [
        "27C68F30524C4E79959688698227246D"
      ],
      "parentId": "27C68F30524C4E79959688698227246D"
    },
    "95A6E0FB23134D05A8EA7C2C334CF977": {
      "id": "95A6E0FB23134D05A8EA7C2C334CF977",
      "asnIdentifier": null,
      "position": 1039,
      "depth": 1,
      "listId": "1",
      "statementNotation": "6.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Common Fibers: Identify common fibers and describe their associated characteristics\nand applications. Perform swatch tests to analyze and evaluate fiber characteristics,\nincluding their aesthetic features as well as mechanical and chemical properties.",
      "comments": [],
      "ancestorIds": [
        "27C68F30524C4E79959688698227246D"
      ],
      "parentId": "27C68F30524C4E79959688698227246D"
