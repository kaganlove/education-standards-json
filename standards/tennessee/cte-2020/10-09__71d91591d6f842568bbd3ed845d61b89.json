    "B322C26652B14E068858A18A5858DA1C": {
      "id": "B322C26652B14E068858A18A5858DA1C",
      "asnIdentifier": null,
      "position": 1043,
      "depth": 1,
      "listId": "18",
      "statementNotation": "18",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create a list of the decisions that must be made and the problems that could potentially\narise in a complex supply chain. Research individual and group problem-solving and\ndecision-making strategies applicable to each decision/problem listed. Choose one of the\nproblems listed, apply knowledge of supply chain management, and prepare a presentation\nrecommending a solution.",
      "comments": [],
      "ancestorIds": [
        "6CB8068FABAE401AB20635C1E0643189"
      ],
      "parentId": "6CB8068FABAE401AB20635C1E0643189"
