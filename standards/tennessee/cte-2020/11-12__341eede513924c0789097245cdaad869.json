    "6C3E3EEC3CBB4DBD8DECA468A3ABAC9E": {
      "id": "6C3E3EEC3CBB4DBD8DECA468A3ABAC9E",
      "asnIdentifier": null,
      "position": 1050,
      "depth": 1,
      "listId": "12",
      "statementNotation": "12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compile and interpret the evaluation rubric and feedback from the client, reading the results\nclosely to allow for critical analysis and reflection. Upon conclusion of the capstone project,\ncraft a reflection paper discussing the experience and its impact on career growth. Use\ntechnology to showcasehighlights, challenges, and lessons learned from the capstone.",
      "comments": [],
      "ancestorIds": [
        "30CDBF2B14CA471D9E5FFCBA220300C7"
      ],
      "parentId": "30CDBF2B14CA471D9E5FFCBA220300C7"
    },
    "3CAB85AC7D41473CB731D4018623062A": {
      "id": "3CAB85AC7D41473CB731D4018623062A",
      "asnIdentifier": null,
      "position": 1049,
      "depth": 1,
      "listId": "11",
      "statementNotation": "11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Using the final approved contract, execute the timeline to demonstrate teamwork, problemsolving, and decision-making skills. Work collaboratively to ensure that the needs and\nexpectations of the client are met for the event. In a personal journal, document the\ncapstone experience, drawing on the connections between the project and course content. ",
      "comments": [],
      "ancestorIds": [
        "30CDBF2B14CA471D9E5FFCBA220300C7"
      ],
      "parentId": "30CDBF2B14CA471D9E5FFCBA220300C7"
    },
    "1B85DA346A5449328D9AC0CCCAB59658": {
