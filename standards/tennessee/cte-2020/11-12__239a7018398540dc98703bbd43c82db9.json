      "position": 1019,
      "depth": 1,
      "listId": "6",
      "statementNotation": "6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Building on standards from Entrepreneurship and Business Management, individually or in\nteams, develop a new or modify an existing business plan for the proposed startup/business\nventure. Include at minimum the following components: ",
      "comments": [],
      "ancestorIds": [
        "3E00433461F54165B5A7373675DCB6D3"
      ],
