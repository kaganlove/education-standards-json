    "1C2701DDE2FF4671AA9E678C2AA1DD20": {
      "id": "1C2701DDE2FF4671AA9E678C2AA1DD20",
      "asnIdentifier": null,
      "position": 1036,
      "depth": 1,
      "listId": "13",
      "statementNotation": "13",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create a portfolio, or similar collection of work, that illustrates mastery of skills and\nknowledge outlined in the previous courses and applied in the practicum. The portfolio\nshould reflect thoughtful assessment and evaluation of the progression of work involving\nthe application of steps of the entrepreneurial or business acquisition process. The following\ndocuments will reside in the student’s portfolio: ",
      "comments": [],
      "ancestorIds": [
        "CC65C9AD369344B2A0B293C5F46C077D"
      ],
      "parentId": "CC65C9AD369344B2A0B293C5F46C077D"
    },
