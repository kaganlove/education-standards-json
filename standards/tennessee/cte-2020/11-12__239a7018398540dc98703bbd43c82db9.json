    "EFA650A56E9E46BCB8B4522B5E9DBDA2": {
      "id": "EFA650A56E9E46BCB8B4522B5E9DBDA2",
      "asnIdentifier": null,
      "position": 1017,
      "depth": 1,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Independently or in teams, conduct a market research project to determine the viability of an\noriginal business idea or the recommendations to improve the marketing activities of an\nexisting business. Demonstrate the ability to design simple surveys, retrieve secondary data\nfrom print and online sources, tabulate results, write analyses, and make decisions based on\nevidence. If possible, consult or collaborate with local area professionals to determine the\nviability of original business ideas. Compile findings and future directions in a professional\nprint or online document.",
      "comments": [],
      "ancestorIds": [
        "5945FF32AE88406ABA8467DB293588D9"
      ],
      "parentId": "5945FF32AE88406ABA8467DB293588D9"
    },
