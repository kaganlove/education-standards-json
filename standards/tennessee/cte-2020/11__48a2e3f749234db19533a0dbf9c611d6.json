    "2AE5DF1A5E6E436885DEB8075BB88A76": {
      "id": "2AE5DF1A5E6E436885DEB8075BB88A76",
      "asnIdentifier": null,
      "position": 11000,
      "depth": 1,
      "listId": "8",
      "statementNotation": "8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Log, store, and export data received from two or more sensors (for example, vision/light,\naudio, and touch) in a robotic or automated system. Explain why these procedures would be\nuseful and provide specific examples.",
      "comments": [],
      "ancestorIds": [
        "4B69B65E44EB4A48B3305EDD02959936"
      ],
      "parentId": "4B69B65E44EB4A48B3305EDD02959936"
    },
    "E9656C6ACFFE4963B7F987E2155BFAB2": {
      "id": "E9656C6ACFFE4963B7F987E2155BFAB2",
      "asnIdentifier": null,
      "position": 10000,
      "depth": 1,
      "listId": "7",
      "statementNotation": "7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create a flowchart of a program for a robotic system. Convert the flowchart into a working\nprogram. Test, modify, and optimize the program. Write a technical report evaluating the\nperformance of the program. Support all claims with specific examples. ",
      "comments": [],
      "ancestorIds": [
        "4B69B65E44EB4A48B3305EDD02959936"
      ],
      "parentId": "4B69B65E44EB4A48B3305EDD02959936"
