    "55AA3D3A31AE4F28B8BB3925BFECCAB6": {
      "id": "55AA3D3A31AE4F28B8BB3925BFECCAB6",
      "asnIdentifier": null,
      "position": 5000,
      "depth": 1,
      "listId": "4",
      "statementNotation": "4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Write a persuasive essay explaining why robots should be used in certain circumstances.\nCite textual evidence to support claims (for example, assemble evidence from medical\njournals to support a claim that the use of robots has lowered costs and increased efficiency\namong medical providers). Other examples may derive from the areas identified in standard\n3. During a class discussion, defend original arguments and debate peer perspectives using\nclaim(s) and counterclaim(s) developed in the persuasive essay. ",
      "comments": [],
      "ancestorIds": [
        "77B794633B9445B39FD0D7FC5F423678"
      ],
      "parentId": "77B794633B9445B39FD0D7FC5F423678"
    },
