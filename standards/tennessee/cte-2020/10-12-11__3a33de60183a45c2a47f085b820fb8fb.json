      "asnIdentifier": null,
      "position": 1026,
      "depth": 1,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate the ability to calculate and explain to others the ordering cost, holding cost,\nand shortage costs for a given material or product within the supply chain. Determine total\ncost as a function of these costs and demonstrate the effects on profit for a specified price\nand quantity. Create a spreadsheet and graphical comparison of cost variations and\nrecommend best price/quantity amounts to optimize profit. ",
      "comments": [],
      "ancestorIds": [
        "1B35E2984AB1451CB5B04035FC32274D"
      ],
      "parentId": "1B35E2984AB1451CB5B04035FC32274D"
    },
