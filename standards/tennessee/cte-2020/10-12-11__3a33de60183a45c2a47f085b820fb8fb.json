    "93309626473B4C70B62B41CDBAD23053": {
      "id": "93309626473B4C70B62B41CDBAD23053",
      "asnIdentifier": null,
      "position": 1031,
      "depth": 1,
      "listId": "9",
      "statementNotation": "9",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate various warehouse management system (WMS) software programs and create a\ncomparison chart that could be used by a warehouse manager to select software to meet\nthe specific needs of his/her operation. ",
      "comments": [],
      "ancestorIds": [
        "B614A9C2047B47DB9221E93B096E4EC2"
