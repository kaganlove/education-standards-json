    "642FFA8ADF6741C2A5998D56E53EE369": {
      "id": "642FFA8ADF6741C2A5998D56E53EE369",
      "asnIdentifier": null,
      "position": 1037,
      "depth": 1,
      "listId": "14",
      "statementNotation": "14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Investigate the importance of negotiation in business in general and specifically in supply\nchain management. Prepare a “how-to” demonstration that includes preparations for\nnegotiations, effective negotiation strategies and tactics, developing a negotiation plan that\nprovides benefits to all involved parties, and selecting a business partner that is like-minded\nin mission. ",
      "comments": [],
      "ancestorIds": [
        "157FA228DE2F47639A2B1410A3B305FD"
