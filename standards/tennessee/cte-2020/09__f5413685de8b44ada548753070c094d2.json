    "7A8F183B54E744AD8BEAE02FBE6DC5D0": {
      "id": "7A8F183B54E744AD8BEAE02FBE6DC5D0",
      "asnIdentifier": null,
      "position": 1024,
      "depth": 1,
      "listId": "14",
      "statementNotation": "14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify and explain the intended use of safety equipment available in the classroom. For\nexample, demonstrate how to properly inspect, use, and maintain safe operating\nprocedures with tools and equipment. Also demonstrate the use of safety glasses, gloves,\nfire extinguisher, shower and eyewash stations.",
      "comments": [],
      "ancestorIds": [
        "4D1926E4F57F482C8E0B4B91AAC47A95"
      ],
      "parentId": "4D1926E4F57F482C8E0B4B91AAC47A95"
