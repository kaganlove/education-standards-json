    "7E8D622266404ADF8D1647821CB04E53": {
      "id": "7E8D622266404ADF8D1647821CB04E53",
      "asnIdentifier": null,
      "position": 7008,
      "depth": 2,
      "listId": "C",
      "statementNotation": "AquatSci.c.2.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "know that scientific theories are based on natural and physical phenomena and are capable of being tested by multiple independent researchers. Unlike hypotheses, scientific theories are well-established and highly-reliable explanations, but they may be subject to change as new areas of science and new technologies are developed; ",
      "comments": [],
      "ancestorIds": [
        "CDADF0C3DD9A44C69C65D3C9C038B352",
        "008C5AC3043C439B8D1D33DBF21EB15B"
      ],
      "parentId": "CDADF0C3DD9A44C69C65D3C9C038B352"
    },
    "BBE64AD6DF1B4878BA27F8136CB8D4FC": {
      "id": "BBE64AD6DF1B4878BA27F8136CB8D4FC",
      "asnIdentifier": null,
      "position": 7007,
      "depth": 2,
      "listId": "B",
      "statementNotation": "AquatSci.c.2.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "know that scientific hypotheses are tentative and testable statements that must be capable of being supported or not supported by observational evidence. Hypotheses of durable explanatory power which have been tested over a wide variety of conditions are incorporated into theories; ",
      "comments": [],
      "ancestorIds": [
        "CDADF0C3DD9A44C69C65D3C9C038B352",
        "008C5AC3043C439B8D1D33DBF21EB15B"
      ],
      "parentId": "CDADF0C3DD9A44C69C65D3C9C038B352"
    },
