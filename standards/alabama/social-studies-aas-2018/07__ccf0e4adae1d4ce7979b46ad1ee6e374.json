    "2359A272C7C446CE944B36B044CE118F": {
      "id": "2359A272C7C446CE944B36B044CE118F",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 1,
      "listId": "11",
      "statementNotation": "SS.AAS.7.11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize Alabama’s growing role in the\nworld politically and socially including its impact of the\nmodern Civil Rights Movement from 1954 to the present.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "D58552BDA9474A7389BF9F8F1B0CAB32": {
      "id": "D58552BDA9474A7389BF9F8F1B0CAB32",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 1,
      "listId": "11",
      "statementNotation": "SS.AAS.7.11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize modern social and economic\nopportunities and define what they are. Examples\ninclude legal protections for workers with disabilities and\nanti-discrimination laws.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "DB49629E5C01482B9C40E7AB67305F7F": {
      "id": "DB49629E5C01482B9C40E7AB67305F7F",
      "asnIdentifier": null,
      "position": 1010,
      "depth": 1,
      "listId": "10",
      "statementNotation": "SS.AAS.7.10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate that individuals have a\nresponsibility to be good citizens and community\nmembers; identify the legal definition of a United States\ncitizen and non-citizen.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "4D0A7AA0D4824755A20C3E0BC5CD5A2D": {
      "id": "4D0A7AA0D4824755A20C3E0BC5CD5A2D",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 1,
      "listId": "9",
      "statementNotation": "SS.AAS.7.9",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify the principles and purposes of a\nbudget; identify wants and needs and recognize that\neach has a cost, including food, clothing, shelter,\nentertainment, utilities.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "A438124395B74D55B00B6E414454A96E": {
      "id": "A438124395B74D55B00B6E414454A96E",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "8",
      "statementNotation": "SS.AAS.7.8",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recall that consumers have to make\ndecisions based on resources and scarcity; recognize\nexamples of consumer and marketplace interaction;\nidentifying economic systems, including market,\ncommand, and traditional.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "02A920EB5DF94B9898467F5D5F045273": {
      "id": "02A920EB5DF94B9898467F5D5F045273",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "7",
      "statementNotation": "SS.AAS.7.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Define goods and services; identify the basic\nmodern economic system based around currency;\nidentify goods and services and ways consumers get\naccess to them.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "54E9671505FC4D3E9F40F19087608028": {
      "id": "54E9671505FC4D3E9F40F19087608028",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "6",
      "statementNotation": "SS.AAS.7.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify the basic rights under the Bill of\nRights; recognize how government protects individual\nrights; recognize that citizens have a responsibility to\nfollow laws and that there are consequences for\nbreaking laws.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "2826C2C745BE49BA83C8B05F3A6E2003": {
      "id": "2826C2C745BE49BA83C8B05F3A6E2003",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "5",
      "statementNotation": "SS.AAS.7.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Define the three branches of government;\nrecognize the function of each branch of government as\nmaking laws, enforcing laws, or reviewing laws;\nidentifying concepts of separation of powers and checks\nand balances.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "EAA61B77F23A4E659CA81D4BB8AF9EB2": {
      "id": "EAA61B77F23A4E659CA81D4BB8AF9EB2",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "4",
      "statementNotation": "SS.AAS.7.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize that there are different levels of\ngovernment, including local and state government.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
    "58D279C57EB247189E97CA37F11A41E7": {
