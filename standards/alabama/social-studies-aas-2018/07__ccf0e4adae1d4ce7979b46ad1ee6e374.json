      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "3",
      "statementNotation": "SS.AAS.7.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe the basic ideals of American\ndemocracy, including natural rights, basic freedoms, and\ndemocratic representation; identify characteristics of\nother government systems including, monarchy,\ndictatorship, and democracy.",
      "comments": [],
      "ancestorIds": [
        "15D927B76CF64DF58698695991EC7BE0"
      ],
      "parentId": "15D927B76CF64DF58698695991EC7BE0"
    },
