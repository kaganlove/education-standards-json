      "position": 1003,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Quantitative reasoning includes\nand mathematical modeling requires attention to units of\nmeasurement.",
