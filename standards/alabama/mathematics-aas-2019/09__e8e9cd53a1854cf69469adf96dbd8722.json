    "51E601A5B65D4142851997A336647FD0": {
      "id": "51E601A5B65D4142851997A336647FD0",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 2,
      "listId": "5",
      "statementNotation": "M.G.AAS.9.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Graphs can be used to obtain exact or approximate solutions of\nequations, inequalities, and systems of equations and inequalities—including systems of linear\nequations in two variables and\nsystems of linear and quadratic\nequations (given or obtained by\nusing technology).",
      "comments": [],
