    "32AC291D89FD4BF3861231DD503C4B93": {
      "id": "32AC291D89FD4BF3861231DD503C4B93",
      "asnIdentifier": null,
      "position": 1002,
      "depth": 2,
      "listId": "1",
      "statementNotation": "M.A.AAS.11.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Determine the value of a\nquantity that is squared or\ncubed. (Limited to perfect\nsquares and perfect cubes). ",
      "comments": [],
