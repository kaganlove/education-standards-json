    },
    "D7B4F7C7DF13460CACB458A9F624829E": {
      "id": "D7B4F7C7DF13460CACB458A9F624829E",
      "asnIdentifier": null,
      "position": 1001,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Together, irrational numbers\nand rational numbers complete\nthe real number system,\nrepresenting all points on the\nnumber line, while there exist\nnumbers beyond the real\nnumbers called complex\nnumbers.",
      "comments": [],
      "ancestorIds": [
        "7A7C4F5D33B448E59F1564E8FF54DC0F"
      ],
      "parentId": "7A7C4F5D33B448E59F1564E8FF54DC0F"
