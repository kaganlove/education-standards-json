      "ancestorIds": [
        "8EAD6D575B0E4299B324025E0CD39ADA",
        "506B23F3C1784E029AE41465FA2E3F6B"
      ],
      "parentId": "8EAD6D575B0E4299B324025E0CD39ADA"
    },
    "8EAD6D575B0E4299B324025E0CD39ADA": {
      "id": "8EAD6D575B0E4299B324025E0CD39ADA",
      "asnIdentifier": null,
      "position": 1018,
      "depth": 1,
      "listId": "f",
      "statementNotation": "H.WI.f",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: editing and revising full texts to clarify intent and meaning, making\njudgments about completeness, accuracy, and significance of text/visual/auditory information,\nvalidity and format of sources cited, overall cohesion, and impact of style, tone, and voice",
      "comments": [],
      "ancestorIds": [
        "506B23F3C1784E029AE41465FA2E3F6B"
      ],
      "parentId": "506B23F3C1784E029AE41465FA2E3F6B"
    },
    "B6DE633592BB4D6EB4C82BCCCBC52777": {
