    "C1D223ED52D743C394E5D84C9FC3E487": {
      "id": "C1D223ED52D743C394E5D84C9FC3E487",
      "asnIdentifier": null,
      "position": 1020,
      "depth": 2,
      "listId": "2",
      "statementNotation": "1112.WI.f2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Strengthen\nwriting by revising and\nediting. ",
      "comments": [],
      "ancestorIds": [
        "8EAD6D575B0E4299B324025E0CD39ADA",
        "506B23F3C1784E029AE41465FA2E3F6B"
      ],
      "parentId": "8EAD6D575B0E4299B324025E0CD39ADA"
    },
    "62435993E2C94E5689B22F4288E936AB": {
