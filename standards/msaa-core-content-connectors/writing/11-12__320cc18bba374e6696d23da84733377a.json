      "parentId": "DA709890A7C5419DB8E8651B31978AB6"
    },
    "DA709890A7C5419DB8E8651B31978AB6": {
      "id": "DA709890A7C5419DB8E8651B31978AB6",
      "asnIdentifier": null,
      "position": 1016,
      "depth": 1,
      "listId": "e",
      "statementNotation": "H.WI.e",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: drawing a conclusion, and articulating implications or stating the\nsignificance of the topic by synthesizing information that moves beyond a single source and\nflows from ideas presented",
      "comments": [],
      "ancestorIds": [
        "506B23F3C1784E029AE41465FA2E3F6B"
      ],
      "parentId": "506B23F3C1784E029AE41465FA2E3F6B"
    },
    "8BD1EC2EACAA4FF7A0C19AC8A7D928E3": {
