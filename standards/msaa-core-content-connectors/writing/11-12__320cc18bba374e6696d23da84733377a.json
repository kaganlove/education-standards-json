      "asnIdentifier": null,
      "position": 1021,
      "depth": 2,
      "listId": "3",
      "statementNotation": "1112.WI.f3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Report on a topic\nusing a logical sequence of\nideas, appropriate facts, and\nrelevant, descriptive details\nwhich support the main ideas.",
