      "asnIdentifier": null,
      "position": 1023,
      "depth": 1,
      "listId": "a",
      "statementNotation": "H.WL.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: employing advanced strategies (e.g., writing log, mentor texts, peer\nconferencing, researching author styles and genre structures and features) to develop images,\ncharacters, plot/subplots, central message/theme, or discourse style",
      "comments": [],
      "ancestorIds": [
        "5AF2A11C03E54F809A6C6C3711710E6D"
      ],
      "parentId": "5AF2A11C03E54F809A6C6C3711710E6D"
    },
