      "asnIdentifier": null,
      "position": 1024,
      "depth": 2,
      "listId": "1",
      "statementNotation": "1112.WL.a1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop a plan\nfor writing (e.g., choose a\ntopic, introduce story\nelements, develop storyline,\nconclude story) focused on a\nspecific purpose and\naudience.",
      "comments": [],
      "ancestorIds": [
        "7A32D6E5CAAD40BD81889C50B240A56B",
        "5AF2A11C03E54F809A6C6C3711710E6D"
      ],
      "parentId": "7A32D6E5CAAD40BD81889C50B240A56B"
    },
    "7A32D6E5CAAD40BD81889C50B240A56B": {
