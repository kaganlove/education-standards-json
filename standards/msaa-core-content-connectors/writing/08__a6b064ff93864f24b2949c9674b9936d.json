      "asnIdentifier": null,
      "position": 1066,
      "depth": 2,
      "listId": "1",
      "statementNotation": "8.WP.o1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Produce a clear and\ncoherent permanent product\nthat is appropriate to the\nspecific task, purpose, (e.g.,\nto persuade), and audience.  ",
      "comments": [],
      "ancestorIds": [
        "10D6D9907B274621AB54667C2425FE53",
        "C01BBE718C4A47DE8A47CB5D07601E11"
