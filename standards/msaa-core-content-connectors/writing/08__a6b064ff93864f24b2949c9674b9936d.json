      "asnIdentifier": null,
      "position": 1061,
      "depth": 1,
      "listId": "m",
      "statementNotation": "M.WP.m",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: utilizing emotive, precise, or technical language, transitional\ndevices, and rhetorical questions for effect, while maintaining an authoritative stance and\nconsistent discourse style and voice",
      "comments": [],
      "ancestorIds": [
        "C01BBE718C4A47DE8A47CB5D07601E11"
