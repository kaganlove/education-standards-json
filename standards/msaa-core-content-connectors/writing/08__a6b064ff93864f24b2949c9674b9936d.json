      "asnIdentifier": null,
      "position": 1062,
      "depth": 2,
      "listId": "1",
      "statementNotation": "8.WP.m1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Maintain a\nconsistent style and voice\nthroughout writing (e.g., third\nperson for formal style,\naccurate and efficient word\nchoice, sentence fluency, and\nvoice should be active\nversus passive).  ",
      "comments": [],
      "ancestorIds": [
        "2D991279DC064D5F88F0F2BA98760F55",
        "C01BBE718C4A47DE8A47CB5D07601E11"
