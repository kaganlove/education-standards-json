    "10D6D9907B274621AB54667C2425FE53": {
      "id": "10D6D9907B274621AB54667C2425FE53",
      "asnIdentifier": null,
      "position": 1065,
      "depth": 1,
      "listId": "o",
      "statementNotation": "M.WP.o",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: applying editing (cohesion of subject-verb, pronoun use, verb tense,\nand impact of word choice and sentence variety/complexity) and revision strategies to full texts\nthat clarify intent and meaning; making judgments about completeness and accuracy of\ninformation/visual/auditory components, validity of sources cited, discourse style, and approach\nto addressing audience needs (e.g., emotion, interest, moral authority, potential objections) ",
      "comments": [],
      "ancestorIds": [
        "C01BBE718C4A47DE8A47CB5D07601E11"
      ],
      "parentId": "C01BBE718C4A47DE8A47CB5D07601E11"
    },
    "DF3EE18B3F8341A2BED122B67F3D27CF": {
      "id": "DF3EE18B3F8341A2BED122B67F3D27CF",
      "asnIdentifier": null,
      "position": 1064,
      "depth": 2,
      "listId": "1",
      "statementNotation": "8.WP.n1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Provide a\nconcluding statement or\nsection that supports and\nsummarizes the argument\npresented.",
      "comments": [],
      "ancestorIds": [
        "8A755B086010416C9C447214DD68BB99",
        "C01BBE718C4A47DE8A47CB5D07601E11"
      ],
      "parentId": "8A755B086010416C9C447214DD68BB99"
    },
    "8A755B086010416C9C447214DD68BB99": {
      "id": "8A755B086010416C9C447214DD68BB99",
      "asnIdentifier": null,
      "position": 1063,
      "depth": 1,
      "listId": "n",
      "statementNotation": "M.WP.n",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: drawing and stating conclusions by synthesizing information,\nsummarizing key points of reasoning chain that link back to focus/thesis, and reflecting a\nresponse to the opposition",
      "comments": [],
      "ancestorIds": [
        "C01BBE718C4A47DE8A47CB5D07601E11"
      ],
      "parentId": "C01BBE718C4A47DE8A47CB5D07601E11"
    },
    "4B32CF0BBE0D49089529F552BC81E7F6": {
