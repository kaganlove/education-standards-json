      "listId": "h",
      "statementNotation": "M.WP.h",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: applying editing (subject-verb, pronoun use, verb tense, transitions,\nsentence variety, etc.) and revision strategies to full texts that clarify intent and meaning. Making\njudgments about accuracy and relevance of evidence, cohesion of text/visual/auditory\ncomponents, and approach to addressing audience needs (e.g., emotion, interest, sense of humor,\npotential objections)",
      "comments": [],
      "ancestorIds": [
        "D507B917BD804D2CB821F7DF2629CF1E"
      ],
      "parentId": "D507B917BD804D2CB821F7DF2629CF1E"
    },
