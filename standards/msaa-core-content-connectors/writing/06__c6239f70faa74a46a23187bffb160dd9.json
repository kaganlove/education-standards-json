      "id": "F1A1BBE9BEDA4DD996A08129F193C957",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 1,
      "listId": "e",
      "statementNotation": "M.WI.e",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Progress Indicator: maintaining a (formal) style and text structure(s) of longer writing\npieces appropriate to purpose and genre, including use of transitional words and phrases to\nconnect ideas",
      "comments": [],
      "ancestorIds": [
        "F100E5EC248A42BFB3598E2A745C0AAE"
      ],
      "parentId": "F100E5EC248A42BFB3598E2A745C0AAE"
    },
