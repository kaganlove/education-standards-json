      "asnIdentifier": null,
      "position": 1013,
      "depth": 2,
      "listId": "2",
      "statementNotation": "6.WI.d2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use precise\nlanguage and domain-specific\nvocabulary to inform about or\nexplain the topic.",
      "comments": [],
      "ancestorIds": [
        "72FF890D9D4A4928955EA4EF0B26DC59",
        "F100E5EC248A42BFB3598E2A745C0AAE"
      ],
      "parentId": "72FF890D9D4A4928955EA4EF0B26DC59"
    },
