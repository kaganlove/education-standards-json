      "depth": 2,
      "listId": "1",
      "statementNotation": "6.WI.d1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop the topic\n(add additional information\nrelated to the topic) with\nrelevant facts, definitions,\nconcrete details, quotations,\nor other information and\nexamples.",
