      "position": 1006,
      "depth": 2,
      "listId": "d",
      "statementNotation": "2.1d",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Activities available for people living in urban, suburban, and rural communities are different. The type of community a person grows up in\nwill affect a person’s development and identity.",
      "comments": [],
      "ancestorIds": [
        "0CA2DBEF99474370B98D93A4296EEDE2",
        "C2B99EE18E6B48CEAA2DFD42BF333B57"
      ],
      "parentId": "0CA2DBEF99474370B98D93A4296EEDE2"
    },
