      "depth": 2,
      "listId": "a",
      "statementNotation": "2.1a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "An urban community, or city, is characterized by dense population and land occupied primarily by buildings and structures that are used for\nresidential and business purposes.",
      "comments": [],
      "ancestorIds": [
        "0CA2DBEF99474370B98D93A4296EEDE2",
