      "asnIdentifier": null,
      "position": 54000,
      "depth": 4,
      "listId": "1",
      "statementNotation": "3.10a.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will use an understanding of economics to identify the goods and services produced by DoDEA-connected world communities. ",
      "comments": [],
      "ancestorIds": [
