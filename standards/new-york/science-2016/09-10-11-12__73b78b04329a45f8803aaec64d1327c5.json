    "FF88AE12E85E48A7AE598DECDDB1314F": {
      "id": "FF88AE12E85E48A7AE598DECDDB1314F",
      "asnIdentifier": null,
      "position": 23000,
      "depth": 3,
      "listId": "a",
      "statementNotation": "HS.ED.DCI.ETS1.C.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Criteria may need to be broken down into simpler ones that can be approached systematically, and decisions about the priority of certain criteria over others (tradeoffs) may be needed. (HS-ETS1-2)",
      "comments": [],
      "ancestorIds": [
        "52EB9A8E87074836BC9B6055EA3D93D4",
        "01DB9B144ED64000AD385F6F843FED61",
