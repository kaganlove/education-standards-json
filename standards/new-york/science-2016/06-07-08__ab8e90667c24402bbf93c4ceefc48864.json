      "statementNotation": "MS.ME.DCI.LS2.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "LS2.C: Ecosystem Dynamics, Functioning, and Resilience",
      "comments": [],
      "ancestorIds": [
