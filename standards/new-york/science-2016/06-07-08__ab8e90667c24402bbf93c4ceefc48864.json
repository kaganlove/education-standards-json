    "D1F0424F27E74349A45469706A051D42": {
      "id": "D1F0424F27E74349A45469706A051D42",
      "asnIdentifier": null,
      "position": 24000,
      "depth": 3,
      "listId": "b",
      "statementNotation": "MS.ME.DCI.LS1.C.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Within individual organisms, food moves through a series of chemical reactions in which it is broken down and rearranged to form new molecules, to support growth, or to release energy. (MS-LS1-7)",
      "comments": [],
      "ancestorIds": [
        "BD9F3DD8CF53458099195D30C3322B78",
        "E16F61C015C041E78B495E6F8E1301D4",
        "E4593ACBB65543ABB6ED933BF176D664"
      ],
