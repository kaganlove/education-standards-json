    },
    "CB32C9912996481CBA56E7E3AFB229C7": {
      "id": "CB32C9912996481CBA56E7E3AFB229C7",
      "asnIdentifier": null,
      "position": 26000,
      "depth": 3,
      "listId": "a",
      "statementNotation": "MS.ME.DCI.LS2.A.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Organisms, and populations of organisms, are dependent on their environmental interactions both with other living things and with nonliving factors. (MS-LS2-1)",
