      "statementNotation": "MS.ME.DCI.LS2.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "LS2.B: Cycle of Matter and Energy Transfer in Ecosystems",
      "comments": [],
      "ancestorIds": [
        "E16F61C015C041E78B495E6F8E1301D4",
        "E4593ACBB65543ABB6ED933BF176D664"
