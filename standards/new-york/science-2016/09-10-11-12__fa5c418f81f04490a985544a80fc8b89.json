    },
    "2900FBBCF363465F98DA9D39709725B2": {
      "id": "2900FBBCF363465F98DA9D39709725B2",
      "asnIdentifier": null,
      "position": 23000,
      "depth": 3,
      "listId": "a",
      "statementNotation": "HS.IRE.DCI.LS2.A.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Ecosystems have carrying capacities, which are limits to the numbers of organisms and populations they can support. Organisms would have the capacity to produce populations of great size were it not for the fact that environments and resources are finite. This fundamental tension affects the abundance (number of individuals) of species in any given ecosystem. (HSLS2-1),(HS-LS2-2)",
      "comments": [],
      "ancestorIds": [
        "BB40C53B7A9D4C47928FB0988386E1E0",
        "644F949EFB9C45B287BA7299FEB68DB1",
        "D3BED92405C44CBFA7EC43F84195502D"
      ],
      "parentId": "BB40C53B7A9D4C47928FB0988386E1E0"
    },
