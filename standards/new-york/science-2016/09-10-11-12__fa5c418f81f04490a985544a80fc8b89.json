    "A2029593DEB6426890D70524339658D3": {
      "id": "A2029593DEB6426890D70524339658D3",
      "asnIdentifier": null,
      "position": 24000,
      "depth": 3,
      "listId": "b",
      "statementNotation": "HS.IRE.DCI.LS2.A.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "(NYSED) Carrying capacity results from the availability of biotic and abiotic factors and from challenges such as predation, competition, and disease. (HS-LS2-1),(HSLS2-2)",
      "comments": [],
      "ancestorIds": [
        "BB40C53B7A9D4C47928FB0988386E1E0",
        "644F949EFB9C45B287BA7299FEB68DB1",
        "D3BED92405C44CBFA7EC43F84195502D"
