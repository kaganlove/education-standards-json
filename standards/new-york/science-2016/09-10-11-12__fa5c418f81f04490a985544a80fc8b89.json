    "C2666AF023FD4F50995E0B835CF9C3D1": {
      "id": "C2666AF023FD4F50995E0B835CF9C3D1",
      "asnIdentifier": null,
      "position": 32000,
      "depth": 3,
      "listId": "b",
      "statementNotation": "HS.IRE.DCI.LS4.D.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Humans depend on the living world for the resources and other benefits provided by biodiversity. But human activity is also having adverse impacts on biodiversity through overpopulation, overexploitation, habitat destruction, pollution, introduction of invasive species, and climate change. Thus sustaining biodiversity so that ecosystem functioning and productivity are maintained is essential to supporting and enhancing life on Earth. Sustaining biodiversity also aids humanity by preserving landscapes of recreational or inspirational value. (secondary to HS-LS2-7)",
      "comments": [],
      "ancestorIds": [
        "27D9B1F1116D44CD8DDA0240382E2DD6",
        "644F949EFB9C45B287BA7299FEB68DB1",
        "D3BED92405C44CBFA7EC43F84195502D"
      ],
      "parentId": "27D9B1F1116D44CD8DDA0240382E2DD6"
    },
    "4B7745B90575486398A5C7A60DA1FACE": {
