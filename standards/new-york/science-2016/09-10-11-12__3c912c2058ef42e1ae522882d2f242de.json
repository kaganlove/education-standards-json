    "865EF8060FD94078A1F5F80461DE0A6A": {
      "id": "865EF8060FD94078A1F5F80461DE0A6A",
      "asnIdentifier": null,
      "position": 28000,
      "depth": 3,
      "listId": "a",
      "statementNotation": "HS.ES.DCI.ESS2.C.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The abundance of liquid water on Earth’s surface and its unique combination of physical and chemical properties are central to the planet’s dynamics. These properties include water’s exceptional capacity to absorb, store, and release large amounts of energy, transmit sunlight, expand upon freezing, dissolve and transport materials, and lower the viscosities and melting points of rocks. (HS-ESS2-5)",
      "comments": [],
      "ancestorIds": [
        "5F89240531C144BF9AEC784815F7C146",
        "BBFCC01929AF41A38BD41BD4ABCBCB96",
        "C055EDA8A3FE4F79BCFD351AB57FD0F8"
      ],
      "parentId": "5F89240531C144BF9AEC784815F7C146"
    },
    "5F89240531C144BF9AEC784815F7C146": {
