      "listId": "3",
      "statementNotation": "MS.WC.DCI.ESS3.D",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "ESS3.D: Global Climate Change",
      "comments": [],
      "ancestorIds": [
        "4C9D05C79B0E4E6C9994696188A8129F",
        "388BFAE3117A4B7F9BFFEA2B8983AF75"
