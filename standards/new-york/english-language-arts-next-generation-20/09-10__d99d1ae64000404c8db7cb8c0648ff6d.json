      "asnIdentifier": null,
      "position": 1051,
      "depth": 2,
      "listId": "5",
      "statementNotation": "9-10SL5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make strategic use of digital media and/or visual displays in presentations to enhance\nunderstanding of findings, reasoning, and evidence, and to add elements of interest to engage\nthe audience.",
      "comments": [],
      "ancestorIds": [
        "D9DB61B98ED44F3EB2320BDBD5000E76",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
