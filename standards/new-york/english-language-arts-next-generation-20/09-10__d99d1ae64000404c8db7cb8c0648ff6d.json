      "asnIdentifier": null,
      "position": 1063,
      "depth": 2,
      "listId": "5",
      "statementNotation": "9-10L5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate understanding of figurative language, word relationships, and nuances in\nword meanings.",
      "comments": [],
      "ancestorIds": [
        "C5D3DFC45C8F4F05AC28E7BC33C21BE4",
        "68786B20B7094B8DAA4F6D4C00D7CE18"
