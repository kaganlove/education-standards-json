    "47AB8E98A9FA48E3BDC0746E024DD5E5": {
      "id": "47AB8E98A9FA48E3BDC0746E024DD5E5",
      "asnIdentifier": null,
      "position": 1059,
      "depth": 3,
      "listId": "a",
      "statementNotation": "9-10L4a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use context (e.g., the overall meaning of a sentence, paragraph, or text; a\nword’s position or function in a sentence) as a clue to the meaning of a word or phrase.",
      "comments": [],
      "ancestorIds": [
        "C8B7014EB9B043C0B24C6D57EE65B9E0",
        "C5D3DFC45C8F4F05AC28E7BC33C21BE4",
        "68786B20B7094B8DAA4F6D4C00D7CE18"
