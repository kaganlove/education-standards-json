    "820AEF2BF3E74C72BD4A12492B9E7075": {
      "id": "820AEF2BF3E74C72BD4A12492B9E7075",
      "asnIdentifier": null,
      "position": 1039,
      "depth": 2,
      "listId": "7",
      "statementNotation": "9-10W7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Gather relevant information from multiple sources, using advanced searches\neffectively; assess the usefulness of each source in answering the research question; integrate\ninformation into the text selectively to maintain the flow of ideas; avoid plagiarism and follow a\nstandard format for citation. ",
      "comments": [],
      "ancestorIds": [
        "43F8B72963CE4BCAA8B777728D7DBA3F",
        "343DE79FECCB4FA0991F4A8F59419845"
      ],
      "parentId": "43F8B72963CE4BCAA8B777728D7DBA3F"
    },
    "44D98B40E52A4930BEFF7BC4C834C4F3": {
      "id": "44D98B40E52A4930BEFF7BC4C834C4F3",
      "asnIdentifier": null,
      "position": 1038,
      "depth": 2,
      "listId": "6",
      "statementNotation": "9-10W6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Conduct research to answer questions, including self-generated questions, or solve a\nproblem; narrow or broaden the inquiry when appropriate. Synthesize multiple sources,\ndemonstrating understanding of the subject under investigation.",
      "comments": [],
      "ancestorIds": [
        "43F8B72963CE4BCAA8B777728D7DBA3F",
        "343DE79FECCB4FA0991F4A8F59419845"
      ],
      "parentId": "43F8B72963CE4BCAA8B777728D7DBA3F"
