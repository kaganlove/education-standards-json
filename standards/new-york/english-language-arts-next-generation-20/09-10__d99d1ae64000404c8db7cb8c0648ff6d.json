      "asnIdentifier": null,
      "position": 1050,
      "depth": 2,
      "listId": "4",
      "statementNotation": "9-10SL4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Present claims, findings, and supporting evidence clearly, concisely, and logically;\norganization, development, substance, and style are appropriate to task, purpose, and\naudience.",
      "comments": [],
      "ancestorIds": [
        "D9DB61B98ED44F3EB2320BDBD5000E76",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
