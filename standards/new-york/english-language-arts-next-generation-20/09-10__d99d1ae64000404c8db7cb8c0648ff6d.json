    "08010C0ACDAF4198B41188AFD69DD7A6": {
      "id": "08010C0ACDAF4198B41188AFD69DD7A6",
      "asnIdentifier": null,
      "position": 1048,
      "depth": 2,
      "listId": "3",
      "statementNotation": "9-10SL3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Evaluate a speaker’s point of view, reasoning, and use of evidence and rhetoric;\nidentify any fallacious reasoning or exaggerated or distorted evidence.",
      "comments": [],
      "ancestorIds": [
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "9A8015ACB44A4E178D4F1E2CF1849CDD"
    },
    "D15F7D00F124449E80D019F3FDC2D761": {
      "id": "D15F7D00F124449E80D019F3FDC2D761",
      "asnIdentifier": null,
      "position": 1047,
      "depth": 2,
      "listId": "2",
      "statementNotation": "9-10SL2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Integrate multiple sources of information presented in diverse formats (e.g., including\nvisual, quantitative, and oral), evaluating the credibility, accuracy, and relevance of each source.",
      "comments": [],
      "ancestorIds": [
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "9A8015ACB44A4E178D4F1E2CF1849CDD"
    },
    "339DACDB3FBD4A08932CC261E49C38BD": {
      "id": "339DACDB3FBD4A08932CC261E49C38BD",
      "asnIdentifier": null,
      "position": 1046,
      "depth": 3,
      "listId": "d",
      "statementNotation": "9-10SL1d",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Respond thoughtfully to diverse perspectives, summarize points of\nagreement and disagreement, and, when warranted, qualify or justify personal views\nand understanding and make new connections in light of the evidence and reasoning\npresented.",
      "comments": [],
      "ancestorIds": [
        "CD39C3C0A4A045AEA89CB588E837A366",
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "CD39C3C0A4A045AEA89CB588E837A366"
    },
    "88C86B7F3D694765BCD0260D69D86956": {
      "id": "88C86B7F3D694765BCD0260D69D86956",
      "asnIdentifier": null,
      "position": 1045,
      "depth": 3,
      "listId": "c",
      "statementNotation": "9-10SL1c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Pose and respond to questions that relate the discussion to broader themes\nor ideas; actively incorporate others into the discussion; and clarify, verify, or challenge\nideas and conclusions.",
      "comments": [],
      "ancestorIds": [
        "CD39C3C0A4A045AEA89CB588E837A366",
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "CD39C3C0A4A045AEA89CB588E837A366"
    },
    "B3DAE9BD9C154335BC82C6F2793E9B1C": {
      "id": "B3DAE9BD9C154335BC82C6F2793E9B1C",
      "asnIdentifier": null,
      "position": 1044,
      "depth": 3,
      "listId": "b",
      "statementNotation": "9-10SL1b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Work with peers to set norms for collegial discussions and decision-making,\nestablish clear goals, deadlines, and individual roles as needed.",
      "comments": [],
      "ancestorIds": [
        "CD39C3C0A4A045AEA89CB588E837A366",
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "CD39C3C0A4A045AEA89CB588E837A366"
    },
    "E2AD3D788CDE46BA811B9D45D6A655B7": {
      "id": "E2AD3D788CDE46BA811B9D45D6A655B7",
      "asnIdentifier": null,
      "position": 1043,
      "depth": 3,
      "listId": "a",
      "statementNotation": "9-10SL1a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Come to discussions prepared, having read and researched material under\nstudy; draw on that preparation by referring to evidence to stimulate a thoughtful, wellreasoned exchange of ideas.",
      "comments": [],
      "ancestorIds": [
        "CD39C3C0A4A045AEA89CB588E837A366",
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "CD39C3C0A4A045AEA89CB588E837A366"
    },
    "CD39C3C0A4A045AEA89CB588E837A366": {
      "id": "CD39C3C0A4A045AEA89CB588E837A366",
      "asnIdentifier": null,
      "position": 1042,
      "depth": 2,
      "listId": "1",
      "statementNotation": "9-10SL1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Initiate and participate effectively in a range of collaborative discussions with diverse\npartners on complex topics, texts, and issues; express ideas clearly and persuasively, and build\non those of others.",
      "comments": [],
      "ancestorIds": [
        "9A8015ACB44A4E178D4F1E2CF1849CDD",
        "EAA3A827C6114E9CAFD7C5DEBD2D0423"
      ],
      "parentId": "9A8015ACB44A4E178D4F1E2CF1849CDD"
    },
