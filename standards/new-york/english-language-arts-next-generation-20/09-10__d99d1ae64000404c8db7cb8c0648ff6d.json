      "asnIdentifier": null,
      "position": 1062,
      "depth": 3,
      "listId": "d",
      "statementNotation": "9-10L4d",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Verify the preliminary determination of the meaning of a word or phrase (e.g.,\nby checking the inferred meaning in context or in a dictionary).",
      "comments": [],
      "ancestorIds": [
        "C8B7014EB9B043C0B24C6D57EE65B9E0",
        "C5D3DFC45C8F4F05AC28E7BC33C21BE4",
