      "position": 12000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "12.A3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Using Learning and Thinking Strategies",
      "comments": [],
      "ancestorIds": [
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "9E1687672F8B4740B7A0048E3DAECF08"
