      "asnIdentifier": null,
      "position": 36000,
      "depth": 2,
      "listId": "b",
      "statementNotation": "12.B3.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "describe how such factors as personal goals, priorities, and needs affect their use of money as a resource;",
      "comments": [],
      "ancestorIds": [
        "C2371E56328C4297ABD54B2D0F115BB8",
        "F0E8DB5588BE49D4A838B76A460946D4"
