      "comments": [],
      "ancestorIds": [
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "9E1687672F8B4740B7A0048E3DAECF08"
    },
    "DCA34B255A0946FB82B1F90B24967B90": {
      "id": "DCA34B255A0946FB82B1F90B24967B90",
      "asnIdentifier": null,
      "position": 17000,
      "depth": 2,
      "listId": "e",
      "statementNotation": "12.A3.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "demonstrate effective use of a problemsolving process to deal with issues in school and in the workplace.",
      "comments": [],
      "ancestorIds": [
        "32A47151F3DB4226957EDD826986C8B3",
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "32A47151F3DB4226957EDD826986C8B3"
    },
    "17910F34D0FC4B8DB2A305960D641002": {
      "id": "17910F34D0FC4B8DB2A305960D641002",
      "asnIdentifier": null,
      "position": 16000,
      "depth": 2,
      "listId": "d",
      "statementNotation": "12.A3.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "use a decision-making process to make informed choices (e.g., to select opportunities for learning, to prioritize workplace tasks) in the school, community, or workplace, and analyse the impact of their decisions on themselves and others;",
      "comments": [],
      "ancestorIds": [
        "32A47151F3DB4226957EDD826986C8B3",
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "32A47151F3DB4226957EDD826986C8B3"
    },
