      "id": "679C675958314CB8AC35761FEAD1BD46",
      "asnIdentifier": null,
      "position": 31000,
      "depth": 2,
      "listId": "c",
      "statementNotation": "12.B2.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify strategies that contribute to effective teamwork (e.g., presuming positive intentions, managing conflict, paying attention to self and others, seeking diverse strengths and perspectives), and use them to work collaboratively to accomplish team goals in real or simulated workplace settings;",
      "comments": [],
      "ancestorIds": [
        "08247FCA213345CE9ABD32FD5574B3DA",
        "F0E8DB5588BE49D4A838B76A460946D4"
      ],
      "parentId": "08247FCA213345CE9ABD32FD5574B3DA"
    },
