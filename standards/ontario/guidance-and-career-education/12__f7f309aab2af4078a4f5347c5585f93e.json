      "position": 19000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "12.A4.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "assess their application of selected workplace essential skills, using feedback from others (e.g., peers, teachers, supervisors);",
      "comments": [],
      "ancestorIds": [
        "642389989E2E4F19A9EAFF2117534514",
        "9E1687672F8B4740B7A0048E3DAECF08"
