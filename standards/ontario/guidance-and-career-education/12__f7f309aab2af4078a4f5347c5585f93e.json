      "id": "DFD2F18ED5214483AA11E94B7CBD3A51",
      "asnIdentifier": null,
      "position": 32000,
      "depth": 2,
      "listId": "d",
      "statementNotation": "12.B2.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify a specific need in the community and respond to this need, working as part of a team (e.g., organize a career fair for parents and teachers);",
      "comments": [],
      "ancestorIds": [
        "08247FCA213345CE9ABD32FD5574B3DA",
        "F0E8DB5588BE49D4A838B76A460946D4"
      ],
      "parentId": "08247FCA213345CE9ABD32FD5574B3DA"
    },
