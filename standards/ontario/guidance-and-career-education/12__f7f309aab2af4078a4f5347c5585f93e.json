      "depth": 2,
      "listId": "a",
      "statementNotation": "12.C1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "use an inquiry process (i.e., formulate questions; gather, organize, interpret, assess, and communicate information) to locate information about postsecondary learning and work options (e.g., on-the-job training, apprenticeship, college, university, fields of work, specific occupations);",
      "comments": [],
      "ancestorIds": [
        "B6E4785C8ED146D6AB51E194C6FC75E4",
        "89C2CE02565043E39F120078DFAAB7F9"
