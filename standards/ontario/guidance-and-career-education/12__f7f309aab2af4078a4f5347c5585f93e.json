      "position": 21000,
      "depth": 2,
      "listId": "c",
      "statementNotation": "12.A4.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "produce a plan for the development of the workplace essential skills that they require to complete specific tasks in the community or workplace;",
      "comments": [],
      "ancestorIds": [
        "642389989E2E4F19A9EAFF2117534514",
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "642389989E2E4F19A9EAFF2117534514"
    },
