      "depth": 2,
      "listId": "a",
      "statementNotation": "12.B3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify and use time-management skills (e.g., prioritizing, allocating adequate time, limiting interruptions) and timemanagement tools (e.g., personal planners, schedules,\"to-do\" lists) to complete tasks and projects in real or simulated workplace settings;",
      "comments": [],
      "ancestorIds": [
        "C2371E56328C4297ABD54B2D0F115BB8",
        "F0E8DB5588BE49D4A838B76A460946D4"
