      "depth": 2,
      "listId": "c",
      "statementNotation": "12.B3.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "demonstrate an understanding of the money-management strategies (e.g., budgeting, using credit, investing) required to handle their short- and long-term expenses.",
      "comments": [],
      "ancestorIds": [
        "C2371E56328C4297ABD54B2D0F115BB8",
        "F0E8DB5588BE49D4A838B76A460946D4"
