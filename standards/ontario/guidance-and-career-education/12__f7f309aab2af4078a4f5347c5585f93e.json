      "asnIdentifier": null,
      "position": 20000,
      "depth": 2,
      "listId": "b",
      "statementNotation": "12.A4.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "produce a gap analysis of their workplace essential skills by identifying the workplace essential skill requirements for a specific occupation, comparing the requirements with the skills they have, and identifying the skills they need to develop;",
      "comments": [],
      "ancestorIds": [
        "642389989E2E4F19A9EAFF2117534514",
        "9E1687672F8B4740B7A0048E3DAECF08"
