      "id": "77F81A1E36A94DA7A37DB22B96930E8D",
      "asnIdentifier": null,
      "position": 13000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "12.A3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "demonstrate the ability to use a process to reflect on their home, school, or work experiences (that is, describe their experience, assess their performance, set goals for future performance, identify strategies for achieving them, apply the learning in a new situation), and explain how this reflection process contributes to continuous learning;",
      "comments": [],
      "ancestorIds": [
        "32A47151F3DB4226957EDD826986C8B3",
        "9E1687672F8B4740B7A0048E3DAECF08"
      ],
      "parentId": "32A47151F3DB4226957EDD826986C8B3"
    },
