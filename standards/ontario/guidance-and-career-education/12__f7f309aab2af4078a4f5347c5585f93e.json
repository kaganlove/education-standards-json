      "id": "B7BA7A52C8954024BBD27824921DECEA",
      "asnIdentifier": null,
      "position": 30000,
      "depth": 2,
      "listId": "b",
      "statementNotation": "12.B2.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "describe and use strategies for managing conflict (e.g., negotiation, mediation) and explain the issues and difficulties that may be encountered when resolving conflict;",
      "comments": [],
      "ancestorIds": [
        "08247FCA213345CE9ABD32FD5574B3DA",
        "F0E8DB5588BE49D4A838B76A460946D4"
      ],
      "parentId": "08247FCA213345CE9ABD32FD5574B3DA"
    },
