      "id": "08247FCA213345CE9ABD32FD5574B3DA",
      "asnIdentifier": null,
      "position": 28000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "12.B2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Working With Others",
      "comments": [],
      "ancestorIds": [
        "F0E8DB5588BE49D4A838B76A460946D4"
      ],
      "parentId": "F0E8DB5588BE49D4A838B76A460946D4"
