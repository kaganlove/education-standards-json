      "depth": 2,
      "listId": "e",
      "statementNotation": "12.B2.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify and use appropriate strategies (e.g., clarifying needs) to obtain the resources and support required to meet their needs when communicating with employers and postsecondary education and training providers",
      "comments": [],
      "ancestorIds": [
        "08247FCA213345CE9ABD32FD5574B3DA",
        "F0E8DB5588BE49D4A838B76A460946D4"
      ],
      "parentId": "08247FCA213345CE9ABD32FD5574B3DA"
    },
