      "depth": 2,
      "listId": "a",
      "statementNotation": "10.C1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify harmful effects of computer use on the environment (e.g., resources used and wastes created during production; disposal of old computers in landfill);",
      "comments": [],
      "ancestorIds": [
