      "asnIdentifier": null,
      "position": 50000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "10.D1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Health and Safety: follow appropriate health and safety procedures when assembling, using, and maintaining computer systems;",
      "comments": [],
      "ancestorIds": [
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "53231F922DB640679A0909DEE0482E8A"
