    "ACBDE9E9CD614585A46359F4C1A4A978": {
      "id": "ACBDE9E9CD614585A46359F4C1A4A978",
      "asnIdentifier": null,
      "position": 55000,
      "depth": 2,
      "listId": "b",
      "statementNotation": "10.D2.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "comply with acceptable-use policies (e.g., protection of client information; prohibitions on unsolicited bulk mailings, transfers of copyright material, and posting of demeaning comments; safeguarding of passwords).",
      "comments": [],
      "ancestorIds": [
        "7379634AFAA7424493287147D69337A3",
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "7379634AFAA7424493287147D69337A3"
    },
    "DB7DA6EAFAA342078EA862389C9B3D1A": {
      "id": "DB7DA6EAFAA342078EA862389C9B3D1A",
      "asnIdentifier": null,
      "position": 54000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "10.D2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "demonstrate an understanding of the importance of ethical computer use (e.g., the social cost of hacking, lost and corrupt data, and plagiarism);",
      "comments": [],
      "ancestorIds": [
        "7379634AFAA7424493287147D69337A3",
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "7379634AFAA7424493287147D69337A3"
    },
    "7379634AFAA7424493287147D69337A3": {
      "id": "7379634AFAA7424493287147D69337A3",
      "asnIdentifier": null,
      "position": 53000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "10.D2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Ethics and Security: demonstrate an understanding of ethical and security issues related to the use of computers;",
      "comments": [],
      "ancestorIds": [
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "53231F922DB640679A0909DEE0482E8A"
    },
    "1505F5082BA44139B92DA7A614493E2C": {
      "id": "1505F5082BA44139B92DA7A614493E2C",
      "asnIdentifier": null,
      "position": 52000,
      "depth": 2,
      "listId": "b",
      "statementNotation": "10.D1.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify issues related to Internet safety and personal identity security (e.g., protection of information stored on computers or transmitted over a network, identity theft, cyberstalking, cyberbullying, privacy policies).",
      "comments": [],
      "ancestorIds": [
        "712CFD25CFA04D7F80742DA83669AA9F",
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "712CFD25CFA04D7F80742DA83669AA9F"
    },
    "8C7D8175317F4B29B80BCAA737521C6D": {
      "id": "8C7D8175317F4B29B80BCAA737521C6D",
      "asnIdentifier": null,
      "position": 51000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "10.D1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "use appropriate equipment, procedures, and techniques (e.g., use a wrist support, ensure power is off before opening the case of a computer, use proper lifting techniques when moving heavy equipment) to protect health and ensure safety when working with computers (e.g., to avoid musculoskeletal injuries, eye strain, repetitive strain injuries)",
      "comments": [],
      "ancestorIds": [
        "712CFD25CFA04D7F80742DA83669AA9F",
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "712CFD25CFA04D7F80742DA83669AA9F"
    },
