      "depth": 2,
      "listId": "a",
      "statementNotation": "10.D3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify various careers related to computer technology, and determine which ones can be entered directly from secondary school and which ones require college, university, apprenticeship, or other postsecondary training;",
      "comments": [],
      "ancestorIds": [
        "71C6D9EFEAFB4099A3674D4F90D9EE6B",
        "53231F922DB640679A0909DEE0482E8A"
