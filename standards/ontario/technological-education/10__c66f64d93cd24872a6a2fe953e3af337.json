      "asnIdentifier": null,
      "position": 56000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "10.D3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Career Opportunities: identify various careers related to computer technology, and describe the education and/or training required for them.",
      "comments": [],
      "ancestorIds": [
        "53231F922DB640679A0909DEE0482E8A"
      ],
      "parentId": "53231F922DB640679A0909DEE0482E8A"
