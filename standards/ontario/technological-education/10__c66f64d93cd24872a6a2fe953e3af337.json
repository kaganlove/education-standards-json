      "depth": 1,
      "listId": "2",
      "statementNotation": "10.C2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Technology and Society: identify effects of the widespread use of computers and associated technologies on society.",
      "comments": [],
      "ancestorIds": [
        "E019780694F54073B69E6164CAA66008"
      ],
      "parentId": "E019780694F54073B69E6164CAA66008"
