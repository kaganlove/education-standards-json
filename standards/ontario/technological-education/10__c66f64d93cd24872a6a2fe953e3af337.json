      "depth": 2,
      "listId": "c",
      "statementNotation": "10.D3.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "identify groups and programs that are available to support students who are interested in pursuing non-traditional career choices in computer technology (e.g., mentoring programs, virtual networking/support groups, specialized postsecondary programs, relevant trade/industry associations);",
      "comments": [],
      "ancestorIds": [
        "71C6D9EFEAFB4099A3674D4F90D9EE6B",
        "53231F922DB640679A0909DEE0482E8A"
