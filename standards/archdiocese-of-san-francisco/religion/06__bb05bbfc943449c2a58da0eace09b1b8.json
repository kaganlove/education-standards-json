      "depth": 0,
      "listId": "P2",
      "statementNotation": "P2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Catechism of Catholic Church Pillar #2--The Celebration of the\nChristian Mystery (CCC #1066-1690) ",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
