    "0E823E9CD56042C48D6BE32C728C8CA3": {
      "id": "0E823E9CD56042C48D6BE32C728C8CA3",
      "asnIdentifier": null,
      "position": 16000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "P1.14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "a. How the books of the Bible were assembled into a “canon”",
      "comments": [],
      "ancestorIds": [
        "7BA927A128C44675ADE7060EA1ECAA0C",
        "6468505A2A894C128DC9447C0EA25CB7"
