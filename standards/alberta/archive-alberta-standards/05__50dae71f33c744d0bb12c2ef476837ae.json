      "listId": "1",
      "statementNotation": "2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use Strategies and Cues",
      "comments": [],
      "ancestorIds": [
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "594B26DC4D6C432595E99EF6A7BEDFD1"
