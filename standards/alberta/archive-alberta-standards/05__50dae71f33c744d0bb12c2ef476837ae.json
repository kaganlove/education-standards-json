      "depth": 2,
      "listId": "b",
      "statementNotation": "1.2b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Combine ideas\n\nuse talk, notes, personal writing and representing to explore relationships among own ideas and experiences, those of others and those encountered in oral, print and other media texts",
      "comments": [],
      "ancestorIds": [
        "74F63CFC37944E239AFEC6949272F6E2",
