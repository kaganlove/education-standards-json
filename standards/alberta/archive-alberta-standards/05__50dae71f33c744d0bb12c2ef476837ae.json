      "asnIdentifier": null,
      "position": 8014,
      "depth": 2,
      "listId": "a",
      "statementNotation": "2.3a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand forms and genres\n\nidentify and discuss similarities and differences among a variety of forms of oral, print and other media texts\nidentify the main characteristics of familiar media and media texts",
      "comments": [],
      "ancestorIds": [
        "2D3A5284C7584399B43683116312B0C9",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
