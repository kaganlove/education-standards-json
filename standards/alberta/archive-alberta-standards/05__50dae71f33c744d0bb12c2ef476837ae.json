      "asnIdentifier": null,
      "position": 8013,
      "depth": 1,
      "listId": "3",
      "statementNotation": "2.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand Forms, Elements and Techniques",
      "comments": [],
      "ancestorIds": [
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "594B26DC4D6C432595E99EF6A7BEDFD1"
