    },
    "F3C284222D1740778F3973E35C67A57A": {
      "id": "F3C284222D1740778F3973E35C67A57A",
      "asnIdentifier": null,
      "position": 8015,
      "depth": 2,
      "listId": "b",
      "statementNotation": "2.3b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand techniques and elements  \n\nidentify the main problem or conflict in oral, print and other media texts, and explain how it is resolved\nidentify and discuss the main character's point of view and motivation\nidentify examples of apt word choice and imagery that create particular effects\nidentify sections or elements in print or other media texts, such as shots in films or sections in magazines",
      "comments": [],
      "ancestorIds": [
        "2D3A5284C7584399B43683116312B0C9",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "2D3A5284C7584399B43683116312B0C9"
    },
    "CECA39F1021E4C62A4337249C7F05F18": {
