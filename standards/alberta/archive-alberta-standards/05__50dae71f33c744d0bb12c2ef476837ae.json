    "746432D6EFEE48718A289C57EDA99541": {
      "id": "746432D6EFEE48718A289C57EDA99541",
      "asnIdentifier": null,
      "position": 7013,
      "depth": 2,
      "listId": "c",
      "statementNotation": "2.2c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Appreciate the artistry of texts\n\nexplain how simile and hyperbole are used to create mood and mental images\nalter sentences and word choices to enhance meaning and to create mood and special effects",
      "comments": [],
      "ancestorIds": [
        "DCE7204BBA1847DAB30695FA5107CFF8",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "DCE7204BBA1847DAB30695FA5107CFF8"
    },
    "8D5F113BC65741FF93B02ABED3C233F8": {
      "id": "8D5F113BC65741FF93B02ABED3C233F8",
      "asnIdentifier": null,
      "position": 7012,
      "depth": 2,
      "listId": "b",
      "statementNotation": "2.2b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Construct meaning from texts\n\ncompare characters and situations portrayed in oral, print and other media texts to those encountered in the classroom and community\ndescribe characters' qualities based on what they say and do and how they are described in oral, print and other media texts\ndescribe and discuss the influence of setting on the characters and events\nsupport own interpretations of oral, print and other media texts, using evidence from personal experiences and the texts\nretell or represent stories from the points of view of different characters",
      "comments": [],
      "ancestorIds": [
        "DCE7204BBA1847DAB30695FA5107CFF8",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "DCE7204BBA1847DAB30695FA5107CFF8"
    },
    "775F8322FC584DFF9A9E55558994DB4E": {
      "id": "775F8322FC584DFF9A9E55558994DB4E",
      "asnIdentifier": null,
      "position": 7011,
      "depth": 2,
      "listId": "a",
      "statementNotation": "2.2a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Experience various texts\n\nexperience oral, print and other media texts from a variety of cultural traditions and genres, such as historical fiction, myths, biographies, poetry, news reports and guest speakers\nexpress points of view about oral, print and other media texts\nmake connections between fictional texts and historical events\ndescribe and discuss new places, times, characters and events encountered in oral, print and other media texts\nwrite or represent the meaning of texts in different forms",
      "comments": [],
      "ancestorIds": [
        "DCE7204BBA1847DAB30695FA5107CFF8",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "DCE7204BBA1847DAB30695FA5107CFF8"
    },
    "DCE7204BBA1847DAB30695FA5107CFF8": {
      "id": "DCE7204BBA1847DAB30695FA5107CFF8",
      "asnIdentifier": null,
      "position": 6011,
      "depth": 1,
      "listId": "2",
      "statementNotation": "2.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Respond to Texts",
      "comments": [],
      "ancestorIds": [
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "594B26DC4D6C432595E99EF6A7BEDFD1"
    },
    "3E88A38D4D704CB7B62A38B6F5C77931": {
      "id": "3E88A38D4D704CB7B62A38B6F5C77931",
      "asnIdentifier": null,
      "position": 5011,
      "depth": 2,
      "listId": "d",
      "statementNotation": "2.1d",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use references\n\nfind words in dictionaries and glossaries to confirm the spellings or locate the meanings, by using knowledge of phonics and structural analysis, alphabetical order and guide words",
      "comments": [],
      "ancestorIds": [
        "F7EF16FC0790447F86B4137C78120B93",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "F7EF16FC0790447F86B4137C78120B93"
    },
    "442FF436CE0645E4BCDA841E6F1906CE": {
      "id": "442FF436CE0645E4BCDA841E6F1906CE",
      "asnIdentifier": null,
      "position": 5010,
      "depth": 2,
      "listId": "c",
      "statementNotation": "2.1c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use phonics and structural analysis\n\nidentify and know by sight the meaning of high frequency prefixes and suffixes to read unfamiliar, multisyllable words in context\nintegrate knowledge of phonics, sight vocabulary and structural analysis with knowledge of language and context clues to read unfamiliar words in context",
      "comments": [],
      "ancestorIds": [
        "F7EF16FC0790447F86B4137C78120B93",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "F7EF16FC0790447F86B4137C78120B93"
    },
    "0542450AC10C436C81F6A7384F2D9C25": {
      "id": "0542450AC10C436C81F6A7384F2D9C25",
      "asnIdentifier": null,
      "position": 5009,
      "depth": 2,
      "listId": "b",
      "statementNotation": "2.1b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use comprehension strategies\n\npreview sections of print texts, and apply reading rate and strategies appropriate for the purpose, content and format of the texts\ncomprehend new ideas and information by responding personally, taking notes and discussing ideas with others\nuse the meanings of familiar words to predict the meanings of unfamiliar words in context\nmonitor understanding by comparing personal knowledge and experiences with information on the same topic from a variety of sources",
      "comments": [],
      "ancestorIds": [
        "F7EF16FC0790447F86B4137C78120B93",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "F7EF16FC0790447F86B4137C78120B93"
    },
    "DC881D78AA36490394A8DD34478DAD81": {
      "id": "DC881D78AA36490394A8DD34478DAD81",
      "asnIdentifier": null,
      "position": 5008,
      "depth": 2,
      "listId": "a",
      "statementNotation": "2.1a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use prior knowledge\n\ndescribe ways that personal experiences and prior knowledge contribute to understanding new ideas and information\nuse knowledge of organizational structures, such as tables of contents, indices, topic sentences and headings, to locate information and to construct and confirm meaning",
      "comments": [],
      "ancestorIds": [
        "F7EF16FC0790447F86B4137C78120B93",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
      ],
      "parentId": "F7EF16FC0790447F86B4137C78120B93"
    },
