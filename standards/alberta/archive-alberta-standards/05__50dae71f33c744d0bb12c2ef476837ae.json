      "depth": 2,
      "listId": "c",
      "statementNotation": "2.3c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Experiment with language  \n\nexperiment with words and sentence patterns to create word pictures; identify how imagery and figurative language, such as simile and exaggeration, convey meaning",
      "comments": [],
      "ancestorIds": [
        "2D3A5284C7584399B43683116312B0C9",
        "594B26DC4D6C432595E99EF6A7BEDFD1"
