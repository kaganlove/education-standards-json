    "46435DED8DF149D19D25E8CB63260E18": {
      "id": "46435DED8DF149D19D25E8CB63260E18",
      "asnIdentifier": null,
      "position": 12011,
      "depth": 2,
      "listId": "b",
      "statementNotation": "2.3b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand techniques and elements\n\ninclude events, setting and characters when summarizing or retelling oral, print or other media texts\ndescribe the main characters in terms of who they are, their actions in the story and their relations with other characters\nidentify ways that messages are enhanced in oral, print and other media texts by the use of specific techniques",
      "comments": [],
      "ancestorIds": [
        "92AC574D9F904795A08F63B3AF82A3D0",
        "F994D41993614A398831C6DDF91AB27D"
