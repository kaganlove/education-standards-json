    "C76B22F71F96437FAEFE644711D4BB4D": {
      "id": "C76B22F71F96437FAEFE644711D4BB4D",
      "asnIdentifier": null,
      "position": 20000,
      "depth": 1,
      "listId": "1.U. Topic",
      "statementNotation": "2.U. Topic A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Topic A- Creating Colour: Students explore coloured materials, learning\nabout different colours, how they are created,\nwhat happens when they are mixed and how they\ncan be transferred from one material to another.\nStudents learn to distinguish and describe colours\nand work with a variety of materials to create,\nmodify and apply colours. In the process, students\nlearn that different materials have particular\nproperties and that the properties and interactions\nof materials have to be taken into account when\nthey are used for a specific purpose.",
      "comments": [],
      "ancestorIds": [
        "A451641D26714E3BB7701174F38AD897"
      ],
      "parentId": "A451641D26714E3BB7701174F38AD897"
    },
