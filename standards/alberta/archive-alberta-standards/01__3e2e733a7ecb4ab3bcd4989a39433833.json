    "E6FDBDE3C8BE40B49325D77015107AA2": {
      "id": "E6FDBDE3C8BE40B49325D77015107AA2",
      "asnIdentifier": null,
      "position": 18000,
      "depth": 1,
      "listId": "1.A.SLE",
      "statementNotation": "1.A.SLE",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will show growth in acquiring and applying the following traits:\n• curiosity\n• confidence in personal ability to explore materials and learn by direct study\n• inventiveness\n• perseverance: staying with an investigation over a sustained period of time\n• appreciation of the value of experience and careful observation\n• a willingness to work with others and to consider their ideas\n• a sense of responsibility for actions taken\n• respect for living things and environments, and commitment for their care.",
      "comments": [],
      "ancestorIds": [
        "4CDD13F57AD340F68742E6D7DC099555"
      ],
      "parentId": "4CDD13F57AD340F68742E6D7DC099555"
    },
