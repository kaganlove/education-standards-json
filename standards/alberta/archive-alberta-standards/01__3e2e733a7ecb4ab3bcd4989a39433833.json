    "B157AAB2AF5A4BE4A6203110517CB586": {
      "id": "B157AAB2AF5A4BE4A6203110517CB586",
      "asnIdentifier": null,
      "position": 48005,
      "depth": 1,
      "listId": "1.U.TopicE",
      "statementNotation": "1.U.TopicE",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Topic E-Needs of Animals and Plants: Students learn about living things and what they\nneed to live and grow. By studying a variety of\nliving things, students become familiar with\nsimilarities and differences and develop skills for\ndescribing and classifying what they see. As the\ntopic progresses, attention is focused on how\nliving things survive, what they need and how\ntheir needs are met. Through the topic, students\nbecome aware that groups of living things have\nsome common needs and that different animals\nand plants meet those needs in different ways.\nStudents also learn about their own responsibility\nin caring for living things.",
      "comments": [],
      "ancestorIds": [
        "A451641D26714E3BB7701174F38AD897"
      ],
      "parentId": "A451641D26714E3BB7701174F38AD897"
    },
