    "0A6BC827666F462FA64DCD8664B33756": {
      "id": "0A6BC827666F462FA64DCD8664B33756",
      "asnIdentifier": null,
      "position": 29001,
      "depth": 1,
      "listId": "1.U.TopicB",
      "statementNotation": "1.U.TopicB",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Topic B-Seasonal Changes: Students learn that changes in their environment\noccur in a regular pattern known as the seasons.\nThey explore weather change, and how the ups\nand downs of weather affect their own lives.\nLooking beyond themselves and beyond the\nimmediate weather, students are guided to\ndiscover that there are larger patterns of change\nthat affect the life habits of many living things.\nThe interactions among different parts of the\nenvironment, and the recurrence of change as part\nof a cycle, are important science ideas that are\nintroduced in this topic.",
      "comments": [],
      "ancestorIds": [
        "A451641D26714E3BB7701174F38AD897"
      ],
      "parentId": "A451641D26714E3BB7701174F38AD897"
    },
