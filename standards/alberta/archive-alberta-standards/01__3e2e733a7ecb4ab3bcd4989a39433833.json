    "BE139C93DC9D4C5B8A6EB825648C542A": {
      "id": "BE139C93DC9D4C5B8A6EB825648C542A",
      "asnIdentifier": null,
      "position": 34002,
      "depth": 1,
      "listId": "1.U.TopicC",
      "statementNotation": "1.U.TopicC",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Topic C-Building Thing: Students learn about materials by using them to\nconstruct a variety of objects, including model\nbuildings, toys, boats and vehicles. Students\nselect materials to use and gain experience as they\ncut and shape, fold, pile materials on top of one\nanother, join parts, and try different techniques to\nachieve the result that they intend. In the process,\nthey learn to look at objects that are similar to\nwhat they are trying to construct and, with\nguidance, begin to recognize the component parts\nthat make up the whole.",
      "comments": [],
      "ancestorIds": [
        "A451641D26714E3BB7701174F38AD897"
      ],
      "parentId": "A451641D26714E3BB7701174F38AD897"
    },
