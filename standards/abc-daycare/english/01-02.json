      "asnIdentifier": null,
      "position": 8000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-08",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tIdentify the main point of a paragraph or whole text and reflect upon it.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
