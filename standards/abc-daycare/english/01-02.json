    "A5C37F9EA5F740079C100F9EA206A504": {
      "id": "A5C37F9EA5F740079C100F9EA206A504",
      "asnIdentifier": null,
      "position": 14000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tRead at their best pace to accurately read, sound out words correctly, and recognize known words automatically when reading.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
    "87F7BE0BCCF44C87909C0D998C985561": {
      "id": "87F7BE0BCCF44C87909C0D998C985561",
      "asnIdentifier": null,
      "position": 13000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-13",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tRead a poem or rhyme out loud.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
    "646E58A9AF1C438AA0A7BB12CA336276": {
      "id": "646E58A9AF1C438AA0A7BB12CA336276",
      "asnIdentifier": null,
      "position": 12000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tMake or contribute to the making of a mind-map with key theme ideas from a piece of literature.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
    "BBB54F0924C649ACB6FB61831A0A390D": {
      "id": "BBB54F0924C649ACB6FB61831A0A390D",
      "asnIdentifier": null,
      "position": 11000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand the different types of texts that exist in literature and know how to best read and learn from them. ",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
    "FA92EFEBEC814893B4C0B6EE02FAFF41": {
