    "48F6A978909B48A3A122A89743909998": {
      "id": "48F6A978909B48A3A122A89743909998",
      "asnIdentifier": null,
      "position": 79000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-17",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tHave their legs postured nicely in front of them, feet flat on the floor, and elbow on the desk when using any kind of writing/painting utensil.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "55C4538DC5BA4964B01371AA69620D4B": {
      "id": "55C4538DC5BA4964B01371AA69620D4B",
      "asnIdentifier": null,
      "position": 78000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-16",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse the appropriate amount of pressure when using any kind of writing/painting utensil.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "A61CEBD65A4F495493AFD82565598EE0": {
      "id": "A61CEBD65A4F495493AFD82565598EE0",
      "asnIdentifier": null,
      "position": 77000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-15",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse correct, balanced pencil grip when using any kind of writing/painting utensil.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "11B45AC007AC4A23847C3EF79B018806": {
      "id": "11B45AC007AC4A23847C3EF79B018806",
      "asnIdentifier": null,
      "position": 76000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-14",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tWrite full words and sentences using free-hand writing skills on a blank and/or filled out piece of paper.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "0FF0B9B76EF04BE5996B52D21C0C71A6": {
      "id": "0FF0B9B76EF04BE5996B52D21C0C71A6",
      "asnIdentifier": null,
      "position": 75000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-13",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tWrite a sentence on a line with appropriate spacing, size, and legibility.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "8B80A1A525984C8CBEA871E813A1A6F7": {
      "id": "8B80A1A525984C8CBEA871E813A1A6F7",
      "asnIdentifier": null,
      "position": 74000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-12",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tForm all letters in the English language with appropriate sizing and legibility.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "D5BEA2F5261C4606957C51EDC61BD5B8": {
      "id": "D5BEA2F5261C4606957C51EDC61BD5B8",
      "asnIdentifier": null,
      "position": 73000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-11",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tForm a legible clockwise and anti-clockwise circular line.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "7B025133C86A401984F303E1781867F8": {
      "id": "7B025133C86A401984F303E1781867F8",
      "asnIdentifier": null,
      "position": 72000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tForm a legible downward diagonal (right and left), horizontal, upward and downward straight line in a writing space.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "64DF3EE7EF964F2FA949B837E51FD4B0": {
      "id": "64DF3EE7EF964F2FA949B837E51FD4B0",
      "asnIdentifier": null,
      "position": 71000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-09",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tCreate their own character, background, theme, issue, climax, and resolution in a creative piece of writing.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "9EB5D3F42A9B4BE8A0E72D2FFC906786": {
      "id": "9EB5D3F42A9B4BE8A0E72D2FFC906786",
      "asnIdentifier": null,
      "position": 70000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-08",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse their imagination, past and personal experiences, known class-learned vocabulary, and wonder to creatively write texts about a specific topic.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "B9539E797C2747CDBE9BEBC298146077": {
      "id": "B9539E797C2747CDBE9BEBC298146077",
      "asnIdentifier": null,
      "position": 69000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-07",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tRevise a piece of written work to make a sentence or paragraph better.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "892424A1560C4D4CA1D46BE87325454A": {
      "id": "892424A1560C4D4CA1D46BE87325454A",
      "asnIdentifier": null,
      "position": 68000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-06",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tWrite a simple sentence that makes sense with subject-verb agreement.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "71886634EE6B462B9539836BA9B1C38F": {
      "id": "71886634EE6B462B9539836BA9B1C38F",
      "asnIdentifier": null,
      "position": 67000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-05",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand various forms of stage 1 appropriate punctuation and use them appropriately (question/exclamation mark, full stop, comma, apostrophe for ownership and to shorten the word “is” together with another word, listing words, and speech marks).",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "F794B5ACD90B43B29E36CD84686BAEF3": {
      "id": "F794B5ACD90B43B29E36CD84686BAEF3",
      "asnIdentifier": null,
      "position": 66000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-04",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand when to use a capital letter (pronouns and sentence beginnings).",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "FD2D984F0182434BA612D409DBAEF258": {
      "id": "FD2D984F0182434BA612D409DBAEF258",
      "asnIdentifier": null,
      "position": 65000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-03",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand that a prepositional phrase conveys a time or a place in a sentence.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "F74E74EABBB04304B42AB04724CD1B87": {
      "id": "F74E74EABBB04304B42AB04724CD1B87",
      "asnIdentifier": null,
      "position": 64000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-02",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand that a conjunction separates 2 simple sentences to create a compound sentence.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "EE747A83F5F84CD9B141F3C3BDB1A3AB": {
      "id": "EE747A83F5F84CD9B141F3C3BDB1A3AB",
      "asnIdentifier": null,
      "position": 63000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-W-01",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand the differences and similarities between nouns, verbs, pronouns (and personal pronouns), adverbs, and adjectives and use them effectively when writing a sentence.",
      "comments": [],
      "ancestorIds": [
        "C5799AD8B89549A38D93CE1F2071499D"
      ],
      "parentId": "C5799AD8B89549A38D93CE1F2071499D"
    },
    "C5799AD8B89549A38D93CE1F2071499D": {
      "id": "C5799AD8B89549A38D93CE1F2071499D",
      "asnIdentifier": null,
      "position": 62000,
      "depth": 0,
      "listId": "A",
      "statementNotation": "EnS1-W",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Writing Focus",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "13943E304AFF44398E2D1C2E0F04C191": {
      "id": "13943E304AFF44398E2D1C2E0F04C191",
      "asnIdentifier": null,
      "position": 61000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-10",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse knowledge of the letter “e” at the end of a word and split digraphs to decode words with 1+ consonants and at least 2 vowels where the vowel at the end is an “e”.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "9597C915CB2640E58686D673A69686A8": {
      "id": "9597C915CB2640E58686D673A69686A8",
      "asnIdentifier": null,
      "position": 60000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-09",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand that certain consonant digraphs, trigraphs, and quad-graphs can sound completely different from single-letter consonants and other consonant digraphs, even those that may be spelled the same (e.g., “ough” in “tough” and “dough”).",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "2FCC99DBC9F046D8BD2E946C59A7C8BD": {
      "id": "2FCC99DBC9F046D8BD2E946C59A7C8BD",
      "asnIdentifier": null,
      "position": 59000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-08",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand and correctly say more difficult digraphs, trigraphs, and quad-graphs (as German speakers).",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "960BB2D8CAE248188C41489B61EA4E08": {
      "id": "960BB2D8CAE248188C41489B61EA4E08",
      "asnIdentifier": null,
      "position": 58000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-07",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tIdentify a phoneme from a given single-letter grapheme.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "30A6DF64F2DE4C61AA217EFE21840F7E": {
      "id": "30A6DF64F2DE4C61AA217EFE21840F7E",
      "asnIdentifier": null,
      "position": 57000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-06",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand that consonants can have a harsher sound than vowels when speaking.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "ECD7BB41DAC04974A933DEAEEFF657C6": {
      "id": "ECD7BB41DAC04974A933DEAEEFF657C6",
      "asnIdentifier": null,
      "position": 56000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-05",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse encoding with more difficult 4+ letter words consisting of 3+ consonants and at least 1 vowel. ",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "F9E28161812641058BB838F67EBF329E": {
      "id": "F9E28161812641058BB838F67EBF329E",
      "asnIdentifier": null,
      "position": 55000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-04",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse encoding with simple 2+ letter words with 1+ consonants and 1 vowel.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "A64B4B419E654F3DB9D63F5DBB83B45E": {
      "id": "A64B4B419E654F3DB9D63F5DBB83B45E",
      "asnIdentifier": null,
      "position": 54000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-03",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse decoding to know the sounds of a vowel digraph.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "000D8DC30FC5422E8E7FC42C318A2C59": {
      "id": "000D8DC30FC5422E8E7FC42C318A2C59",
      "asnIdentifier": null,
      "position": 53000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-02",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse grapheme/phoneme knowledge to read words fluently.",
      "comments": [],
      "ancestorIds": [
        "DC7A450764B0468791E400132B7281D2"
      ],
      "parentId": "DC7A450764B0468791E400132B7281D2"
    },
    "04A95AC8CECC436BB7A881CF8E40BB4E": {
      "id": "04A95AC8CECC436BB7A881CF8E40BB4E",
      "asnIdentifier": null,
      "position": 52000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-P-01",
      "altStatementNotation": null,
