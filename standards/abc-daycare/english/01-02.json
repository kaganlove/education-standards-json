      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-06",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUse a “topic sentence” to create a text.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
