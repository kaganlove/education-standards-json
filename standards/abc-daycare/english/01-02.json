      "listId": "B",
      "statementNotation": "EnS1-ULRC-07",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand how to create a paragraph.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
