      "asnIdentifier": null,
      "position": 9000,
      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-09",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand “Who”, “What”, “Where”, “How”, “Why”, and “When” questions in literature and answer them accordingly to the best of their abilities.",
      "comments": [],
      "ancestorIds": [
        "7FD31D972D52467899943742DCFA37EA"
      ],
      "parentId": "7FD31D972D52467899943742DCFA37EA"
    },
