      "depth": 1,
      "listId": "B",
      "statementNotation": "EnS1-ULRC-05",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "\tUnderstand positive and negative features of a person or argument and forge their own opinions.",
      "comments": [],
      "ancestorIds": [
