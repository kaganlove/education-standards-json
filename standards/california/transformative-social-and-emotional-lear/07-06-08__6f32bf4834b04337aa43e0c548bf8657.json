      "statementNotation": "1.G.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students develop a sense of personal power and can identify and use their personal, social, and cultural assets in healthy ways.",
      "comments": [],
      "ancestorIds": [
        "37526DBA9A264232ABACC467CB66EAD9"
      ],
