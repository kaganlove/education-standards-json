      "listId": "B",
      "statementNotation": "1.B.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students examine and reflect on the underlying causes of emotions. Students recognize the difference between emotions and feelings. Students work to label, with specificity, their feeling states.",
      "comments": [],
      "ancestorIds": [
        "0408A9DB8E654C54B6673B48A432A43E"
      ],
      "parentId": "0408A9DB8E654C54B6673B48A432A43E"
    },
