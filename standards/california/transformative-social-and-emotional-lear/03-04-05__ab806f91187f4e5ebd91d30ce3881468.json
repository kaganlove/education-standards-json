      "position": 1009,
      "depth": 1,
      "listId": "G",
      "statementNotation": "1.G.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students articulate their needs in constructive ways. Students begin to develop a sense of personal purpose and feel that their lives are important and meaningful.",
      "comments": [],
      "ancestorIds": [
        "DCB2BBFF0AB047DD8917D22ABFDD9329"
      ],
      "parentId": "DCB2BBFF0AB047DD8917D22ABFDD9329"
    },
