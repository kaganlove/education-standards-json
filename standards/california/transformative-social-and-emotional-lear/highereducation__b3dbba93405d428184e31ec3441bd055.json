    },
    "96488CB7290742B1B605F614E5DF9E8F": {
      "id": "96488CB7290742B1B605F614E5DF9E8F",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "D",
      "statementNotation": "4.D.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults model cultural humility and dedication to continuously strive toward cultural competence. Adults remain curious about and respectful of others’ cultures, view difference as an asset, and share their own cultural experiences.",
      "comments": [],
      "ancestorIds": [
        "2F76EC1B493946DD899A700BF8D7D7E3"
      ],
      "parentId": "2F76EC1B493946DD899A700BF8D7D7E3"
    },
    "F363487AF475427EB07529B3C6ECBBF1": {
      "id": "F363487AF475427EB07529B3C6ECBBF1",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "C",
      "statementNotation": "4.C.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults model being trusting friends, showing culturally affirming gratitude, and maintaining healthy relationships among people of different ages, races, cultures, linguistic backgrounds, neurodivergence, ability, genders, and sexual orientations.",
      "comments": [],
      "ancestorIds": [
        "2F76EC1B493946DD899A700BF8D7D7E3"
      ],
      "parentId": "2F76EC1B493946DD899A700BF8D7D7E3"
    },
