  "standards": {
    "5B45A2394C674F069C5767767A80E138": {
      "id": "5B45A2394C674F069C5767767A80E138",
      "asnIdentifier": null,
      "position": 1010,
      "depth": 1,
      "listId": "H",
      "statementNotation": "4.H.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults regularly call out inequities,\nadvocate for social and racial justice, and take action to promote the well-being, humanity, and dignity of others.",
      "comments": [],
      "ancestorIds": [
        "B7BDCE1CD95B4365B91FF3C63A19B740"
      ],
      "parentId": "B7BDCE1CD95B4365B91FF3C63A19B740"
    },
    "901ED7E30E434D658D483F934885F5AA": {
      "id": "901ED7E30E434D658D483F934885F5AA",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 1,
      "listId": "G",
      "statementNotation": "4.G.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults use nonviolent communication to clearly express their needs, advocate for themselves, and compromise with others. Adults are emotionally present and available to students, families, and colleagues.",
      "comments": [],
      "ancestorIds": [
        "B7BDCE1CD95B4365B91FF3C63A19B740"
      ],
      "parentId": "B7BDCE1CD95B4365B91FF3C63A19B740"
    },
    "2E837D777A0A41BB9148DFCB189A6C56": {
      "id": "2E837D777A0A41BB9148DFCB189A6C56",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "F",
      "statementNotation": "4.F.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults model employing restorative justice steps including the five R’s of relationship, respect, responsibility, repair, and reintegration. Adults adopt a restorative, healing-focused stance in their relationships with students and other adults.",
      "comments": [],
      "ancestorIds": [
        "B7BDCE1CD95B4365B91FF3C63A19B740"
      ],
      "parentId": "B7BDCE1CD95B4365B91FF3C63A19B740"
    },
    "7794B33ABFE64018A55499B8010F162C": {
      "id": "7794B33ABFE64018A55499B8010F162C",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "E",
      "statementNotation": "4.E.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults model the process of working with others to solve problems, engaging in active listening, practicing reciprocal vulnerability, and contributing to the common good.",
      "comments": [],
      "ancestorIds": [
        "B7BDCE1CD95B4365B91FF3C63A19B740"
      ],
      "parentId": "B7BDCE1CD95B4365B91FF3C63A19B740"
    },
