  "standards": {
    "85F691965B1643E8B13205B71F731FD7": {
      "id": "85F691965B1643E8B13205B71F731FD7",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 1,
      "listId": "G",
      "statementNotation": "5.G.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults predict how they may feel and respond in various situations and share how they evaluate the impact of these choices on others. Adults understand and share how their own words, thoughts, and actions have impacted themselves and others in a variety of contexts.",
      "comments": [],
      "ancestorIds": [
        "A4D8C327FF7C4B369415812BF2DDA69C"
      ],
      "parentId": "A4D8C327FF7C4B369415812BF2DDA69C"
    },
    "82AAF3286923427893B23F87526605DD": {
      "id": "82AAF3286923427893B23F87526605DD",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "F",
      "statementNotation": "5.F.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adults model the process of using data and facts to make decisions. Adults share how they gather data, synthesize, apply, and reflect on it in new situations to make informed decisions.",
      "comments": [],
      "ancestorIds": [
        "A4D8C327FF7C4B369415812BF2DDA69C"
      ],
      "parentId": "A4D8C327FF7C4B369415812BF2DDA69C"
    },
    "A4D8C327FF7C4B369415812BF2DDA69C": {
