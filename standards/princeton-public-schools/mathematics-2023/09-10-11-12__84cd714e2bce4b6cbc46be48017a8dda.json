    },
    "57BA6CF17F694CAA9C4FCD97597D97C8": {
      "id": "57BA6CF17F694CAA9C4FCD97597D97C8",
      "asnIdentifier": null,
      "position": 39000,
      "depth": 1,
      "listId": "A",
      "statementNotation": "S.MD.A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Calculate expected values and use them to solve problems",
      "comments": [],
      "ancestorIds": [
        "54C5BCBECA59429C82F2D91908B87449"
      ],
      "parentId": "54C5BCBECA59429C82F2D91908B87449"
