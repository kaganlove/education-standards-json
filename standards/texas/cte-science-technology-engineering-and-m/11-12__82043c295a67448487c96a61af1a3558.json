      "depth": 1,
      "listId": "2",
      "statementNotation": "a.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "If the commissioner makes the determination that instructional materials funding has been made available this section shall be implemented beginning with the 2023-2024 school year and apply to the 2023-2024 and subsequent school years.",
      "comments": [],
      "ancestorIds": [
        "8DCFD1A7ED6443309DA5CFEC41C1DA19"
      ],
      "parentId": "8DCFD1A7ED6443309DA5CFEC41C1DA19"
    },
