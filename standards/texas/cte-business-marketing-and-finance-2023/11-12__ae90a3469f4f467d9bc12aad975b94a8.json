    "D9F62560F5124E6991971727F2D5B3EC": {
      "id": "D9F62560F5124E6991971727F2D5B3EC",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "5",
      "statementNotation": "c.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students are encouraged to transition from the idea phase to action and implementation of a\nbusiness, including validation through sales in a real or simulated scenario.",
      "comments": [],
      "ancestorIds": [
        "6DF9E5612E1D46BDACC319F8C0DF519A"
      ],
      "parentId": "6DF9E5612E1D46BDACC319F8C0DF519A"
    },
    "E2B18F3B4EDD45BE8218AA21FF528CDB": {
      "id": "E2B18F3B4EDD45BE8218AA21FF528CDB",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "4",
      "statementNotation": "c.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students are encouraged to participate in extended learning experiences such as career and\ntechnical student organizations, local chamber of commerce meetings, and meetings with\nentrepreneurs, mentors, or industry experts.",
      "comments": [],
      "ancestorIds": [
        "6DF9E5612E1D46BDACC319F8C0DF519A"
      ],
      "parentId": "6DF9E5612E1D46BDACC319F8C0DF519A"
    },
    "E452FD876768444AA0F917D10082D619": {
      "id": "E452FD876768444AA0F917D10082D619",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "3",
      "statementNotation": "c.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Extended Practicum in Entrepreneurship provides students the opportunity to apply classroom\nlearning and experiences to real-world business problems and opportunities in a free enterprise\nsystem while expanding their skill sets and professional relationships as a real or simulated business owner versus the experience one would have as an employee. Students will prepare for\nan entrepreneurial career in their area of interest in their career cluster and build on and apply the\nknowledge and skills gained from courses taken in an array of career areas. Practicum experiences\noccur in a paid or an unpaid arrangement and a variety of locations appropriate to the nature and\nlevel of the student's need for work-based learning experience. Students implement personal and\ninterpersonal skills to strengthen individual performance in the workplace and in society and to\nmake a successful transition to the workforce or postsecondary education. It is recommended that\nstudents are paired with local business owners or employers in their specific industry program of\nstudy.",
      "comments": [],
      "ancestorIds": [
        "6DF9E5612E1D46BDACC319F8C0DF519A"
      ],
      "parentId": "6DF9E5612E1D46BDACC319F8C0DF519A"
    },
    "3C8E662A5A7D4661ABC07660892CF03E": {
      "id": "3C8E662A5A7D4661ABC07660892CF03E",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "2",
      "statementNotation": "c.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "This course can serve in multiple CTE programs of study, as it focuses on planning, managing,\norganizing, directing, and evaluating business functions essential to efficient and productive\nbusiness management, finance, operations, and marketing related to the student's industry focus.",
      "comments": [],
      "ancestorIds": [
        "6DF9E5612E1D46BDACC319F8C0DF519A"
      ],
      "parentId": "6DF9E5612E1D46BDACC319F8C0DF519A"
    },
    "CE6DC39530A14938AA83ADFAD9C63B55": {
      "id": "CE6DC39530A14938AA83ADFAD9C63B55",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "1",
      "statementNotation": "c.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Career and technical education instruction provides content aligned with challenging academic\nstandards and relevant technical knowledge and skills for students to further their education and\nsucceed in current or emerging professions.",
      "comments": [],
      "ancestorIds": [
        "6DF9E5612E1D46BDACC319F8C0DF519A"
      ],
      "parentId": "6DF9E5612E1D46BDACC319F8C0DF519A"
