      "parentId": null
    },
    "55E77449E097435C8C4C1C8C965DACF6": {
      "id": "55E77449E097435C8C4C1C8C965DACF6",
      "asnIdentifier": null,
      "position": 1001,
      "depth": 0,
      "listId": "b",
      "statementNotation": "b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "General requirements. This course is recommended for students in Grades 11 and 12. Recommended\nprerequisites: Entrepreneurship I and Entrepreneurship II or successful completion of at least two courses\nin a career and technical education (CTE) program of study. Students shall be awarded two credits for\nsuccessful completion of this course. A student may repeat this course once for credit provided that the\nstudent is experiencing different aspects of the industry and demonstrating proficiency in additional and\nmore advanced knowledge and skills.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
