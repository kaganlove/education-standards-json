    "3789B71A09134F83A410060D0BFC2531": {
      "id": "3789B71A09134F83A410060D0BFC2531",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 1,
      "listId": "4",
      "statementNotation": "c.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students are encouraged to participate in extended learning experiences such as career and\ntechnical student organizations, local chamber of commerce meetings, and meetings with\nentrepreneurs, mentors, or industry experts.",
      "comments": [],
      "ancestorIds": [
        "6CDAC356673C4FF682B145D68CC5EA7B"
      ],
      "parentId": "6CDAC356673C4FF682B145D68CC5EA7B"
    },
    "1B6FB97DB8AD4B4DBCE03F754C60CAD1": {
      "id": "1B6FB97DB8AD4B4DBCE03F754C60CAD1",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "3",
      "statementNotation": "c.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Practicum in Entrepreneurship provides students the opportunity to apply classroom learning and\nexperiences to real-world business problems and opportunities in a free enterprise system while\nexpanding their skill sets and professional relationships as a real or simulated business owner\nversus the experience one would have as an employee. Students will prepare for an entrepreneurial\ncareer in their area of interest in their career cluster and build on and apply the knowledge and\nskills gained from courses taken in an array of career areas. Practicum experiences occur in a paid\nor an unpaid arrangement and a variety of locations appropriate to the nature and level of the\nstudent's need for work-based learning experience. Students implement personal and interpersonal\nskills to strengthen individual performance in the workplace and in society and to make a\nsuccessful transition to the workforce or postsecondary education. It is recommended that students\nare paired with local business owners or employers in their specific industry program of study.",
      "comments": [],
      "ancestorIds": [
        "6CDAC356673C4FF682B145D68CC5EA7B"
      ],
      "parentId": "6CDAC356673C4FF682B145D68CC5EA7B"
    },
    "D5CCF60448514617BC34BA72389A8988": {
      "id": "D5CCF60448514617BC34BA72389A8988",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "2",
      "statementNotation": "c.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "This course can serve in multiple CTE programs of study, as it focuses on planning, managing,\norganizing, directing, and evaluating business functions essential to efficient and productive\nbusiness management, finance, operations, and marketing related to the student's industry focus.",
      "comments": [],
      "ancestorIds": [
        "6CDAC356673C4FF682B145D68CC5EA7B"
      ],
      "parentId": "6CDAC356673C4FF682B145D68CC5EA7B"
    },
    "F226F2CC40C44752AE272613925D8083": {
      "id": "F226F2CC40C44752AE272613925D8083",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "1",
      "statementNotation": "c.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Career and technical education instruction provides content aligned with challenging academic\nstandards and relevant technical knowledge and skills for students to further their education and\nsucceed in current or emerging professions.",
      "comments": [],
      "ancestorIds": [
        "6CDAC356673C4FF682B145D68CC5EA7B"
      ],
      "parentId": "6CDAC356673C4FF682B145D68CC5EA7B"
    },
