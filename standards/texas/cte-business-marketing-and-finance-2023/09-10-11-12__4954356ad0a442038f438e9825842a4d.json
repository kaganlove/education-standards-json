      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "2",
      "statementNotation": "c.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The Business, Marketing, and Finance Career Cluster focuses on planning, managing, organizing,\ndirecting, and evaluating business functions essential to efficient and productive business\nmanagement, finance, operations, and marketing.",
      "comments": [],
      "ancestorIds": [
        "68CDE991463940A7B921B98C8D19EAC2"
      ],
      "parentId": "68CDE991463940A7B921B98C8D19EAC2"
    },
