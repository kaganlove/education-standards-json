    "6597C5D44E36482AB56A714802F5A0A0": {
      "id": "6597C5D44E36482AB56A714802F5A0A0",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "3",
      "statementNotation": "c.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "In Entrepreneurship I, students will gain the knowledge and skills needed to become an\nentrepreneur in a free enterprise system. Students will learn the key concepts necessary to begin\nand operate a business. The primary focus of the course is to help students identify the types and\nselection criteria of business structures, understand the components of a business plan, determine\nfeasibility of an idea using research, and develop and present a business concept. In addition,\nstudents will understand the basics of management, accounting, finance, marketing, risk, and\nproduct development.",
      "comments": [],
      "ancestorIds": [
        "68CDE991463940A7B921B98C8D19EAC2"
      ],
      "parentId": "68CDE991463940A7B921B98C8D19EAC2"
    },
    "0A20EBD49B8D45D3A0943FE88C74744A": {
