    "7DF9E839749A4148BBDA46A9BC782B64": {
      "id": "7DF9E839749A4148BBDA46A9BC782B64",
      "asnIdentifier": null,
      "position": 3002,
      "depth": 1,
      "listId": "3",
      "statementNotation": "FORENSPSY.b.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Forensic psychology is found at the intersection between psychology and the\ncriminal justice system. It involves understanding criminal law in the relevant\njurisdictions in order to be able to interact within the criminal justice system. The course introduces theoretical frameworks used to identify and diagnose the clinical\npsychiatric status of individuals involved in civil and criminal litigation. The course\napplies basic skills developed in psychology and criminal scenarios resulting in a\nstructured and scientific approach to investigative analysis; thereby, enabling\npolice and law enforcement officials to predict criminal activity via scientific\nanalysis rather than intuition. Students will learn basic structured psychological\ninvestigative techniques in question building, interviewing, criminal behavior\ncharacteristics, truth detection methodology, research methods, statistical analysis\nand probability forecasting. ",
      "comments": [],
      "ancestorIds": [
        "E6CFC14794564CF5A5D418925DE57E87"
      ],
      "parentId": "E6CFC14794564CF5A5D418925DE57E87"
    },
