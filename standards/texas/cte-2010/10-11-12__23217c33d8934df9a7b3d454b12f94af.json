    "219ADBF1694248F99ED7FBACEEDE13F5": {
      "id": "219ADBF1694248F99ED7FBACEEDE13F5",
      "asnIdentifier": null,
      "position": 6000,
      "depth": 1,
      "listId": "(4) ",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "In Accounting I, students will investigate the field of accounting, including how it is impacted by industry standards as well as economic, financial, technological, international, social, legal, and ethical factors. Students will reflect on this knowledge as they engage in the process of recording, classifying, summarizing, analyzing, and communicating accounting information. Students will formulate and interpret financial information for use in management decision making.  ",
      "comments": [],
      "ancestorIds": [
        "DA15185D63F74474928EEF2FA6FB23EC"
      ],
      "parentId": "DA15185D63F74474928EEF2FA6FB23EC"
    },
    "EBA4A88CCE894AB39C8D375461BB6EF9": {
      "id": "EBA4A88CCE894AB39C8D375461BB6EF9",
      "asnIdentifier": null,
      "position": 5000,
      "depth": 1,
      "listId": "(3) ",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Accounting encompasses careers that record, classify, summarize, analyze, and communicate a business's financial information/business transactions for use in management decision making. Accounting includes such activities as bookkeeping, systems design, analysis, and interpretation of accounting information.",
      "comments": [],
      "ancestorIds": [
        "DA15185D63F74474928EEF2FA6FB23EC"
      ],
      "parentId": "DA15185D63F74474928EEF2FA6FB23EC"
    },
