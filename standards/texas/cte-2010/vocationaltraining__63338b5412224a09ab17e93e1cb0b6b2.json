    "B998C37648434069871B6589B7154410": {
      "id": "B998C37648434069871B6589B7154410",
      "asnIdentifier": null,
      "position": 4000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "AMMT.B.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Agricultural Mechanics and Metal Technologies is designed to develop an understanding of\nagricultural mechanics as it relates to safety and skills in tool operation, electrical wiring,\nplumbing, carpentry, fencing, concrete, and metal working techniques. To prepare for careers in\nagricultural power, structural, and technical systems, students must attain academic skills and\nknowledge; acquire technical knowledge and skills related to power, structural, and technical\nagricultural systems and the industry; and develop knowledge and skills regarding career\nopportunities, entry requirements, industry certifications, and industry expectations. To prepare for\nsuccess, students need opportunities to learn, reinforce, apply, and transfer knowledge and skills\nand technologies in a variety of settings.",
      "comments": [],
      "ancestorIds": [
        "B2AEC89AD84E44C1B5E54F0DB653B264"
      ],
      "parentId": "B2AEC89AD84E44C1B5E54F0DB653B264"
    },
