      "depth": 1,
      "listId": "2",
      "statementNotation": "AMMT.B.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The Agriculture, Food, and Natural Resources Career Cluster focuses on the production,\nprocessing, marketing, distribution, financing, and development of agricultural commodities and\nresources, including food, fiber, wood products, natural resources, horticulture, and other plant and\nanimal products/resources.",
      "comments": [],
      "ancestorIds": [
        "B2AEC89AD84E44C1B5E54F0DB653B264"
      ],
      "parentId": "B2AEC89AD84E44C1B5E54F0DB653B264"
    },
