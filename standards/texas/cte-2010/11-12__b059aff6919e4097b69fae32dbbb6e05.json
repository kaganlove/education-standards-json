      "depth": 0,
      "listId": "(3)",
      "statementNotation": "TX MM 3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The student uses scientific methods and equipment during laboratory and field investigations. The student is expected to:",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "867BDDFB7E3749508D41A3B945550001": {
