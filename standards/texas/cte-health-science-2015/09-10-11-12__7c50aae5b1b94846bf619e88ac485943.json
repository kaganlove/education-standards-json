      "position": 8000,
      "depth": 1,
      "listId": "6",
      "statementNotation": "b.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Professional integrity in the health care industry is dependent on acceptance of ethical and legal responsibilities. Students are expected to employ their ethical and legal responsibilities, recognize limitations, and understand the implications of their actions.",
      "comments": [],
      "ancestorIds": [
        "50051E65A76D4DDEBDD1124458D3591E"
