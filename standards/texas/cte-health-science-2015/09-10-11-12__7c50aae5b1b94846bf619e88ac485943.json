    "6C26AAAAD5B04A54B02EA518DA1079C1": {
      "id": "6C26AAAAD5B04A54B02EA518DA1079C1",
      "asnIdentifier": null,
      "position": 7000,
      "depth": 1,
      "listId": "5",
      "statementNotation": "b.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The health care industry is comprised of diagnostic, therapeutic, health informatics, support services, and biotechnology research and development systems that function individually and collaboratively to provide comprehensive health care. Students recognize the employment opportunities, technology, and safety requirements of each system. Students are expected to apply the knowledge and skills necessary to pursue a health science certification or licensure through further education and employment.",
      "comments": [],
      "ancestorIds": [
        "50051E65A76D4DDEBDD1124458D3591E"
      ],
      "parentId": "50051E65A76D4DDEBDD1124458D3591E"
    },
    "7E34ACD04EFE4C45AC6DFE911682A90D": {
      "id": "7E34ACD04EFE4C45AC6DFE911682A90D",
      "asnIdentifier": null,
      "position": 6000,
      "depth": 1,
      "listId": "4",
      "statementNotation": "b.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "To pursue a career in the health care industry, students should learn to reason, think critically, make decisions, solve problems, and communicate effectively. Students should recognize that quality health care depends on the ability to work well with others.",
      "comments": [],
      "ancestorIds": [
        "50051E65A76D4DDEBDD1124458D3591E"
      ],
      "parentId": "50051E65A76D4DDEBDD1124458D3591E"
    },
    "91DF24D8EA924DF29733C4F58906F279": {
