    "2D3E98C0466D4CBEB3116E001DA16D98": {
      "id": "2D3E98C0466D4CBEB3116E001DA16D98",
      "asnIdentifier": null,
      "position": 1001,
      "depth": 1,
      "listId": "1",
      "statementNotation": "a.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "The study of creative writing allows high school students to earn one-half to one credit while\ndeveloping versatility as a writer. Creative Writing, a rigorous composition course, asks high\nschool students to demonstrate their skill in such forms of writing as fictional writing, short stories, poetry, and drama. All students are expected to demonstrate an understanding of the\nrecursive nature of the writing process, effectively applying the conventions of usage and the\nmechanics of written English. The students' evaluation of their own writing as well as the writing\nof others ensures that students completing this course are able to analyze and discuss published\nand unpublished pieces of writing, develop peer and self-assessments for effective writing, and set\ntheir own goals as writers.",
      "comments": [],
      "ancestorIds": [
        "1ED51FCCB7B34BAA9A82259D6B6A2853"
      ],
      "parentId": "1ED51FCCB7B34BAA9A82259D6B6A2853"
    },
