    "3592776349654345BE8256DE314F22BC": {
      "id": "3592776349654345BE8256DE314F22BC",
      "asnIdentifier": null,
      "position": 52000,
      "depth": 2,
      "listId": "a",
      "statementNotation": "5.G.A.1.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand that the first number indicates how far to travel from the\r\norigin in the direction of one axis, and the second number indicates how\r\nfar to travel in the direction of the second axis, with the convention that\r\nthe names of the two axes and the coordinates correspond (e.g., $x$-\r\naxis and $-$-coordinate, $y$-axis and $y$-coordinate).",
      "comments": [],
      "ancestorIds": [
        "070B0A6A23414E7480847016342ADE11",
        "68B309344D12467D91A80A3B0C442C82"
      ],
      "parentId": "070B0A6A23414E7480847016342ADE11"
    },
    "070B0A6A23414E7480847016342ADE11": {
