    "2EA59B1D7F894101A11258276386F79D": {
      "id": "2EA59B1D7F894101A11258276386F79D",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 2,
      "listId": "i",
      "statementNotation": "3-A-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Unpacked: People learn by observation, by being told,\nby asking questions, by experimentation, by practice,\nand by making connections to past experience.\nComputers learn by finding patterns in data, or by trial\nand error. ",
      "comments": [],
