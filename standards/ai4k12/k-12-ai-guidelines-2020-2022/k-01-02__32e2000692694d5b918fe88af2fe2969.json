    "B9701B1B180E4A86AF8CF9F4A983A130": {
      "id": "B9701B1B180E4A86AF8CF9F4A983A130",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 2,
      "listId": "i",
      "statementNotation": "3-A-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Activities: Describe a time when you learned something\nby being told, by watching another person, or by asking\nquestions. A demo such as Teachable Machine can be\nused to illustrate a computer learning something from\npositive and negative examples.",
      "comments": [],
