    "945D8B672CB84DFABC8FE0FE88533B79": {
      "id": "945D8B672CB84DFABC8FE0FE88533B79",
      "asnIdentifier": null,
      "position": 1029,
      "depth": 2,
      "listId": "i",
      "statementNotation": "4-C-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Activity: Use a face demo that recognizes\nfacial landmarks (e.g., eyebrows, eyes, and\nmouth) to infer emotional state. The activity\ncan be introduced using icons to demonstrate\ndifferent emotions.",
      "comments": [],
      "ancestorIds": [
        "19BDC3E9C85A4592AD9EF78250796D7E"
      ],
      "parentId": null
    },
    "9CBF3F25EFF34AC1B9A11B10965231BB": {
      "id": "9CBF3F25EFF34AC1B9A11B10965231BB",
      "asnIdentifier": null,
      "position": 1028,
      "depth": 2,
      "listId": "i",
      "statementNotation": "4-C-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "EU: Computers recognize emotions in faces\nby looking at the shapes of the mouth, eyes,\nand eyebrows.",
      "comments": [],
      "ancestorIds": [
        "19BDC3E9C85A4592AD9EF78250796D7E"
      ],
      "parentId": null
    },
    "E91F162595F34CDEBFAA612C22CE1FA4": {
