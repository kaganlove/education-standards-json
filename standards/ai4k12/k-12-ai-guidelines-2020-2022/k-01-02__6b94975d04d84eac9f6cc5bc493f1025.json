      "listId": "i",
      "statementNotation": "4-D-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "LO: Demonstrate some ways that Intelligent\nagents don't understand things the way people\ndo. ",
      "comments": [],
      "ancestorIds": [
        "5A6F05ACB34C43618B2C999795E1C14A"
      ],
      "parentId": null
    },
