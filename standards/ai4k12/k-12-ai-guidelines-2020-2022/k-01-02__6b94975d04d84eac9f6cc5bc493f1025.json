      "listId": "i",
      "statementNotation": "4-C-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "LO: Demonstrate how computers recognize\nemotions in faces.",
      "comments": [],
      "ancestorIds": [
        "19BDC3E9C85A4592AD9EF78250796D7E"
      ],
      "parentId": null
    },
