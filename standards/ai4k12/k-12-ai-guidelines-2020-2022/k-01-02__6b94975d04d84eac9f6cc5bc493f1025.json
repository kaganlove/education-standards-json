  "standards": {
    "36BC4E0C30CF459DB90565DCED02F590": {
      "id": "36BC4E0C30CF459DB90565DCED02F590",
      "asnIdentifier": null,
      "position": 1033,
      "depth": 2,
      "listId": "i",
      "statementNotation": "4-D-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Unpacked: Today's intelligent agents cannot\nanswer most common sense questions, such\nas \"Would smelly socks make a good birthday\npresent?\" They can read stories to children but\ncannot answer questions about the story. They\nare also unaware of repetitive questions, e.g.,\nif asked the same question five times in a row\nthey would not become frustrated or even\nacknowledge that the questions are repetitive.",
      "comments": [],
      "ancestorIds": [
        "5A6F05ACB34C43618B2C999795E1C14A"
      ],
      "parentId": null
    },
    "9DBF6EE45A994664BE66D9FF5FB9EBF2": {
      "id": "9DBF6EE45A994664BE66D9FF5FB9EBF2",
      "asnIdentifier": null,
      "position": 1032,
      "depth": 2,
      "listId": "i",
      "statementNotation": "4-D-i",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "EU: Artificial Intelligence devices are not\npeople, they are computer programs designed\nby people",
      "comments": [],
      "ancestorIds": [
        "5A6F05ACB34C43618B2C999795E1C14A"
      ],
      "parentId": null
    },
    "5B5DA6CF222146F5B5F24A99460028EB": {
      "id": "5B5DA6CF222146F5B5F24A99460028EB",
      "asnIdentifier": null,
      "position": 1031,
