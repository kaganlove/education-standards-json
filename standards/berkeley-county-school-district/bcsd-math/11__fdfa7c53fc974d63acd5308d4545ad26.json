    "12C8206A28274F3BB4515FD97EC64025": {
      "id": "12C8206A28274F3BB4515FD97EC64025",
      "asnIdentifier": null,
      "position": 28000,
      "depth": 1,
      "listId": "1.",
      "statementNotation": "PC. FLQE.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Express a logarithm as the solution to the exponential equation, 𝑎𝑏𝑐𝑡 = 𝑑 where 𝑎, 𝑐, and 𝑑 are\nnumbers and the base 𝑏 is 2, 10, or 𝑒; evaluate the logarithm using technology.",
      "comments": [],
      "ancestorIds": [
        "82375F72CA6448C48F85B11BF82CDC24"
      ],
      "parentId": "82375F72CA6448C48F85B11BF82CDC24"
