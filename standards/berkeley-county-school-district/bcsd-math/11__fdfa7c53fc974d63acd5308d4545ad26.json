    "82375F72CA6448C48F85B11BF82CDC24": {
      "id": "82375F72CA6448C48F85B11BF82CDC24",
      "asnIdentifier": null,
      "position": 27000,
      "depth": 0,
      "listId": "VI.",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Linear, Quadratic, and Exponential ",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "86B93DD5571F49BEAE87A42F09794DCD": {
      "id": "86B93DD5571F49BEAE87A42F09794DCD",
      "asnIdentifier": null,
      "position": 26000,
      "depth": 1,
      "listId": "4.",
      "statementNotation": "PC.FIF.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Graph functions from their symbolic representations. Indicate key features including intercepts;\nintervals where the function is increasing, decreasing, positive, or negative; relative maximums\nand minimums; symmetries; end behavior and periodicity. Graph simple cases by hand and use\ntechnology for complicated cases.",
      "comments": [],
      "ancestorIds": [
        "0928A2B8DF514BD4AF82101DE8527BBA"
      ],
      "parentId": "0928A2B8DF514BD4AF82101DE8527BBA"
    },
    "F92E6735648D49DAA889096C3DCDD5EA": {
      "id": "F92E6735648D49DAA889096C3DCDD5EA",
      "asnIdentifier": null,
      "position": 25000,
      "depth": 1,
      "listId": "3.",
      "statementNotation": "PC.FIF.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Given a function in graphical, symbolic, or tabular form, determine the average rate of change of\nthe function over a specified interval. Interpret the meaning of the average rate of change in a\ngiven context.",
      "comments": [],
      "ancestorIds": [
        "0928A2B8DF514BD4AF82101DE8527BBA"
      ],
      "parentId": "0928A2B8DF514BD4AF82101DE8527BBA"
    },
    "8E582B902E9D4B459481EDDDF0862E7B": {
      "id": "8E582B902E9D4B459481EDDDF0862E7B",
      "asnIdentifier": null,
      "position": 24000,
      "depth": 1,
      "listId": "2.",
      "statementNotation": "PC.FIF.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Relate the domain and range of a function to its graph and, where applicable, to the quantitative\nrelationship it describes. ",
      "comments": [],
      "ancestorIds": [
        "0928A2B8DF514BD4AF82101DE8527BBA"
      ],
      "parentId": "0928A2B8DF514BD4AF82101DE8527BBA"
    },
    "86AFC9A5DE554594A4023EDDE9168B89": {
      "id": "86AFC9A5DE554594A4023EDDE9168B89",
      "asnIdentifier": null,
      "position": 23000,
      "depth": 1,
      "listId": "1.",
      "statementNotation": "PC.FIF.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret key features of a function that models the relationship between two quantities when\ngiven in graphical or tabular form. Sketch the graph of a function from a verbal description\nshowing key features. Key features include intercepts; intervals where the function is increasing,\ndecreasing, constant, positive, or negative; relative maximums and minimums; symmetries; end\nbehavior and periodicity. ",
      "comments": [],
      "ancestorIds": [
        "0928A2B8DF514BD4AF82101DE8527BBA"
      ],
      "parentId": "0928A2B8DF514BD4AF82101DE8527BBA"
    },
