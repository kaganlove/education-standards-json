      "asnIdentifier": null,
      "position": 25000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "8.D.5.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe, using basic dance terminology, the qualities and characteristics\nof style used in a dance from one’s own cultural movement practice.",
