    "F808DAD0DFE501311D3768A86D17958E": {
      "position": 2000,
      "depth": 1,
      "statementLabel": "Content Standard",
      "description": "Students develop a spatial understanding of Earth's surface and the processes that shape it, the connections between people and places, and the relationship between man and his environment.",
      "id": "F808DAD0DFE501311D3768A86D17958E",
      "asnIdentifier": "S2406076",
      "ancestorIds": [
        "F8088410DFE501311D3668A86D17958E"
      ],
