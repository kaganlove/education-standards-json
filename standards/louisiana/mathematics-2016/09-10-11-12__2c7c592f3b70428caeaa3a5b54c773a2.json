    "DEC0E8B3DB3B4763B3307EC63AC17BC6": {
      "id": "DEC0E8B3DB3B4763B3307EC63AC17BC6",
      "asnIdentifier": null,
      "position": 66000,
      "depth": 3,
      "listId": "5",
      "statementNotation": " S-CP.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize and explain the concepts of conditional probability and independence in everyday language and\neveryday situations. For example, compare the chance of having lung cancer if you are a smoker with the chance\nof being a smoker if you have lung cancer. ",
      "comments": [],
      "ancestorIds": [
        "D5BF691136BD4E0E8B99ABAB5D04E293",
        "943B39F29C7141199E6D12375DC1F8AE",
        "AB8F5CBC93DB4B3DBDBE6DC48A8A0F46"
