    "0A3F5168D7CD44EA8F222BEC5A344F71": {
      "id": "0A3F5168D7CD44EA8F222BEC5A344F71",
      "asnIdentifier": null,
      "position": 1015,
      "depth": 1,
      "listId": "4",
      "statementNotation": "4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will use scientific skills and processes to explain the interactions of matter and energy and the energy transformations that occur. ",
      "comments": [],
      "ancestorIds": [
        "B2BC182E2F32420F90C87EB5A6839D57"
      ],
      "parentId": "B2BC182E2F32420F90C87EB5A6839D57"
    },
    "B2BC182E2F32420F90C87EB5A6839D57": {
