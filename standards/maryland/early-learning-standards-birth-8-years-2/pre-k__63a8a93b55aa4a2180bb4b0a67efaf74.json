    "1EE9EA6269204442BC7DB2951E23733E": {
      "id": "1EE9EA6269204442BC7DB2951E23733E",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 2,
      "listId": "1",
      "statementNotation": "3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use scientific thinking as well as his senses to discover the world around him, and make comparisons between objects (e.g., watch the fish and tell that he likes the biggest one best).",
      "comments": [],
      "ancestorIds": [
        "3BDEED92E2BA4AF4B879E33E0D677C8F",
        "1C1B0EAC5B6C4A49BC1C9F15DAFC88BA"
      ],
      "parentId": "3BDEED92E2BA4AF4B879E33E0D677C8F"
    },
    "3BDEED92E2BA4AF4B879E33E0D677C8F": {
      "id": "3BDEED92E2BA4AF4B879E33E0D677C8F",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 1,
      "listId": "3",
      "statementNotation": "3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Students will use scientific skills and processes to explain the composition, structure, and interactions of matter in order\nto support the predictability of structure and energy transformations.",
      "comments": [],
      "ancestorIds": [
        "1C1B0EAC5B6C4A49BC1C9F15DAFC88BA"
      ],
      "parentId": "1C1B0EAC5B6C4A49BC1C9F15DAFC88BA"
    },
    "1C1B0EAC5B6C4A49BC1C9F15DAFC88BA": {
      "id": "1C1B0EAC5B6C4A49BC1C9F15DAFC88BA",
