    "F8C2AABB938144DB812C85C0ABCC0443": {
      "id": "F8C2AABB938144DB812C85C0ABCC0443",
      "asnIdentifier": null,
      "position": 1034,
      "depth": 2,
      "listId": "4",
      "statementNotation": "S.CP.A.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Construct and interpret two-way frequency tables of data when two categories are\nassociated with each object being classified. Use the two-way table as a sample space to decide if\nevents are independent and to approximate conditional probabilities. For example, collect data\nfrom a random sample of students in your school on their favorite subject among math, science,\nand English. Estimate the probability that a randomly selected student from your school will favor\nscience given that the student is in tenth grade. Do the same for other subjects and compare the\nresults.",
      "comments": [],
      "ancestorIds": [
        "8E90FB167CB546E6BB21AAACC93F8528",
        "35659110984141D7ABBBACEA17CAE257"
      ],
      "parentId": "8E90FB167CB546E6BB21AAACC93F8528"
