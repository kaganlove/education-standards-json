      "id": "31F7C88840BB47F48726862A585A4322",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 3,
      "listId": "4",
      "statementNotation": "2.1C.1I.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can identify places, transportation, occupations, and animals using ASL.",
      "comments": [],
      "ancestorIds": [
        "BA84E6BB04894DAE868F44497E97770D",
        "4DF45943A7774CC09A5BED3AF2674218",
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "BA84E6BB04894DAE868F44497E97770D"
    },
    "06D359DF2042483BBDA1CF6F92BB6B17": {
      "id": "06D359DF2042483BBDA1CF6F92BB6B17",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 3,
      "listId": "3",
      "statementNotation": "2.1C.1I.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can recognize and express basic opinions and feelings through several-word\nresponses. ",
      "comments": [],
      "ancestorIds": [
        "BA84E6BB04894DAE868F44497E97770D",
        "4DF45943A7774CC09A5BED3AF2674218",
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "BA84E6BB04894DAE868F44497E97770D"
    },
    "713DDEA197B24C91905A3616E60527B3": {
      "id": "713DDEA197B24C91905A3616E60527B3",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 3,
      "listId": "2",
      "statementNotation": "2.1C.1I.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can recognize and state basic personal information. Examples: Name, age,\nfamily members, colors, animals.",
      "comments": [],
      "ancestorIds": [
        "BA84E6BB04894DAE868F44497E97770D",
        "4DF45943A7774CC09A5BED3AF2674218",
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "BA84E6BB04894DAE868F44497E97770D"
    },
    "623B8E574CDD48CA98425BDF0E4EDFB1": {
      "id": "623B8E574CDD48CA98425BDF0E4EDFB1",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 3,
      "listId": "1",
      "statementNotation": "2.1C.1I.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can recognize and express basic greetings and farewells. Examples: Hello and\nGood-bye ",
      "comments": [],
      "ancestorIds": [
        "BA84E6BB04894DAE868F44497E97770D",
        "4DF45943A7774CC09A5BED3AF2674218",
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "BA84E6BB04894DAE868F44497E97770D"
    },
