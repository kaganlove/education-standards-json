      "listId": "",
      "statementNotation": "2.3C.IP",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Proficiency Benchmark: I can access and evaluate basic information and perspectives that are available in Deaf culture.",
      "comments": [],
      "ancestorIds": [
        "D5A28A76F0FF4D9E90142316654A099D",
        "AD31B83073344F04AF7E36C978BD5673"
