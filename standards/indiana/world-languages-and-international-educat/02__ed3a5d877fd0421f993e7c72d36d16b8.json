      "id": "A334B5323DC247B4862670F58FA207E1",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 3,
      "listId": "5",
      "statementNotation": "2.1C.1I.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can express conversational phrases with peers and adults. Examples: (Please,\nbye, thank you, and excuse me).",
      "comments": [],
      "ancestorIds": [
        "BA84E6BB04894DAE868F44497E97770D",
        "4DF45943A7774CC09A5BED3AF2674218",
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "BA84E6BB04894DAE868F44497E97770D"
    },
