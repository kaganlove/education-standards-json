      "listId": "1",
      "statementNotation": "2.1C.1I",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpersonal: Learners use American Sign Language to engage in\nconversation, provide information, express feelings and emotions, and exchange opinions.",
      "comments": [],
      "ancestorIds": [
        "815D0296FE0A4064A502FC92753161AF"
      ],
      "parentId": "815D0296FE0A4064A502FC92753161AF"
