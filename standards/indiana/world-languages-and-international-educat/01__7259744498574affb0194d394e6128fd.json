      "depth": 0,
      "listId": "3",
      "statementNotation": "1.3C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Connect with other disciplines and acquire information and diverse\nperspectives in order to use the language to function academic and career-related situations.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "2D0740338FF745E0BE9F276B0F594AA8": {
