      "asnIdentifier": null,
      "position": 1025,
      "depth": 3,
      "listId": "2",
      "statementNotation": "1.2C.RC.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "I can participate in grade level cultural activities. Examples: Celebrating Clerc\nand Gallaudet Week in December.",
