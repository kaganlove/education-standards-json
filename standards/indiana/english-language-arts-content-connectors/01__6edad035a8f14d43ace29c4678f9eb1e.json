    },
    "1A14C9B865A845C38339451968C5167B": {
      "id": "1A14C9B865A845C38339451968C5167B",
      "asnIdentifier": null,
      "position": 1049,
      "depth": 3,
      "listId": "a",
      "statementNotation": "1.W.3a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Produce (when writing or speaking) a simple sentence that\nidentifies a topic and provides a fact or detail about the topic. (E)",
      "comments": [],
      "ancestorIds": [
        "0E1D1AD590A5441B8FE7BD96913E53C3",
        "9362698FEB5447F1BF569B78AC0ED4D8"
      ],
      "parentId": "0E1D1AD590A5441B8FE7BD96913E53C3"
    },
    "0E1D1AD590A5441B8FE7BD96913E53C3": {
      "id": "0E1D1AD590A5441B8FE7BD96913E53C3",
      "asnIdentifier": null,
      "position": 1048,
      "depth": 2,
      "listId": "3",
      "statementNotation": "1.W.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Produce (when writing or speaking) a topic sentence or main\nidea, provide some facts or details about the topic, and provide a\nconcluding statement. (E) ",
      "comments": [],
      "ancestorIds": [
        "9362698FEB5447F1BF569B78AC0ED4D8"
      ],
      "parentId": null
    },
    "4C0466CCF5C9409E9CF26D05D90E01C6": {
