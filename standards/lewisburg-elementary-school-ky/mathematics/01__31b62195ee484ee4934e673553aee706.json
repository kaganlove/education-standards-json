      "statementNotation": "1.OA.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Relate counting to addition and subtraction (e.g., by counting on 2 to add 2).",
      "comments": [],
      "ancestorIds": [
        "D3DBA2C305DA458D92A8D874C8E51A90"
      ],
      "parentId": "D3DBA2C305DA458D92A8D874C8E51A90"
