      "statementNotation": "LA.6.LS.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use effective timing, volume, tone, and alignment of hand and body gestures to sustain audience interest and attention.",
      "comments": [],
      "ancestorIds": [
        "3DFFCB3B382A43ED9BDED85F6C0EDB2E",
        "490B4EF7C68649B4A6BAAD1E213235B9"
