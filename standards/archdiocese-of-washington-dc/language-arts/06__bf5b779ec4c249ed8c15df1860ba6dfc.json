      "asnIdentifier": null,
      "position": 72000,
      "depth": 2,
      "listId": "17",
      "statementNotation": "LA.6.LS.17",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make descriptive presentations that use concrete sensory details to set forth and support unified impressions of people, places, things, or experiences.",
      "comments": [],
      "ancestorIds": [
        "3DFFCB3B382A43ED9BDED85F6C0EDB2E",
        "490B4EF7C68649B4A6BAAD1E213235B9"
