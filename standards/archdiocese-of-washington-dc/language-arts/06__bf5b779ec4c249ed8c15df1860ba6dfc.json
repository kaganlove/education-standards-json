    },
    "A72B0B84F284466B9C01EF4B843BD91D": {
      "id": "A72B0B84F284466B9C01EF4B843BD91D",
      "asnIdentifier": null,
      "position": 71000,
      "depth": 2,
      "listId": "16",
      "statementNotation": "LA.6.LS.16",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Identify powerful techniques used to influence readers or viewers and evaluate evidence used to support these techniques.",
      "comments": [],
      "ancestorIds": [
        "3DFFCB3B382A43ED9BDED85F6C0EDB2E",
        "490B4EF7C68649B4A6BAAD1E213235B9"
