    "A0D0C053A8074A6E9A34CF268EE14F0A": {
      "id": "A0D0C053A8074A6E9A34CF268EE14F0A",
      "asnIdentifier": null,
      "position": 40000,
      "depth": 3,
      "listId": "c",
      "statementNotation": "M-1-4B-1c",
      "statementLabel": null,
      "description": "The numbers 10, 20, 30, 40, 50, 60, 70, 80, 90 refer to one, two, three, four, five, six, seven, eight, or nine tens (and 0 ones).",
      "ancestorIds": [
        "B8A8C6938EBB4320A9D61F1760244AFD",
