    },
    "91E9E68DB6534FDFA8822DD9AA55F7F3": {
      "id": "91E9E68DB6534FDFA8822DD9AA55F7F3",
      "asnIdentifier": null,
      "position": 39001,
      "depth": 3,
      "listId": "b",
      "statementNotation": "M-1-4B-1b",
      "statementLabel": null,
      "description": "The numbers from 11 to 19 are composed of a ten and one, two, three, four, five, six, seven, eight, or nine ones.",
      "ancestorIds": [
        "B8A8C6938EBB4320A9D61F1760244AFD",
        "CDA449378451479BA5709DD9E5603B43",
        "18D1E0E321C64393BC4FB056ACBAEDB0"
