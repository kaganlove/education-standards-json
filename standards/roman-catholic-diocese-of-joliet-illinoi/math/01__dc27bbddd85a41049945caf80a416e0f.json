      "parentId": "B8A8C6938EBB4320A9D61F1760244AFD"
    },
    "23D2EAC714874BD88748A96221B59E37": {
      "id": "23D2EAC714874BD88748A96221B59E37",
      "asnIdentifier": null,
      "position": 39000,
      "depth": 3,
      "listId": "a",
      "statementNotation": "M-1-4B-1a",
      "statementLabel": null,
      "description": "10 can be thought of as a bundle of ten ones — called a “ten.” ",
      "ancestorIds": [
        "B8A8C6938EBB4320A9D61F1760244AFD",
        "CDA449378451479BA5709DD9E5603B43",
