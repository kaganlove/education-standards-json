      "asnIdentifier": null,
      "position": 18000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "5.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Il travaille la correction textuelle et orthographique de son texte d’abord sur des passages ciblés.",
      "comments": [],
      "ancestorIds": [
        "A80D4B9E89194BA483EED5E3232595FC"
      ],
      "parentId": "A80D4B9E89194BA483EED5E3232595FC"
