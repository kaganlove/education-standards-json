      "depth": 0,
      "listId": "5",
      "statementNotation": "5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Prendre en compte les normes de l'écrit pour formuler, transcrire et réviser\r\n",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
