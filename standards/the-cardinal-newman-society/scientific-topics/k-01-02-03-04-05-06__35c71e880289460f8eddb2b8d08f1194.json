    "800B6F10C654439DB0BB773F1E232393": {
      "id": "800B6F10C654439DB0BB773F1E232393",
      "asnIdentifier": null,
      "position": 20000,
      "depth": 1,
      "listId": "DS4",
      "statementNotation": "CS S.K6 DS4",
      "statementLabel": null,
      "description": "Accept that scientific knowledge is a call to serve and not simply a means to gain power, material prosperity, or\nsuccess.",
      "ancestorIds": [
        "7109753ADD3F4763A5D0641EA66A60DC"
      ],
      "parentId": "7109753ADD3F4763A5D0641EA66A60DC"
    },
    "56E6069CB8EF4C38A4E1241A36879367": {
      "id": "56E6069CB8EF4C38A4E1241A36879367",
      "asnIdentifier": null,
      "position": 19000,
      "depth": 1,
      "listId": "DS3",
      "statementNotation": "CS S.K6 DS3",
      "statementLabel": null,
      "description": "Accept the premise that nature should not be manipulated simply at man’s will or only viewed as a thing to be\nused, but that man must cooperate with God’s plan for himself and for nature.",
      "ancestorIds": [
        "7109753ADD3F4763A5D0641EA66A60DC"
      ],
      "parentId": "7109753ADD3F4763A5D0641EA66A60DC"
    },
    "A9835A93E83847729C69F13D6C7D9DE7": {
      "id": "A9835A93E83847729C69F13D6C7D9DE7",
      "asnIdentifier": null,
      "position": 18000,
      "depth": 1,
      "listId": "DS2",
      "statementNotation": "CS S.K6 DS2",
      "statementLabel": null,
      "description": "Share concern and care for the environment as a part of God’s creation.",
      "ancestorIds": [
        "7109753ADD3F4763A5D0641EA66A60DC"
      ],
      "parentId": "7109753ADD3F4763A5D0641EA66A60DC"
    },
    "9A5EF56A259B43E29B6B51A3ABA0DC1C": {
      "id": "9A5EF56A259B43E29B6B51A3ABA0DC1C",
      "asnIdentifier": null,
      "position": 17000,
      "depth": 1,
      "listId": "DS1",
      "statementNotation": "CS S.K6 DS1",
      "statementLabel": null,
      "description": "Display a sense of wonder and delight about the natural universe and its beauty.",
      "ancestorIds": [
        "7109753ADD3F4763A5D0641EA66A60DC"
      ],
      "parentId": "7109753ADD3F4763A5D0641EA66A60DC"
    },
