      "asnIdentifier": null,
      "position": 5000,
      "depth": 1,
      "listId": "IS1",
      "statementNotation": "CS S.K6 IS1",
      "statementLabel": null,
      "description": "Explain what it means to say that God created the world and all matter out of nothing at a certain point in time;\nhow it manifests His wisdom, glory, and purpose; and how He holds everything in existence according to His plan.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
