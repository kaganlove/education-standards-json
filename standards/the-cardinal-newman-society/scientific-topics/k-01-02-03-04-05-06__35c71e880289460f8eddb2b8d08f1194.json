    "5CBC553336D74B01A3CA8A95448B2E88": {
      "id": "5CBC553336D74B01A3CA8A95448B2E88",
      "asnIdentifier": null,
      "position": 15000,
      "depth": 1,
      "listId": "IS11",
      "statementNotation": "CS S.K6 IS11",
      "statementLabel": null,
      "description": "List the basic contributions of significant Catholics to science such as Galileo, Copernicus, Mendel, and others. ",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "E03B94D9606D46E2AF7AAB11B48F2706": {
      "id": "E03B94D9606D46E2AF7AAB11B48F2706",
      "asnIdentifier": null,
      "position": 14000,
      "depth": 1,
      "listId": "IS10",
      "statementNotation": "CS S.K6 IS10",
      "statementLabel": null,
      "description": "Analyze the false assumption that science can replace faith.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "CCF3DB9B191944FAAD72D95714D80FC6": {
      "id": "CCF3DB9B191944FAAD72D95714D80FC6",
      "asnIdentifier": null,
      "position": 13000,
      "depth": 1,
      "listId": "IS9",
      "statementNotation": "CS S.K6 IS9",
      "statementLabel": null,
      "description": "Describe how the use of the scientific method to explore and understand nature differs, yet complements, the\ntheological and philosophical questions one asks in order to understand God and His works. ",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "FA1F1A71B4BE405397F351C92512B19C": {
      "id": "FA1F1A71B4BE405397F351C92512B19C",
      "asnIdentifier": null,
      "position": 12000,
      "depth": 1,
      "listId": "IS8",
      "statementNotation": "CS S.K6 IS8",
      "statementLabel": null,
      "description": "Explain how science properly limits its focus to “how” things physically exist and is not designed to answer issues\nof meaning, the value of things, or the mysteries of the human person.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "A7E4A515ABA04FD9A466D734623FA04D": {
      "id": "A7E4A515ABA04FD9A466D734623FA04D",
      "asnIdentifier": null,
      "position": 11000,
      "depth": 1,
      "listId": "IS7",
      "statementNotation": "CS S.K6 IS7",
      "statementLabel": null,
      "description": "Describe how science and technology should always be at the service of humanity and, ultimately, to God, in\nharmony with His purposes.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "C33552041EFD473BA64288992E468503": {
      "id": "C33552041EFD473BA64288992E468503",
      "asnIdentifier": null,
      "position": 10000,
      "depth": 1,
      "listId": "IS6",
      "statementNotation": "CS S.K6 IS6",
      "statementLabel": null,
      "description": "Describe God’s relationship with man and nature. ",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "7F920717617743B89D1AF4BC1C7FEB00": {
      "id": "7F920717617743B89D1AF4BC1C7FEB00",
      "asnIdentifier": null,
      "position": 9000,
      "depth": 1,
      "listId": "IS5",
      "statementNotation": "CS S.K6 IS5",
      "statementLabel": null,
      "description": "Explain the processes of conservation, preservation, overconsumption, and stewardship in relation to caring for\nthat which God has given to sustain and delight us.  ",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "7888C38134FE412897D647EB38000238": {
      "id": "7888C38134FE412897D647EB38000238",
      "asnIdentifier": null,
      "position": 8000,
      "depth": 1,
      "listId": "IS4",
      "statementNotation": "CS S.K6 IS4",
      "statementLabel": null,
      "description": "Give examples of the beauty evident in God’s creation.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "49BD2FDDD3DA4D24A503B960CF70B59B": {
      "id": "49BD2FDDD3DA4D24A503B960CF70B59B",
      "asnIdentifier": null,
      "position": 7000,
      "depth": 1,
      "listId": "IS3",
      "statementNotation": "CS S.K6 IS3",
      "statementLabel": null,
      "description": "Explain how creation is an outward sign of God’s love and goodness and, therefore, is “sacramental” in nature.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "39DC4519CB5C41F996726CAEA39347F8": {
      "id": "39DC4519CB5C41F996726CAEA39347F8",
      "asnIdentifier": null,
      "position": 6000,
      "depth": 1,
      "listId": "IS2",
      "statementNotation": "CS S.K6 IS2",
      "statementLabel": null,
      "description": "Describe the relationships, elements, underlying order, harmony, and meaning in God’s creation.",
      "ancestorIds": [
        "FEC87E2037354B8289BE96091AB616BA"
      ],
      "parentId": "FEC87E2037354B8289BE96091AB616BA"
    },
    "56FA48345A984F89AAC2862F189A449C": {
