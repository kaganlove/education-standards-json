  "standards": {
    "F183E8852D2E4D1F9AE576EF08DA8D62": {
      "id": "F183E8852D2E4D1F9AE576EF08DA8D62",
      "asnIdentifier": null,
      "position": 23000,
      "depth": 1,
      "listId": "5",
      "statementNotation": "A.20",
      "statementLabel": null,
      "description": "Students will plan and carry out collective\naction against bias and injustice in the world\nand will evaluate what strategies are most\neffective.",
      "ancestorIds": [
        "7442879E59B841278E797A31CCD4304E"
      ],
      "parentId": "7442879E59B841278E797A31CCD4304E"
    },
    "D6E32C706B874FB1B764EE9B57609D2B": {
      "id": "D6E32C706B874FB1B764EE9B57609D2B",
      "asnIdentifier": null,
      "position": 22000,
      "depth": 1,
      "listId": "4",
      "statementNotation": "A.19",
      "statementLabel": null,
      "description": "Students will make principled decisions about\nwhen and how to take a stand against bias and\ninjustice in their everyday lives and will do so\ndespite negative peer or group pressure.",
      "ancestorIds": [
        "7442879E59B841278E797A31CCD4304E"
      ],
      "parentId": "7442879E59B841278E797A31CCD4304E"
    },
    "E7A48FC5D92E4AFDA68ECDC00FF11BC8": {
      "id": "E7A48FC5D92E4AFDA68ECDC00FF11BC8",
      "asnIdentifier": null,
      "position": 21000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "A.18",
      "statementLabel": null,
      "description": ". Students will speak up with courage and\nrespect when they or someone else has been\nhurt or wronged by bias.",
      "ancestorIds": [
        "7442879E59B841278E797A31CCD4304E"
      ],
      "parentId": "7442879E59B841278E797A31CCD4304E"
    },
    "DD57E48D5B0A4C4D99969A02532AD76C": {
      "id": "DD57E48D5B0A4C4D99969A02532AD76C",
      "asnIdentifier": null,
      "position": 20000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "A.17",
      "statementLabel": null,
      "description": "Students will recognize their own\nresponsibility to stand up to exclusion,\nprejudice and injustice.",
      "ancestorIds": [
        "7442879E59B841278E797A31CCD4304E"
      ],
      "parentId": "7442879E59B841278E797A31CCD4304E"
    },
    "7447BD7147B5434280D6F97A935D3D73": {
      "id": "7447BD7147B5434280D6F97A935D3D73",
      "asnIdentifier": null,
      "position": 19000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "A.16",
      "statementLabel": null,
      "description": "Students will express empathy when people\nare excluded or mistreated because of their\nidentities and concern when they themselves\nexperience bias.",
      "ancestorIds": [
        "7442879E59B841278E797A31CCD4304E"
      ],
      "parentId": "7442879E59B841278E797A31CCD4304E"
    },
    "7442879E59B841278E797A31CCD4304E": {
      "id": "7442879E59B841278E797A31CCD4304E",
      "asnIdentifier": null,
      "position": 18000,
      "depth": 0,
      "listId": "A",
      "statementNotation": "",
      "statementLabel": null,
      "description": "Action",
      "ancestorIds": [],
      "parentId": null
    },
    "1BC67218872D49AA994DB01900D92A92": {
      "id": "1BC67218872D49AA994DB01900D92A92",
      "asnIdentifier": null,
      "position": 17000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "J.15",
      "statementLabel": null,
      "description": "Students will identify figures, groups, events\nand a variety of strategies and philosophies\nrelevant to the history of social justice around\nthe world. ",
      "ancestorIds": [
        "0E54818510314523B8D42E6ED7D41149"
      ],
      "parentId": "0E54818510314523B8D42E6ED7D41149"
    },
    "BAA19E2D4B3B4F29A23A61EB01EBA10C": {
      "id": "BAA19E2D4B3B4F29A23A61EB01EBA10C",
      "asnIdentifier": null,
      "position": 16000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "J.14",
      "statementLabel": null,
      "description": "Students will recognize that power and\nprivilege influence relationships on\ninterpersonal, intergroup and institutional\nlevels and consider how they have been\naffected by those dynamics.",
      "ancestorIds": [
        "0E54818510314523B8D42E6ED7D41149"
      ],
      "parentId": "0E54818510314523B8D42E6ED7D41149"
    },
    "6A7318773929475D9D93923C06AC2636": {
      "id": "6A7318773929475D9D93923C06AC2636",
      "asnIdentifier": null,
      "position": 15000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "J.13",
      "statementLabel": null,
      "description": "Students will analyze the harmful impact of\nbias and injustice on the world, historically and\ntoday",
      "ancestorIds": [
        "0E54818510314523B8D42E6ED7D41149"
      ],
      "parentId": "0E54818510314523B8D42E6ED7D41149"
    },
    "D2C84D7A4E994C91AE23C32140329F01": {
      "id": "D2C84D7A4E994C91AE23C32140329F01",
      "asnIdentifier": null,
      "position": 14000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "J.12",
      "statementLabel": null,
      "description": "Students will recognize unfairness on the\nindividual level (e.g., biased speech) and\ninjustice at the institutional or systemic level\n(e.g., discrimination).",
      "ancestorIds": [
        "0E54818510314523B8D42E6ED7D41149"
      ],
      "parentId": "0E54818510314523B8D42E6ED7D41149"
    },
    "8882CA03EA2348E1A46AF9C30D26F46B": {
      "id": "8882CA03EA2348E1A46AF9C30D26F46B",
      "asnIdentifier": null,
      "position": 13000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "J.11",
      "statementLabel": null,
      "description": "Students will recognize stereotypes and\nrelate to people as individuals rather than\nrepresentatives of groups.",
      "ancestorIds": [
        "0E54818510314523B8D42E6ED7D41149"
      ],
      "parentId": "0E54818510314523B8D42E6ED7D41149"
    },
    "0E54818510314523B8D42E6ED7D41149": {
      "id": "0E54818510314523B8D42E6ED7D41149",
      "asnIdentifier": null,
      "position": 12000,
      "depth": 0,
      "listId": "J",
      "statementNotation": "",
      "statementLabel": null,
      "description": "Justice",
      "ancestorIds": [],
      "parentId": null
    },
    "CB1D44DE5CD440F6A46940CE8FCDAAD4": {
      "id": "CB1D44DE5CD440F6A46940CE8FCDAAD4",
      "asnIdentifier": null,
      "position": 11000,
      "depth": 1,
      "listId": "5",
      "statementNotation": "D.10",
      "statementLabel": null,
      "description": "Students will examine diversity in social,\ncultural, political and historical contexts\nrather than in ways that are superficial or\noversimplified. ",
      "ancestorIds": [
        "3A30D4CF74C24C6B906A17B503BAFA6B"
      ],
      "parentId": "3A30D4CF74C24C6B906A17B503BAFA6B"
    },
    "576559F10A7F4214AFFB1A5C2C5EBC64": {
      "id": "576559F10A7F4214AFFB1A5C2C5EBC64",
      "asnIdentifier": null,
      "position": 10000,
      "depth": 1,
      "listId": "4",
      "statementNotation": "D.9",
      "statementLabel": null,
      "description": "Students will respond to diversity by building\nempathy, respect, understanding and\nconnection",
      "ancestorIds": [
        "3A30D4CF74C24C6B906A17B503BAFA6B"
      ],
      "parentId": "3A30D4CF74C24C6B906A17B503BAFA6B"
    },
    "F37FA57D66884039B1B697F9C10572AF": {
      "id": "F37FA57D66884039B1B697F9C10572AF",
      "asnIdentifier": null,
      "position": 9000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "D.8",
      "statementLabel": null,
      "description": "Students will respectfully express curiosity\nabout the history and lived experiences of\nothers and will exchange ideas and beliefs in an\nopen-minded way.",
      "ancestorIds": [
        "3A30D4CF74C24C6B906A17B503BAFA6B"
      ],
      "parentId": "3A30D4CF74C24C6B906A17B503BAFA6B"
    },
    "672C553ED5A54E6B953CA3002CC9A9FC": {
      "id": "672C553ED5A54E6B953CA3002CC9A9FC",
      "asnIdentifier": null,
      "position": 8000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "D.7",
      "statementLabel": null,
      "description": "Students will develop language and knowledge\nto accurately and respectfully describe how\npeople (including themselves) are both similar\nto and different from each other and others in\ntheir identity groups.",
      "ancestorIds": [
        "3A30D4CF74C24C6B906A17B503BAFA6B"
      ],
      "parentId": "3A30D4CF74C24C6B906A17B503BAFA6B"
    },
    "56716AE39E114387B6ED62ED2B1D13DA": {
      "id": "56716AE39E114387B6ED62ED2B1D13DA",
      "asnIdentifier": null,
      "position": 7000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "D.6",
      "statementLabel": null,
      "description": "Students will express comfort with people who\nare both similar to and different from them and\nengage respectfully with all people.",
      "ancestorIds": [
        "3A30D4CF74C24C6B906A17B503BAFA6B"
      ],
      "parentId": "3A30D4CF74C24C6B906A17B503BAFA6B"
    },
    "3A30D4CF74C24C6B906A17B503BAFA6B": {
      "id": "3A30D4CF74C24C6B906A17B503BAFA6B",
      "asnIdentifier": null,
      "position": 6000,
      "depth": 0,
      "listId": "D",
      "statementNotation": "",
      "statementLabel": null,
      "description": "Diversity",
      "ancestorIds": [],
      "parentId": null
    },
    "BA68CC0D59114829BB1FEB4F83C1E79D": {
      "id": "BA68CC0D59114829BB1FEB4F83C1E79D",
      "asnIdentifier": null,
      "position": 5000,
      "depth": 1,
      "listId": "5",
      "statementNotation": "I.5",
      "statementLabel": null,
      "description": "Students will recognize traits of the dominant\nculture, their home culture and other cultures\nand understand how they negotiate their own\nidentity in multiple spaces. ",
      "ancestorIds": [
        "57260E51B1BB49758BD08BFB88B9B8B3"
      ],
      "parentId": "57260E51B1BB49758BD08BFB88B9B8B3"
    },
    "FA810A170FAD45A1BDA14E83B1A2E22C": {
      "id": "FA810A170FAD45A1BDA14E83B1A2E22C",
      "asnIdentifier": null,
      "position": 4000,
      "depth": 1,
      "listId": "4",
      "statementNotation": "I.4",
      "statementLabel": null,
      "description": "Students will express pride, confidence and\nhealthy self-esteem without denying the value\nand dignity of other people.",
      "ancestorIds": [
        "57260E51B1BB49758BD08BFB88B9B8B3"
      ],
      "parentId": "57260E51B1BB49758BD08BFB88B9B8B3"
    },
    "E59B3B5383ED4DD49BC2AD17E004912A": {
      "id": "E59B3B5383ED4DD49BC2AD17E004912A",
      "asnIdentifier": null,
      "position": 3000,
      "depth": 1,
      "listId": "3",
      "statementNotation": "I.3",
      "statementLabel": null,
      "description": "Students will recognize that people’s multiple\nidentities interact and create unique and\ncomplex individuals.",
      "ancestorIds": [
        "57260E51B1BB49758BD08BFB88B9B8B3"
      ],
      "parentId": "57260E51B1BB49758BD08BFB88B9B8B3"
    },
    "EA169F89FFD5448BB34288A9ED44AAEA": {
      "id": "EA169F89FFD5448BB34288A9ED44AAEA",
      "asnIdentifier": null,
      "position": 2000,
      "depth": 1,
      "listId": "2",
      "statementNotation": "I.2",
      "statementLabel": null,
      "description": "Students will develop language and historical\nand cultural knowledge that affirm and\naccurately describe their membership in\nmultiple identity groups.",
      "ancestorIds": [
        "57260E51B1BB49758BD08BFB88B9B8B3"
      ],
      "parentId": "57260E51B1BB49758BD08BFB88B9B8B3"
    },
    "F20A48F917EA4B619C3D67F299C1E2B7": {
      "id": "F20A48F917EA4B619C3D67F299C1E2B7",
      "asnIdentifier": null,
      "position": 1000,
      "depth": 1,
      "listId": "1",
      "statementNotation": "I.1",
      "statementLabel": null,
      "description": "Students will develop positive social identities\nbased on their membership in multiple groups\nin society.",
      "ancestorIds": [
        "57260E51B1BB49758BD08BFB88B9B8B3"
      ],
      "parentId": "57260E51B1BB49758BD08BFB88B9B8B3"
    },
