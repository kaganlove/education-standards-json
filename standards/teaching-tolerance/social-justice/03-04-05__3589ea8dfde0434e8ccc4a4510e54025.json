  "document": {
    "title": "Social Justice Standards: The Teaching Tolerance Anti-Bias Framework",
    "sourceURL": "https://www.tolerance.org/sites/default/files/2019-06/TT-Social-Justice-Standards-June-2019.pdf"
  },
  "jurisdiction": {
