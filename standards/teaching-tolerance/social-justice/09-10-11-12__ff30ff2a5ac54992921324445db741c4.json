  "document": {
    "title": "Teaching Tolerance: Anti-Bias Framework",
    "sourceURL": "https://www.learningforjustice.org/sites/default/files/2020-09/TT-Social-Justice-Standards-Anti-bias-framework-2020.pdf"
  },
  "jurisdiction": {
    "id": "B1BF1726D3C24724B651FC3EB2394634",
