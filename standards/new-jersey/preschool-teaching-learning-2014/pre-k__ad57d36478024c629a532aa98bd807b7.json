  "standards": {
    "C04BB7F2F0434536ACBF29ED46DA1F3B": {
      "id": "C04BB7F2F0434536ACBF29ED46DA1F3B",
      "asnIdentifier": null,
      "position": 1018,
      "depth": 1,
      "listId": "3",
      "statementNotation": "9.4.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Demonstrate understanding of what others think and feel\nthrough words or actions (e.g., children act out a story that the\nteacher has told them, mirroring the characters’ emotions). ",
      "comments": [],
      "ancestorIds": [
        "CD1A6F00455A46F48DC92B82AE2ED1C0"
      ],
      "parentId": "CD1A6F00455A46F48DC92B82AE2ED1C0"
    },
    "90BA552A3D864E93A001A5FC19AC9832": {
      "id": "90BA552A3D864E93A001A5FC19AC9832",
      "asnIdentifier": null,
      "position": 1017,
      "depth": 1,
      "listId": "2",
      "statementNotation": "9.4.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make connections between ideas, concepts, and subjects (e.g.,\nchildren take pictures from a field trip or nature walk, and use\nthem to write and illustrate classroom books)",
      "comments": [],
      "ancestorIds": [
        "CD1A6F00455A46F48DC92B82AE2ED1C0"
      ],
      "parentId": "CD1A6F00455A46F48DC92B82AE2ED1C0"
    },
    "CE9F9DA4F29541E99024AFB4E3B5453D": {
      "id": "CE9F9DA4F29541E99024AFB4E3B5453D",
      "asnIdentifier": null,
      "position": 1016,
      "depth": 1,
      "listId": "1",
      "statementNotation": "9.4.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use prior knowledge to understand new experiences or a\nproblem in a new context (e.g., after learning about snakes,\nchildren make comparisons when finding a worm on the\nplayground).",
      "comments": [],
      "ancestorIds": [
        "CD1A6F00455A46F48DC92B82AE2ED1C0"
      ],
      "parentId": "CD1A6F00455A46F48DC92B82AE2ED1C0"
    },
    "CD1A6F00455A46F48DC92B82AE2ED1C0": {
      "id": "CD1A6F00455A46F48DC92B82AE2ED1C0",
      "asnIdentifier": null,
      "position": 1015,
      "depth": 0,
      "listId": "4",
      "statementNotation": "9.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children apply what they have learned to new situations.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "27A3F6247E22448BA4EA29BB7BD7C26E": {
      "id": "27A3F6247E22448BA4EA29BB7BD7C26E",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 1,
      "listId": "4",
      "statementNotation": "9.3.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Reflect on, evaluate, and communicate what was learned (e.g.,\nchildren in the class demonstrating and explaining their project\nto children in a younger group). ",
      "comments": [],
      "ancestorIds": [
        "9899FB938FED4206BC7F404D2E6442A8"
      ],
      "parentId": "9899FB938FED4206BC7F404D2E6442A8"
    },
    "DB28096C99F546D8BD7866D8693B73F6": {
      "id": "DB28096C99F546D8BD7866D8693B73F6",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 1,
      "listId": "3",
      "statementNotation": "9.3.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Predict what will happen next based on prior experience and\nknowledge and test the prediction for accuracy (e.g., raising the\nheight of the ramp to see if the ball will roll farther than when\nthe ramp was lower).",
      "comments": [],
      "ancestorIds": [
        "9899FB938FED4206BC7F404D2E6442A8"
      ],
      "parentId": "9899FB938FED4206BC7F404D2E6442A8"
    },
    "8C7ABD0F560E4388920390038D11E37A": {
      "id": "8C7ABD0F560E4388920390038D11E37A",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 1,
      "listId": "2",
      "statementNotation": "9.3.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use varied strategies to seek or recall information and to find\nanswers (e.g., questioning, trial and error, testing, building on\nideas, finding resources, drawing, or thinking aloud). ",
      "comments": [],
      "ancestorIds": [
        "9899FB938FED4206BC7F404D2E6442A8"
      ],
      "parentId": "9899FB938FED4206BC7F404D2E6442A8"
    },
    "0DD9BA8DF0284697B70F2927EA8A2E8A": {
      "id": "0DD9BA8DF0284697B70F2927EA8A2E8A",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 1,
      "listId": "1",
      "statementNotation": "9.3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recognize a problem and describe or demonstrate ways to solve\nit alone or with others (e.g., “I know! Jamar and I can work\ntogether to clean off the table so that we can have a place to eat\nlunch.”)",
      "comments": [],
      "ancestorIds": [
        "9899FB938FED4206BC7F404D2E6442A8"
      ],
      "parentId": "9899FB938FED4206BC7F404D2E6442A8"
    },
    "9899FB938FED4206BC7F404D2E6442A8": {
      "id": "9899FB938FED4206BC7F404D2E6442A8",
      "asnIdentifier": null,
      "position": 1010,
      "depth": 0,
      "listId": "3",
      "statementNotation": "9.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children identify and solve problems. ",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "B05E11A5042D4BDDAD89416043E869DF": {
      "id": "B05E11A5042D4BDDAD89416043E869DF",
      "asnIdentifier": null,
      "position": 1009,
      "depth": 1,
      "listId": "3",
      "statementNotation": "9.2.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use multiple means of communication to creatively express\nthoughts, ideas, and feelings (e.g., sing a song and act out the\nstory of the life cycle of a butterfly).",
      "comments": [],
      "ancestorIds": [
        "4846091299B84BA6947F3E16E37CAAFC"
      ],
      "parentId": "4846091299B84BA6947F3E16E37CAAFC"
    },
    "C90B88EFDF874491A38F26710A986675": {
      "id": "C90B88EFDF874491A38F26710A986675",
      "asnIdentifier": null,
      "position": 1008,
      "depth": 1,
      "listId": "2",
      "statementNotation": "9.2.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use the imagination to solve problems, use materials, role\nplay, write stories, move the body, or create works of art (e.g.,\ncreate pretend spinach out of torn green construction paper to\nserve for dinner).",
      "comments": [],
      "ancestorIds": [
        "4846091299B84BA6947F3E16E37CAAFC"
      ],
      "parentId": "4846091299B84BA6947F3E16E37CAAFC"
    },
    "F7E2613B15534D6987642FE69AA65865": {
      "id": "F7E2613B15534D6987642FE69AA65865",
      "asnIdentifier": null,
      "position": 1007,
      "depth": 1,
      "listId": "1",
      "statementNotation": "9.2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Show flexibility in approaching tasks by being open to new\nideas (i.e., doesn’t cling to one approach to a task, but is\nwilling to experiment and to risk trying out a new idea or\napproach).",
      "comments": [],
      "ancestorIds": [
        "4846091299B84BA6947F3E16E37CAAFC"
      ],
      "parentId": "4846091299B84BA6947F3E16E37CAAFC"
    },
    "4846091299B84BA6947F3E16E37CAAFC": {
      "id": "4846091299B84BA6947F3E16E37CAAFC",
      "asnIdentifier": null,
      "position": 1006,
      "depth": 0,
      "listId": "2",
      "statementNotation": "9.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children show creativity and imagination.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "CF09B20D97724E86BCFE4EA47BB8D705": {
      "id": "CF09B20D97724E86BCFE4EA47BB8D705",
      "asnIdentifier": null,
      "position": 1005,
      "depth": 1,
      "listId": "5",
      "statementNotation": "9.1.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Bring a teacher-directed or self-initiated task,\nactivity or project to completion (e.g., showing\nthe teacher, “Look—I finished it all by myself!”). ",
      "comments": [],
      "ancestorIds": [
        "1D3FF04CA5254574B377FB67444609AD"
      ],
      "parentId": "1D3FF04CA5254574B377FB67444609AD"
    },
    "FE0BA82B63DD4A399CE3708047E55630": {
      "id": "FE0BA82B63DD4A399CE3708047E55630",
      "asnIdentifier": null,
      "position": 1004,
      "depth": 1,
      "listId": "4",
      "statementNotation": "9.1.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Show persistence when faced with challenging\ntasks and uncertainty, seeking and accepting help\nwhen appropriate (e.g., saying to a friend, ‘This is\nhard. Can you help me figure it out?).  ",
      "comments": [],
      "ancestorIds": [
        "1D3FF04CA5254574B377FB67444609AD"
      ],
      "parentId": "1D3FF04CA5254574B377FB67444609AD"
    },
    "724073B81E6D4292A60B7807ABF95167": {
      "id": "724073B81E6D4292A60B7807ABF95167",
      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "3",
      "statementNotation": "9.1.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Focus attention on tasks and experiences, despite\ninterruptions or distractions (e.g., working hard\non a drawing even when children nearby are\nplaying a game).",
      "comments": [],
      "ancestorIds": [
        "1D3FF04CA5254574B377FB67444609AD"
      ],
      "parentId": "1D3FF04CA5254574B377FB67444609AD"
    },
    "4D3B04789E554027A78A4F93C1D48DCD": {
      "id": "4D3B04789E554027A78A4F93C1D48DCD",
      "asnIdentifier": null,
      "position": 1002,
      "depth": 1,
      "listId": "2",
      "statementNotation": "9.1.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Show curiosity and initiative by choosing to\nexplore a variety of activities and experiences\nwith a willingness to try new challenges (e.g.,\nchoosing harder and harder puzzles).",
      "comments": [],
      "ancestorIds": [
        "1D3FF04CA5254574B377FB67444609AD"
      ],
      "parentId": "1D3FF04CA5254574B377FB67444609AD"
    },
    "4A68A7DA7A314DEC8FBEE2844ABE63B2": {
      "id": "4A68A7DA7A314DEC8FBEE2844ABE63B2",
      "asnIdentifier": null,
      "position": 1001,
      "depth": 1,
      "listId": "1",
      "statementNotation": "9.1.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Make plans and decisions to actively engage in\nlearning (e.g., two children greet each other as they arrive to school and decide that they will\nfinish counting all the bottle caps they collected\nduring choice time.)",
      "comments": [],
      "ancestorIds": [
        "1D3FF04CA5254574B377FB67444609AD"
      ],
      "parentId": "1D3FF04CA5254574B377FB67444609AD"
    },
    "1D3FF04CA5254574B377FB67444609AD": {
      "id": "1D3FF04CA5254574B377FB67444609AD",
      "asnIdentifier": null,
      "position": 1000,
      "depth": 0,
      "listId": "1",
      "statementNotation": "9.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children demonstrate initiative, engagement, and persistence.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    }
  }
}