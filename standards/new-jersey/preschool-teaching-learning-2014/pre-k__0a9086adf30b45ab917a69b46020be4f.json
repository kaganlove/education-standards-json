    },
    "093BCF2DA86746D8B760230E0B128726": {
      "id": "093BCF2DA86746D8B760230E0B128726",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 2,
      "listId": "a",
      "statementNotation": "4.2.1.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "putting together and adding to (e.g., “3 blue pegs, 2 yellow\npegs, 5 pegs altogether.”); and  ",
      "comments": [],
      "ancestorIds": [
        "C4A8D5EA3D31482E95C150ADF0D224CE",
        "8D8A36BAD0714B0998F3F6BEADF62693"
      ],
      "parentId": "C4A8D5EA3D31482E95C150ADF0D224CE"
    },
    "C4A8D5EA3D31482E95C150ADF0D224CE": {
      "id": "C4A8D5EA3D31482E95C150ADF0D224CE",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 1,
      "listId": "1",
      "statementNotation": "4.2.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Represent addition and subtraction by manipulating up to 5\nobjects: ",
      "comments": [],
      "ancestorIds": [
        "8D8A36BAD0714B0998F3F6BEADF62693"
      ],
      "parentId": "8D8A36BAD0714B0998F3F6BEADF62693"
    },
