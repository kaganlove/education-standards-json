      "asnIdentifier": null,
      "position": 1003,
      "depth": 1,
      "listId": "3",
      "statementNotation": "4.1.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Know that written numbers are symbols for number quantities\nand, with support, begin to write numbers from 0 to 10.",
      "comments": [],
      "ancestorIds": [
        "738B8A67E89F443FA247A6BBCFEFC1B7"
      ],
      "parentId": "738B8A67E89F443FA247A6BBCFEFC1B7"
    },
