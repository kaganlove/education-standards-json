    "9066340805634B1E849762A291848A93": {
      "id": "9066340805634B1E849762A291848A93",
      "asnIdentifier": null,
      "position": 1024,
      "depth": 2,
      "listId": "b",
      "statementNotation": "4.4.3.b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "three-dimensional shapes by building with blocks and with\nother materials having height, width and depth (e.g., unit\nblocks, hollow blocks, attribute blocks, boxes, empty food\ncontainers, plastic pipe).",
      "comments": [],
      "ancestorIds": [
        "FC41727438F844B490C14561E7A14A73",
        "327DCD047D6042C1B4B827197BA58B49"
      ],
      "parentId": "FC41727438F844B490C14561E7A14A73"
    },
    "B4FD16616FA44B3FB087F77703107EA4": {
      "id": "B4FD16616FA44B3FB087F77703107EA4",
      "asnIdentifier": null,
      "position": 1023,
      "depth": 2,
      "listId": "a",
      "statementNotation": "4.4.3.a",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "two-dimensional shapes (e.g., use two dimensional shapes\nto make designs, patterns and pictures by manipulating\nmaterials such as paper shapes, puzzle pieces, tangrams;\nconstruct shapes from materials such as straws; match\nidentical shapes; sort shapes based on rules [something that\nmakes them alike/different]; describe shapes by\nsides/angles; use pattern blocks to compose/decompose\nshapes when making and taking apart compositions of\nseveral shapes).  ",
      "comments": [],
      "ancestorIds": [
        "FC41727438F844B490C14561E7A14A73",
        "327DCD047D6042C1B4B827197BA58B49"
      ],
      "parentId": "FC41727438F844B490C14561E7A14A73"
    },
    "FC41727438F844B490C14561E7A14A73": {
      "id": "FC41727438F844B490C14561E7A14A73",
      "asnIdentifier": null,
      "position": 1022,
      "depth": 1,
      "listId": "3",
      "statementNotation": "4.4.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Manipulate, compare and discuss the attributes of: ",
      "comments": [],
      "ancestorIds": [
        "327DCD047D6042C1B4B827197BA58B49"
      ],
      "parentId": "327DCD047D6042C1B4B827197BA58B49"
    },
    "C024270E412246AAA6D05DE6BE38CF29": {
      "id": "C024270E412246AAA6D05DE6BE38CF29",
      "asnIdentifier": null,
      "position": 1021,
      "depth": 1,
      "listId": "2",
      "statementNotation": "4.4.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use accurate terms to name and describe some two-dimensional\nshapes and begin to use accurate terms to name and describe\nsome three-dimensional shapes (e.g., circle, square, triangle,\nsphere, cylinder, cube, side point, angle).",
      "comments": [],
      "ancestorIds": [
        "327DCD047D6042C1B4B827197BA58B49"
      ],
      "parentId": "327DCD047D6042C1B4B827197BA58B49"
    },
    "7EFA2269FBBB4142AC8769D3EAB61AC5": {
      "id": "7EFA2269FBBB4142AC8769D3EAB61AC5",
      "asnIdentifier": null,
      "position": 1020,
      "depth": 1,
      "listId": "1",
      "statementNotation": "4.4.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Respond to and use positional words (e.g., in, under, between,\ndown, behind).",
      "comments": [],
      "ancestorIds": [
        "327DCD047D6042C1B4B827197BA58B49"
      ],
      "parentId": "327DCD047D6042C1B4B827197BA58B49"
    },
    "327DCD047D6042C1B4B827197BA58B49": {
      "id": "327DCD047D6042C1B4B827197BA58B49",
      "asnIdentifier": null,
      "position": 1019,
      "depth": 0,
      "listId": "4",
      "statementNotation": "4.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children develop spatial and geometric sense.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "F05554610A21468E8995D4D1CCAAA4EB": {
      "id": "F05554610A21468E8995D4D1CCAAA4EB",
      "asnIdentifier": null,
      "position": 1018,
      "depth": 1,
      "listId": "3",
      "statementNotation": "4.3.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare (e.g., which container holds more) and order (e.g.,\nshortest to longest) up to 5 objects according to measurable\nattributes.",
      "comments": [],
      "ancestorIds": [
        "891F8D63466E4AA68CE7B45194846D98"
      ],
      "parentId": "891F8D63466E4AA68CE7B45194846D98"
    },
    "E6EC57D9AE9A4EF2ABFD36A175A99853": {
      "id": "E6EC57D9AE9A4EF2ABFD36A175A99853",
      "asnIdentifier": null,
      "position": 1017,
      "depth": 1,
      "listId": "2",
      "statementNotation": "4.3.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Begin to use appropriate vocabulary to demonstrate awareness\nof the measurable attributes of length, area, weight and capacity\nof everyday objects (e.g., long, short, tall, light, heavy, full).",
      "comments": [],
      "ancestorIds": [
        "891F8D63466E4AA68CE7B45194846D98"
      ],
      "parentId": "891F8D63466E4AA68CE7B45194846D98"
    },
    "5024D9048FF64EEE86AACFECE30478DE": {
      "id": "5024D9048FF64EEE86AACFECE30478DE",
      "asnIdentifier": null,
      "position": 1016,
      "depth": 1,
      "listId": "1",
      "statementNotation": "4.3.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Sort, order, pattern, and classify objects by non-measurable\n(e.g., color, texture, type of material) and measurable attributes (e.g., length, capacity, height). ",
      "comments": [],
      "ancestorIds": [
        "891F8D63466E4AA68CE7B45194846D98"
      ],
      "parentId": "891F8D63466E4AA68CE7B45194846D98"
    },
    "891F8D63466E4AA68CE7B45194846D98": {
      "id": "891F8D63466E4AA68CE7B45194846D98",
      "asnIdentifier": null,
      "position": 1015,
      "depth": 0,
      "listId": "3",
      "statementNotation": "4.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Children begin to conceptualize measurable attributes of objects.",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "11E78BEE4C2343A5871B4624ACDC3F65": {
      "id": "11E78BEE4C2343A5871B4624ACDC3F65",
      "asnIdentifier": null,
      "position": 1014,
      "depth": 1,
      "listId": "2",
      "statementNotation": "4.2.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Begin to represent simple word problem data in pictures and\ndrawings.",
      "comments": [],
      "ancestorIds": [
        "8D8A36BAD0714B0998F3F6BEADF62693"
      ],
      "parentId": "8D8A36BAD0714B0998F3F6BEADF62693"
    },
    "23C6CDE125874C2C8F819EE2D95B097E": {
      "id": "23C6CDE125874C2C8F819EE2D95B097E",
      "asnIdentifier": null,
      "position": 1013,
      "depth": 2,
      "listId": "b",
      "statementNotation": "4.2.1.b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "taking apart and taking from (“I have four carrot sticks.\nI’m eating one. Now I have 3.”).",
      "comments": [],
      "ancestorIds": [
        "C4A8D5EA3D31482E95C150ADF0D224CE",
        "8D8A36BAD0714B0998F3F6BEADF62693"
