      "id": "0A1A1E17973C439DBA611BAFF654FC73",
      "asnIdentifier": null,
      "position": 1047,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.IW.6.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Write informative/explanatory texts (including the narration of historical events, scientific\nprocedures/ experiments, or technical processes) to examine a topic and convey ideas, concepts, and\ninformation through the selection, organization, and analysis of relevant content.",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
