  "standards": {
    "CB21A7EF31164F1B9E90CA793B9CAE04": {
      "id": "CB21A7EF31164F1B9E90CA793B9CAE04",
      "asnIdentifier": null,
      "position": 1074,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.AS.6.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Adapt speech to a variety of contexts and tasks, demonstrating command of formal English\nwhen indicated or appropriate.",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "5889B48C00654ABAA7D28BD1A26CB139": {
      "id": "5889B48C00654ABAA7D28BD1A26CB139",
      "asnIdentifier": null,
      "position": 1073,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.UM.6.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Include multimedia components (e.g., graphics, images, music, sound) and visual displays in\npresentations to clarify information.",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "B0B8B0453E904143A166B5F50BE9EA30": {
      "id": "B0B8B0453E904143A166B5F50BE9EA30",
      "asnIdentifier": null,
      "position": 1072,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.PI.6.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Present claims and findings, sequencing ideas logically and using pertinent descriptions, facts,\nand details to accentuate main ideas or themes; use appropriate speaking behaviors (e.g., eye contact,\nadequate volume, and clear pronunciation).",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "B839F0B471D74DBAA967FF4BDC3BB2FF": {
      "id": "B839F0B471D74DBAA967FF4BDC3BB2FF",
      "asnIdentifier": null,
      "position": 1071,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.ES.6.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Deconstruct a speaker’s argument and specific claims, distinguishing claims that are supported\nby reasons and evidence from claims that are not.",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "CBBEB8011DD44D2ABE1A6F6F2398D2C5": {
      "id": "CBBEB8011DD44D2ABE1A6F6F2398D2C5",
      "asnIdentifier": null,
      "position": 1070,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.II.6.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Interpret information presented in diverse media and formats (e.g., visually, quantitatively,\norally) and explain how it contributes to a topic, text, or issue under study.",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "8BB1BA6E20E34F72891004439BE89F45": {
      "id": "8BB1BA6E20E34F72891004439BE89F45",
      "asnIdentifier": null,
      "position": 1069,
      "depth": 4,
      "listId": "D",
      "statementNotation": "SL.PE.6.1.D",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Review the key ideas expressed and demonstrate understanding of multiple perspectives\nthrough reflection and paraphrasing. ",
      "comments": [],
      "ancestorIds": [
        "17BD778CABBA45D69434D87145E0B8A3",
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": "17BD778CABBA45D69434D87145E0B8A3"
    },
    "B7E8C5A066C14498A0615BBA1FC6FCD2": {
      "id": "B7E8C5A066C14498A0615BBA1FC6FCD2",
      "asnIdentifier": null,
      "position": 1068,
      "depth": 4,
      "listId": "C",
      "statementNotation": "SL.PE.6.1.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Pose and respond to specific questions with elaboration and detail by making comments\nthat contribute to the topic, text, or issue under discussion. ",
      "comments": [],
      "ancestorIds": [
        "17BD778CABBA45D69434D87145E0B8A3",
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": "17BD778CABBA45D69434D87145E0B8A3"
    },
    "4C340A4E08794AC280F4AFA9562C5F28": {
      "id": "4C340A4E08794AC280F4AFA9562C5F28",
      "asnIdentifier": null,
      "position": 1067,
      "depth": 4,
      "listId": "B",
      "statementNotation": "SL.PE.6.1.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Follow rules for collegial discussions, set specific goals and deadlines, and define individual\nroles as needed. ",
      "comments": [],
      "ancestorIds": [
        "17BD778CABBA45D69434D87145E0B8A3",
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": "17BD778CABBA45D69434D87145E0B8A3"
    },
    "E78EF5DD6327497891CFC6A899E25F2E": {
      "id": "E78EF5DD6327497891CFC6A899E25F2E",
      "asnIdentifier": null,
      "position": 1066,
      "depth": 4,
      "listId": "A",
      "statementNotation": "SL.PE.6.1.A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Come to discussions prepared, having read or studied required material; explicitly draw on\nthat preparation by referring to evidence on the topic, text, or issue to probe and reflect on\nideas under discussion.",
      "comments": [],
      "ancestorIds": [
        "17BD778CABBA45D69434D87145E0B8A3",
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": "17BD778CABBA45D69434D87145E0B8A3"
    },
    "17BD778CABBA45D69434D87145E0B8A3": {
      "id": "17BD778CABBA45D69434D87145E0B8A3",
      "asnIdentifier": null,
      "position": 1065,
      "depth": 3,
      "listId": "",
      "statementNotation": "SL.PE.6.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Engage effectively in a range of collaborative discussions (one-on-one, in groups, and\nteacher-led) with diverse partners on grade 6 topics, texts, and issues, building on others’ ideas and\nexpressing their own clearly.",
      "comments": [],
      "ancestorIds": [
        "7A04D9AE99AB430C87C901007B3E162A"
      ],
      "parentId": null
    },
    "7A04D9AE99AB430C87C901007B3E162A": {
      "id": "7A04D9AE99AB430C87C901007B3E162A",
      "asnIdentifier": null,
      "position": 1064,
      "depth": 0,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Speaking and Listening Domain",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "BADFE17508C449C68FC8E15B65728B07": {
      "id": "BADFE17508C449C68FC8E15B65728B07",
      "asnIdentifier": null,
      "position": 1063,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.RW.6.7",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Write routinely over extended time frames (time for research, reflection, metacognition/self-correction, and revision) and shorter time frames (a single sitting or a day or two) for a range of\ndiscipline-specific tasks, purposes, and audiences.",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
    "9E3B7DF79AF840C9B3053E614E420A8C": {
      "id": "9E3B7DF79AF840C9B3053E614E420A8C",
      "asnIdentifier": null,
      "position": 1062,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.SE.6.6",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Gather relevant information from multiple print and digital sources; assess the credibility of\neach source; and quote or paraphrase the data and conclusions of others while avoiding plagiarism and\nproviding basic bibliographic information for sources.",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
    "0214C7065E834D37B579D53635B34FB0": {
      "id": "0214C7065E834D37B579D53635B34FB0",
      "asnIdentifier": null,
      "position": 1061,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.WR.6.5",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Conduct short research projects to answer a question, drawing on several sources and\nrefocusing the inquiry when appropriate.",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
    "09EF4C2A18FC4D6F953C3DD2025F42A6": {
      "id": "09EF4C2A18FC4D6F953C3DD2025F42A6",
      "asnIdentifier": null,
      "position": 1060,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.WP.6.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "With some guidance and support from peers and adults, develop and strengthen writing as\nneeded by planning; flexibly making editing and revision choices; sustaining effort to fit composition\nneeds and purposes; and attempting to address purpose and audience. ",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
    "36B524E029404002B382EE62EC586E95": {
      "id": "36B524E029404002B382EE62EC586E95",
      "asnIdentifier": null,
      "position": 1059,
      "depth": 4,
      "listId": "E",
      "statementNotation": "W.NW.6.3.E",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Provide a conclusion that follows from the narrated experiences or events.",
      "comments": [],
      "ancestorIds": [
        "75D51D51261342B1BB5B59E4FC2FDCB8",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "75D51D51261342B1BB5B59E4FC2FDCB8"
    },
    "8F81AA52FF62400EB34BF3E5A1E768C4": {
      "id": "8F81AA52FF62400EB34BF3E5A1E768C4",
      "asnIdentifier": null,
      "position": 1058,
      "depth": 4,
      "listId": "D",
      "statementNotation": "W.NW.6.3.D",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use precise words and phrases, relevant descriptive details, and sensory language to convey\nexperiences and events.",
      "comments": [],
      "ancestorIds": [
        "75D51D51261342B1BB5B59E4FC2FDCB8",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "75D51D51261342B1BB5B59E4FC2FDCB8"
    },
    "039733DA83EF4331AE805D1EAD24C69E": {
      "id": "039733DA83EF4331AE805D1EAD24C69E",
      "asnIdentifier": null,
      "position": 1057,
      "depth": 4,
      "listId": "C",
      "statementNotation": "W.NW.6.3.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use a variety of transition words, phrases, and clauses to convey sequence and signal shifts\nfrom one time frame or setting to another.",
      "comments": [],
      "ancestorIds": [
        "75D51D51261342B1BB5B59E4FC2FDCB8",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "75D51D51261342B1BB5B59E4FC2FDCB8"
    },
    "A4740A5132A441A1A4187B665FD82FF0": {
      "id": "A4740A5132A441A1A4187B665FD82FF0",
      "asnIdentifier": null,
      "position": 1056,
      "depth": 4,
      "listId": "B",
      "statementNotation": "W.NW.6.3.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use narrative techniques, such as dialogue, pacing, and description, to develop experiences,\nevents, and/or characters.",
      "comments": [],
      "ancestorIds": [
        "75D51D51261342B1BB5B59E4FC2FDCB8",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "75D51D51261342B1BB5B59E4FC2FDCB8"
    },
    "C9E02FBEA0054A318FD6EA372B441A78": {
      "id": "C9E02FBEA0054A318FD6EA372B441A78",
      "asnIdentifier": null,
      "position": 1055,
      "depth": 4,
      "listId": "A",
      "statementNotation": "W.NW.6.3.A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Engage and orient the reader by establishing a context and introducing a narrator and/or\ncharacters; organize an event sequence that unfolds naturally and logically. ",
      "comments": [],
      "ancestorIds": [
        "75D51D51261342B1BB5B59E4FC2FDCB8",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "75D51D51261342B1BB5B59E4FC2FDCB8"
    },
    "75D51D51261342B1BB5B59E4FC2FDCB8": {
      "id": "75D51D51261342B1BB5B59E4FC2FDCB8",
      "asnIdentifier": null,
      "position": 1054,
      "depth": 3,
      "listId": "",
      "statementNotation": "W.NW.6.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Write narratives to develop real or imagined experiences or events using effective technique,\nrelevant descriptive details, and well-structured event sequences. ",
      "comments": [],
      "ancestorIds": [
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": null
    },
    "9C1E9DE045D5494F986B893322958192": {
      "id": "9C1E9DE045D5494F986B893322958192",
      "asnIdentifier": null,
      "position": 1053,
      "depth": 4,
      "listId": "F",
      "statementNotation": "W.IW.6.2.F",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Provide a concluding statement or section (e.g., sentence, part of a paragraph, paragraph, or\nmultiple paragraphs) that follows from and supports the information or explanation\npresented. ",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
    "8B1F9D381FBA417A92C05CDC02C0AA96": {
      "id": "8B1F9D381FBA417A92C05CDC02C0AA96",
      "asnIdentifier": null,
      "position": 1052,
      "depth": 4,
      "listId": "E",
      "statementNotation": "W.IW.6.2.E",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Acknowledge and attempt a formal/academic style, approach, and form.",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
    "6A5C609E64C144FD9113BB9C78C38B54": {
      "id": "6A5C609E64C144FD9113BB9C78C38B54",
      "asnIdentifier": null,
      "position": 1051,
      "depth": 4,
      "listId": "D",
      "statementNotation": "W.IW.6.2.D",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use precise language and domain-specific vocabulary to inform about or explain the topic. ",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
    "4EEFCD82CD804A89AF81767EA32223E4": {
      "id": "4EEFCD82CD804A89AF81767EA32223E4",
      "asnIdentifier": null,
      "position": 1050,
      "depth": 4,
      "listId": "C",
      "statementNotation": "W.IW.6.2.C",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use appropriate transitions to clarify the relationships among ideas and concepts.",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
    "A93EFA41E040423B90E970057B72DAB8": {
      "id": "A93EFA41E040423B90E970057B72DAB8",
      "asnIdentifier": null,
      "position": 1049,
      "depth": 4,
      "listId": "B",
      "statementNotation": "W.IW.6.2.B",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Develop the topic with relevant facts, definitions, concrete details, quotations, or other\ninformation and examples. ",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
    "74698E548ECD44E5AC7BAEA1EA383263": {
      "id": "74698E548ECD44E5AC7BAEA1EA383263",
      "asnIdentifier": null,
      "position": 1048,
      "depth": 4,
      "listId": "A",
      "statementNotation": "W.IW.6.2.A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Introduce a topic and organize ideas, concepts, and information, using text structures (e.g.,\ndefinition, classification, comparison/contrast, cause/effect, etc.) and text features (e.g.,\nheadings, graphics, and multimedia) when useful to aid in comprehension.",
      "comments": [],
      "ancestorIds": [
        "0A1A1E17973C439DBA611BAFF654FC73",
        "E3A96AEA76424DD5BCB65FC807CFD5AB"
      ],
      "parentId": "0A1A1E17973C439DBA611BAFF654FC73"
    },
