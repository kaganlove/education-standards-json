      "depth": 3,
      "listId": "",
      "statementNotation": "8.1.2.DA.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Collect and present data, including climate change data, in various visual formats.",
      "comments": [],
      "ancestorIds": [
        "6FF49EF6A7C3426F8BF0413F18F8D247",
        "F318DF63D07C4CFBA4F36EFD7878BE8E",
