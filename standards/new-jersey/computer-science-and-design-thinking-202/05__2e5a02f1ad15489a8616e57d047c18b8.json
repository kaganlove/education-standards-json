      "depth": 2,
      "listId": "1",
      "statementNotation": "2.CS.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Individuals use computing\ndevices to perform a variety\nof tasks accurately and\nquickly. Computing devices\ninterpret and follow the\ninstructions they are given\nliterally.",
      "comments": [],
      "ancestorIds": [
        "5E588456E5C74CD9AAEC2A9474B77D8D",
        "DE0350E88E694295A3D43879627CA618"
