      "depth": 0,
      "listId": "",
      "statementNotation": "2.DA",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Data & Analysis",
      "comments": [],
      "ancestorIds": [],
      "parentId": null
    },
    "67CDA713BBDB485F9599C5B7DB7468A3": {
      "id": "67CDA713BBDB485F9599C5B7DB7468A3",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 3,
      "listId": "",
      "statementNotation": "8.1.2.IC.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Compare how individuals live and work before and after the implementation of new\ncomputing technology.",
      "comments": [],
      "ancestorIds": [
        "9717621EC5064FADB2AE1A25E7805275",
        "D124E232BBF24762AA3D0D5589C62893",
        "66CB0F846C5242DC8CE04F9EAADB435B"
      ],
      "parentId": "9717621EC5064FADB2AE1A25E7805275"
    },
    "9717621EC5064FADB2AE1A25E7805275": {
      "id": "9717621EC5064FADB2AE1A25E7805275",
      "asnIdentifier": null,
      "position": 1012,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.IC.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Computing technology has\npositively and negatively changed\nthe way individuals live and work\n(e.g., entertainment,\ncommunication, productivity\ntools). ",
      "comments": [],
      "ancestorIds": [
        "D124E232BBF24762AA3D0D5589C62893",
        "66CB0F846C5242DC8CE04F9EAADB435B"
      ],
      "parentId": "D124E232BBF24762AA3D0D5589C62893"
    },
    "D124E232BBF24762AA3D0D5589C62893": {
      "id": "D124E232BBF24762AA3D0D5589C62893",
      "asnIdentifier": null,
      "position": 1011,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Computing affects many aspects of the world in both positive and negative ways at local, national, and global levels. Individuals and communities\ninfluence computing through their behaviors and cultural and social interactions, and, in turn, computing influences new cultural practices. ",
      "comments": [],
      "ancestorIds": [
        "66CB0F846C5242DC8CE04F9EAADB435B"
      ],
      "parentId": "66CB0F846C5242DC8CE04F9EAADB435B"
    },
