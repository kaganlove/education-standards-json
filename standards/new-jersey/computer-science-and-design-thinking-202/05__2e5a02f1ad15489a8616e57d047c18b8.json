      "asnIdentifier": null,
      "position": 1020,
      "depth": 2,
      "listId": "1",
      "statementNotation": "2.AP.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Individuals develop and\nfollow directions as part of\ndaily life. A sequence of steps can be expressed as an algorithm that a computer can process.",
      "comments": [],
      "ancestorIds": [
        "8CDBFC18255D433C91ED85BBCAD6351B",
        "4D725EAE3D7F47A2B65689472BEC9A46"
      ],
      "parentId": "8CDBFC18255D433C91ED85BBCAD6351B"
    },
    "8CDBFC18255D433C91ED85BBCAD6351B": {
      "id": "8CDBFC18255D433C91ED85BBCAD6351B",
      "asnIdentifier": null,
      "position": 1019,
      "depth": 1,
      "listId": "",
      "statementNotation": "",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "An algorithm is a sequence of steps designed to accomplish a specific task. Algorithms are translated into programs, or code, to provide instructions for\ncomputing devices. Algorithms and programming control all computing systems, empowering people to communicate with the world in new ways and\nsolve compelling problems.  ",
      "comments": [],
      "ancestorIds": [
        "4D725EAE3D7F47A2B65689472BEC9A46"
      ],
      "parentId": "4D725EAE3D7F47A2B65689472BEC9A46"
    },
