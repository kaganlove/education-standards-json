      "depth": 3,
      "listId": "",
      "statementNotation": "8.1.2.AP.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Model daily processes by creating and following algorithms to complete tasks.",
      "comments": [],
      "ancestorIds": [
        "A5E568774C7C4BC19FD80B73793C04A5",
        "8CDBFC18255D433C91ED85BBCAD6351B",
        "4D725EAE3D7F47A2B65689472BEC9A46"
