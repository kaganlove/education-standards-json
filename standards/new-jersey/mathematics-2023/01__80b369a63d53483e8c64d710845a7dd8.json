    "27EAB8EA8EB449909527BECB00C0FDA4": {
      "id": "27EAB8EA8EB449909527BECB00C0FDA4",
      "asnIdentifier": null,
      "position": 1024,
      "depth": 2,
      "listId": "2",
      "statementNotation": "1.NBT.C.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Given a two-digit number, mentally find 10 more or 10 less than the number, without\nhaving to count; explain the reasoning used.",
      "comments": [],
      "ancestorIds": [
        "CE02EE1A740245108573340A2350217A",
        "95C06680C8064D24B2DFD86B6474DA1C"
