    "DF49E206FC0B4BBF8778CE99CE5BBF15": {
      "id": "DF49E206FC0B4BBF8778CE99CE5BBF15",
      "asnIdentifier": null,
      "position": 1034,
      "depth": 2,
      "listId": "2",
      "statementNotation": "1.M.C.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use dollars in the solutions of problems up to $20. Find equivalent monetary values (e.g.,\na nickel is equivalent in value to five pennies). Show monetary values in multiple ways.\nFor example, show 25¢ as two dimes and one nickel, and as five nickels. Show $20 as\ntwo tens and as 20 ones.",
      "comments": [],
      "ancestorIds": [
