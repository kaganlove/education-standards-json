      "asnIdentifier": null,
      "position": 1015,
      "depth": 2,
      "listId": "2",
      "statementNotation": "A.CED.A.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Create equations in two or more variables to represent relationships between quantities;\ngraph equations on coordinate axes with labels and scales. ",
      "comments": [],
      "ancestorIds": [
        "043BE8BAC69F412E9C0D7F8EEBED73FD",
        "E45E5EC7C86540CC932B37E5174A61ED"
      ],
      "parentId": "043BE8BAC69F412E9C0D7F8EEBED73FD"
    },
