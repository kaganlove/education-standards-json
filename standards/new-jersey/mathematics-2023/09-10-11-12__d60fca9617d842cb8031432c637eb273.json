    "8856A8F78EE042BAA7CAD830CF054F6B": {
      "id": "8856A8F78EE042BAA7CAD830CF054F6B",
      "asnIdentifier": null,
      "position": 1042,
      "depth": 2,
      "listId": "2",
      "statementNotation": "A.SSE.A.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use the structure of an expression to identify ways to rewrite it. For example, see x^4-y^4 as (x^2 )^2-(y^2 )^2, thus recognizing it as a difference of squares that can be factored as (x^2-y^2 )(x^2+y^2).",
      "comments": [],
      "ancestorIds": [
        "2DC52274DCA44BB49770EFB9BA13E3B1",
        "6E5AB7CB4CC84CE3BAE901CB4362E67A"
      ],
      "parentId": "2DC52274DCA44BB49770EFB9BA13E3B1"
    },
    "E3CABA204B1A474381B14CA4B0C0153E": {
