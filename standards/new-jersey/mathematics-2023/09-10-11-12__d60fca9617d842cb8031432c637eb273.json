    },
    "0FEA69145EC249FA85C31BA76E79E40B": {
      "id": "0FEA69145EC249FA85C31BA76E79E40B",
      "asnIdentifier": null,
      "position": 1016,
      "depth": 2,
      "listId": "3",
      "statementNotation": "A.CED.A.3",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Represent constraints by equations or inequalities, and by systems of equations and/or\ninequalities, and interpret solutions as viable or nonviable options in a modeling context.\nFor example, represent inequalities describing nutritional and cost constraints on\ncombinations of different foods. ",
      "comments": [],
      "ancestorIds": [
        "043BE8BAC69F412E9C0D7F8EEBED73FD",
        "E45E5EC7C86540CC932B37E5174A61ED"
      ],
      "parentId": "043BE8BAC69F412E9C0D7F8EEBED73FD"
    },
    "A2D58659CBC04A12981608013916D474": {
