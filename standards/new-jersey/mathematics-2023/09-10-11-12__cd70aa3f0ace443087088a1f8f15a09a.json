      "asnIdentifier": null,
      "position": 1027,
      "depth": 2,
      "listId": "1",
      "statementNotation": "S.CP.A.1",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Describe events as subsets of a sample space (the set of outcomes) using characteristics\n(or categories) of the outcomes, or as unions, intersections, or complements of other\nevents (“or,” “and,” “not”).",
      "comments": [],
      "ancestorIds": [
        "CA597315669544EBAA42FAC9C2F22A8F",
        "F23B1CC7E2AE4C4FB9273EEF22C1AAFE"
      ],
      "parentId": "CA597315669544EBAA42FAC9C2F22A8F"
    },
