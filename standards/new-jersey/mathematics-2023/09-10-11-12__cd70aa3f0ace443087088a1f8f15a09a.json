    },
    "81BF1265E9924D38A81420F2E327E3B1": {
      "id": "81BF1265E9924D38A81420F2E327E3B1",
      "asnIdentifier": null,
      "position": 1047,
      "depth": 2,
      "listId": "2",
      "statementNotation": "S.MD.B.2",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use probabilities to make fair decisions (e.g., drawing by lots, using a random number\ngenerator). ",
      "comments": [],
      "ancestorIds": [
