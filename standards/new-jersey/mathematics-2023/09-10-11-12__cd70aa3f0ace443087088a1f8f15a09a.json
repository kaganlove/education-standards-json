    "3C444D7260C842C28C0064A4F86C7D15": {
      "id": "3C444D7260C842C28C0064A4F86C7D15",
      "asnIdentifier": null,
      "position": 1036,
      "depth": 2,
      "listId": "4",
      "statementNotation": "S.CP.B.4",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Use permutations and combinations to compute probabilities of compound events and\nsolve problems.",
      "comments": [],
      "ancestorIds": [
        "F8CD061628004D7689A6AEE15316544D",
        "F23B1CC7E2AE4C4FB9273EEF22C1AAFE"
      ],
