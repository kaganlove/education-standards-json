      "position": 1026,
      "depth": 1,
      "listId": "A",
      "statementNotation": "S.CP.A",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Understand independence and conditional probability and use them to interpret data",
      "comments": [],
      "ancestorIds": [
        "F23B1CC7E2AE4C4FB9273EEF22C1AAFE"
      ],
      "parentId": "F23B1CC7E2AE4C4FB9273EEF22C1AAFE"
