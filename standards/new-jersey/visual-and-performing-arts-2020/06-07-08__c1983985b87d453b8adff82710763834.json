      "depth": 2,
      "listId": "c",
      "statementNotation": "1.1.8.Pr6c",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Apply feedback to make changes to and adapt movements to performance area. Use\nperformance etiquette and performance practices during class, rehearsal and performance. Accept\nnotes from the choreographer, apply corrections and document performance process using dance and\nproduction terminology. Analyze and evaluate the success of a performance.",
      "comments": [],
      "ancestorIds": [
        "F925EB1FCF644E2E888666D5D0410BD7",
        "44DE312551C049F0B4B76875BAB30F83"
