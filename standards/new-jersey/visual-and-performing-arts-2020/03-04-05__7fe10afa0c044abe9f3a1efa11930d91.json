    "3424FDB97D484027A8C7B323C3984EE6": {
      "id": "3424FDB97D484027A8C7B323C3984EE6",
      "asnIdentifier": null,
      "position": 10008,
      "depth": 2,
      "listId": "b",
      "statementNotation": "1.1.5.Pr5b",
      "altStatementNotation": null,
      "statementLabel": null,
      "description": "Recall joint actions, articulations and basic anatomical terms (e.g., muscles, bones,\ntendons, ligaments) as they relate to dance and apply basic kinesthetic principles (e.g.,\nflexion/extension, inward/outward rotation). Track how basic body organs (e.g., lungs, heart, brain)\nrespond to different intensities of dance movement. ",
      "comments": [],
      "ancestorIds": [
        "F5319A464BDC4E6AA499989BC7CBCE40",
        "22BBB482814744C1B65CA03AE8B42053"
      ],
      "parentId": "F5319A464BDC4E6AA499989BC7CBCE40"
    },
